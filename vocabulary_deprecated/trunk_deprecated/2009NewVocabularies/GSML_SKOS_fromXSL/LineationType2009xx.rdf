    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:augen_lineation -->

    <skos:Concept rdf:about="&xx;augen_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A shape lineation defined by elongate augen.</skos:definition>
        <skos:prefLabel xml:lang="en">Augen lineation</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:broader rdf:resource="&xx;particle_shape_lineation"/>
        <skos:broader rdf:resource="&xx;tectonic_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:bedding_cleavage_intersection_lineation -->

    <skos:Concept rdf:about="&xx;bedding_cleavage_intersection_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Lineation defined by the intersection of bedding surfaces and cleavage surfaces.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Bedding cleavage intersection lineation</skos:prefLabel>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:broader rdf:resource="&xx;intersection_lineation"/>
        <skos:broader rdf:resource="&xx;tectonic_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:biotite_mineral_lineation -->

    <skos:Concept rdf:about="&xx;biotite_mineral_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en"
            >Biotite mineral lineation</skos:prefLabel>
        <skos:definition
            >Mineral lineation defined by aligned elongate biotite aggregates in a foliation surface. Biotite does not have an elongate (prismatic, acicular) mineral habit, so production of lineation must reflect elongate aggregates of biotite crystals.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:broader rdf:resource="&xx;mineral_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:cleavage_intersection_lineation -->

    <owl:Thing rdf:about="&xx;cleavage_intersection_lineation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Cleavage intersection lineation</skos:prefLabel>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Lineation defined by the intersection of two cleavages.</skos:definition>
        <skos:broader rdf:resource="&xx;intersection_lineation"/>
        <skos:broader rdf:resource="&xx;tectonic_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:crenulation_lineation -->

    <skos:Concept rdf:about="&xx;crenulation_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Crenulation lineation</skos:prefLabel>
        <skos:definition
            >Lineation defined by small-scale wrinkles, typically of phylosillicate minerals, on another surface, typically a cleavage or schistosity. Use instead of intersection lineation if the wrinkles are the measured feature and the intersecting planar feature is not apparent.</skos:definition>
        <skos:broader rdf:resource="&xx;lineation"/>
        <skos:broader rdf:resource="&xx;tectonic_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:flow_lineation -->

    <owl:Thing rdf:about="&xx;flow_lineation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A linear alignment of fabric elements, especially elongate or rod-shaped minerals, interpreted to have been aligned by flow of magma, lava, pyroclastic material, or diapiric flow of sedimentary rock.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote
            >Geoscience Victoria, Australia</skos:historyNote>
        <skos:prefLabel xml:lang="en">Flow lineation</skos:prefLabel>
        <skos:broader rdf:resource="&xx;lineation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:hornblende_mineral_lineation -->

    <skos:Concept rdf:about="&xx;hornblende_mineral_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Mineral lineation defined by aligned hornblende crystals.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Hornblende mineral lineation</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;mineral_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:igneous_lineation -->

    <skos:Concept rdf:about="&xx;igneous_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Lineation defined by alignment of elongate fabric elements formed in lava or magma before complete crystallization. Some typical fabric elements are elongate phenocrysts and vesicles.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Igneous lineation</skos:prefLabel>
        <skos:broader rdf:resource="&xx;lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:igneous_mineral_lineation -->

    <skos:Concept rdf:about="&xx;igneous_mineral_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Lineation in igneous rock defined by aligned crystals where there is evidence that the crystal alignment is related to process occurring before the rock was solidified.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Igneous mineral lineation</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;igneous_lineation"/>
        <skos:broader rdf:resource="&xx;mineral_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:igneous_particle_shape_lineation -->

    <owl:Thing rdf:about="&xx;igneous_particle_shape_lineation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Lineation in an igneous rock, usually welded tuff, defined by elongate objects in the rock (usually pumice pads, also elongate crystals). Descriptive--elogation may be due to deformation or to rotation/alignment of originally elongate (prolate ellipsoid) objects in the rock.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Igneous particle shape lineation</skos:prefLabel>
        <skos:broader rdf:resource="&xx;igneous_lineation"/>
        <skos:broader rdf:resource="&xx;particle_shape_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:igneous_vesicle_lineation -->

    <owl:Thing rdf:about="&xx;igneous_vesicle_lineation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en"
            >Igneous vesicle lineation</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >lineation defined in a volcanic rock by aligned elongation direction of vesicles.</skos:definition>
        <skos:broader rdf:resource="&xx;igneous_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:intersection_lineation -->

    <skos:Concept rdf:about="&xx;intersection_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A lineation produced by the intersection of two planar fabrics.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote
            >Neuendorf et al 2005; this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Intersection lineation</skos:prefLabel>
        <skos:broader rdf:resource="&xx;lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:lineation -->

    <skos:Concept rdf:about="&xx;lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Lineation</skos:prefLabel>
        <skos:definition
            >Nongenetic term for a penetrative linear structure.</skos:definition>
        <skos:historyNote
            >Neuendorf et al 2005; this vocabulary</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:mineral_lineation -->

    <skos:Concept rdf:about="&xx;mineral_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Lineation defined by alignment of elongate crystals that have a prismatic crystal habit or of monomineralic grain aggregates.</skos:definition>
        <skos:historyNote
            >Neuendorf et al 2005; this vocabulary</skos:historyNote>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Mineral lineation</skos:prefLabel>
        <skos:broader rdf:resource="&xx;particle_shape_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:particle_shape_lineation -->

    <owl:Thing rdf:about="&xx;particle_shape_lineation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Lineation defined by alignment of long axes of prolate-shaped particles such as clasts, fossils, crystals, or phenocrysts. Particles may be polycrystalline aggregates formed by deformation of pre-existing particles. General category does not specify genesis of particles or their alignment.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Particle shape lineation</skos:prefLabel>
        <skos:broader rdf:resource="&xx;lineation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:pencil_lineation -->

    <owl:Thing rdf:about="&xx;pencil_lineation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote
            >based on Neuendorf et al 2005</skos:historyNote>
        <skos:definition
            >Lineation defined by tendency of rock to break into long, slender pieces resembling pencils. Typically produced by intesection of bedding and cleavage or intersection of two cleavages. Fabric is generally only observed in weakly metamorphosed rocks.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Pencil lineation</skos:prefLabel>
        <skos:broader rdf:resource="&xx;lineation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:quartz_fibre_lineation -->

    <owl:Thing rdf:about="&xx;quartz_fibre_lineation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Aligned quartz fibres that define a measurable lineation</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Quartz fibre lineation</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;particle_shape_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:rodding_lineation -->

    <owl:Thing rdf:about="&xx;rodding_lineation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Linear structure in metamorphic rocks in which the stronger parts have been shaped into parallel rods (eg. vein quartz)</skos:definition>
        <skos:historyNote>Jackson 1997</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Rodding lineation</skos:prefLabel>
        <skos:broader rdf:resource="&xx;lineation"/>
        <skos:broader rdf:resource="&xx;tectonic_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:sillimanite_mineral_lineation -->

    <skos:Concept rdf:about="&xx;sillimanite_mineral_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Mineral lineation defined by aligned sillimanite crystals.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Sillimanite mineral lineation</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:broader rdf:resource="&xx;mineral_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:slickenline -->

    <skos:Concept rdf:about="&xx;slickenline">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Lineation on a brittle slip surface defined by grooves, ridges, or striations</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Slickenline</skos:prefLabel>
        <skos:historyNote
            >based on Neuendorf et al 2005</skos:historyNote>
        <skos:broader rdf:resource="&xx;lineation"/>
        <skos:broader rdf:resource="&xx;tectonic_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:stretching_lineation -->

    <skos:Concept rdf:about="&xx;stretching_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Lineation defined by elongate (prolate) fabric elements in which the alignment of the fabric elements, and the formation of prolate fabric elements is interpreted to be the product of crystal plastic deformation processes. Associated with mylontic foliation.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Stretching lineation</skos:prefLabel>
        <skos:broader rdf:resource="&xx;particle_shape_lineation"/>
        <skos:broader rdf:resource="&xx;tectonic_lineation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:LineationType:2009xx:tectonic_lineation -->

    <skos:Concept rdf:about="&xx;tectonic_lineation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Penetrative linear structure in a rock body defined by fabric elements that are the product of tectonic processes. Linear is used in the sense that the orientation of the structure can be represented as a line in some sufficiently small neighborhood of any point in the material containig the fabric. Penetrative denotes that the fabric elements defining the structure are repeated at distances so small, compared with the scale of the whole&#8230;that they can be considered to pervade it uniformly and be present at every point (paraphrase of Turner and Weiss, 1963, p. 21).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Tectonic lineation</skos:prefLabel>
