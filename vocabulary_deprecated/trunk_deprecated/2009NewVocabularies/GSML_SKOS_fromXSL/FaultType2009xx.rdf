    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:detachment_fault -->

    <skos:Concept rdf:about="&xx;detachment_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A regional-scale, large displacement, low-angle normal fault.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Detachment fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;low_angle_normal_fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:dextral_strike_slip_fault -->

    <skos:Concept rdf:about="&xx;dextral_strike_slip_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Fault with right-lateral strike-parallel displacement component of slip vector more than 10 times the dip-parallel component of the slip vector at at least one location along the fault, and right-lateral displacement over more than half the mapped trace of the fault.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Dextral strike slip fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;strike_slip_fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:extraction_fault -->

    <owl:Thing rdf:about="&xx;extraction_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A fault whose two sides have approached each other substantially in the direction perpendicular to the fault.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote
            >Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Extraction fault</skos:prefLabel>
        <skos:broader rdf:resource="&xx;fault"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:fault -->

    <skos:Concept rdf:about="&xx;fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >A discrete surface, or zone of discrete surfaces, with some thickness, separating two rock masses across which one mass has slid past the other and characterized by brittle deformation.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Fault</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:high_angle_fault -->

    <skos:Concept rdf:about="&xx;high_angle_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >Fault that dips at least 45 degrees over more than half of its recognized extent, for which slip or separation is not explicitly specified.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >High-angle fault</skos:prefLabel>
        <skos:broader rdf:resource="&xx;fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:high_angle_normal_fault -->

    <skos:Concept rdf:about="&xx;high_angle_normal_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Fault that dips at least 45 degrees over more than half of the recognized extent of the fault with the hanging wall displaced from a structurally higher position relative to footwall rocks.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >High-angle normal fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;high_angle_fault"/>
        <skos:broader rdf:resource="&xx;normal_fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:high_angle_reverse -->

    <skos:Concept rdf:about="&xx;high_angle_reverse">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Reverse fault that dips at least 45 degrees over more than half of its recognized extent, for which slip or separation is not explicitly specified.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >High angle reverse</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;high_angle_fault"/>
        <skos:broader rdf:resource="&xx;reverse_fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:horizontal_fault -->

    <owl:Thing rdf:about="&xx;horizontal_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote
            >GGIC vocabularies working group (Australia)</skos:historyNote>
        <skos:definition
            >Fault that dips less than 10 degrees over more than half the recognized extent of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Horizontal fault</skos:prefLabel>
        <skos:broader rdf:resource="&xx;low_angle_fault"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:left_normal_fault -->

    <skos:Concept rdf:about="&xx;left_normal_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >High angle fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace, with left-lateral strike-parallel component and normal dip-parallel component over at least half the mapped trace of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Left normal fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;oblique_slip_fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:left_reverse_fault -->

    <owl:Thing rdf:about="&xx;left_reverse_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >High angle fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace, with left-lateral strike-parallel component and reverse dip-parallel component over at least half the mapped trace of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Left reverse fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;oblique_slip_fault"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:low_angle_fault -->

    <owl:Thing rdf:about="&xx;low_angle_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Fault that dips less than 45 degrees over more than half of the recognized extent of the fault.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Low angle fault</skos:prefLabel>
        <skos:broader rdf:resource="&xx;fault"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:low_angle_normal_fault -->

    <skos:Concept rdf:about="&xx;low_angle_normal_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Fault that dips less than 45 degrees over more than half of the recognized extent of the fault with the hanging wall displaced from a structurally higher position relative to footwall rocks.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Low-angle normal fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;low_angle_fault"/>
        <skos:broader rdf:resource="&xx;normal_fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:mixed_extraction_fault -->

    <skos:Concept rdf:about="&xx;mixed_extraction_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >An extraction fault with some displacement within the fault plane.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote
            >Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Mixed extraction fault</skos:prefLabel>
        <skos:broader rdf:resource="&xx;extraction_fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:normal_fault -->

    <skos:Concept rdf:about="&xx;normal_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Fault with dip-parallel displacement component of slip vector more than 10 times the strike-parallel component of the slip vector over more than half recognized extent of the fault, and for which the fault dips consistently in the same direction, and for which the hanging wall has been displaced down relative to the footwall.</skos:definition>
        <skos:prefLabel xml:lang="en">Normal fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:oblique_slip_fault -->

    <owl:Thing rdf:about="&xx;oblique_slip_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Oblique slip fault</skos:prefLabel>
        <skos:broader rdf:resource="&xx;fault"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:pure_extraction_fault -->

    <skos:Concept rdf:about="&xx;pure_extraction_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >An extraction fault with no discernible displacement within the fault plane.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote
            >Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Pure extraction fault</skos:prefLabel>
        <skos:broader rdf:resource="&xx;extraction_fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:reverse_fault -->

    <owl:Thing rdf:about="&xx;reverse_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Fault with dip-parallel displacement component of slip vector more than 10 times the strike-parallel component of the slip vector at at least one location along the mapped trace of the fault, and the fault dips consistently in the same direction with the hanging wall displaced up relative to the footwall over at least half the mapped trace of the fault.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en">Reverse fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;fault"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:right_normal_fault -->

    <skos:Concept rdf:about="&xx;right_normal_fault">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >High angle fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace, with right-lateral strike-parallel component and normal dip-parallel component of slip over at least half the mapped trace of the fault</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Right normal fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;oblique_slip_fault"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:right_reverse_fault -->

    <owl:Thing rdf:about="&xx;right_reverse_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >High angle fault with slip vector that has ratio of strike-parallel to dip-parallel displacement between 10 to 1 and 1 to 10 at at least one location along the mapped trace, with a right-lateral strike-parallel component and reverse dip-parallel component of slip over at least half the mapped trace of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Right reverse fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;oblique_slip_fault"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:scissor_fault -->

    <owl:Thing rdf:about="&xx;scissor_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >A fault on which there is increasing offset or separation along the strike from an initial point of no offset, with the opposite sense of offset in the opposite direction.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote
            >Neuendorf et al 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">Scissor fault</skos:prefLabel>
        <skos:broader rdf:resource="&xx;fault"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:sinistral_strike_slip_fault -->

    <owl:Thing rdf:about="&xx;sinistral_strike_slip_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Fault with left-lateral strike-parallel displacement component of slip vector more than 10 times the dip-parallel component of the slip vector at at least one location along the fault, and left-lateral displacement over more than half the mapped trace of the fault.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Sinistral strike slip fault</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&xx;strike_slip_fault"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:FaultType:2009xx:strike_slip_fault -->

    <owl:Thing rdf:about="&xx;strike_slip_fault">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Fault with strike-parallel displacement component of slip vector more than 10 times the dip-parallel component of the slip vector at at least one location along the mapped trace of the fault.</skos:definition>
