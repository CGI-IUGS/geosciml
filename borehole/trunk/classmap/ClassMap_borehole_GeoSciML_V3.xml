<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gsml="urn:cgi:xmlns:CGI:GeoSciML:3.0"
	xmlns:gbh="urn:cgi:xmlns:CGI:xxxxxxxxxxx:3.0" 
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:3.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:3.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_borehole_GeoSciML_V3.xml">    <!--Need to register Borehole namespace??-->
	
	<description>Provides a mapping of classes from the borehole module of GeoSciML to elements from GeoSciML XML Schema Version 3.0.</description>
	<imports>
		<schema prefix="gsml" namespace="urn:cgi:xmlns:CGI:GeoSciML:3.0" schemaLocation="http://www.geosciml.org/geosciml/3.0/xsd/geosciml.xsd" />
	</imports>
	<entry>
		<umlClass>Borehole</umlClass>
		<gmlObjectElement>gbh:Borehole</gmlObjectElement>
		<gmlType>gbh:BoreholeType</gmlType>
		<gmlPropertyType>gbh:BoreholePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeCollar</umlClass>
		<gmlObjectElement>gbh:BoreholeCollar</gmlObjectElement>
		<gmlType>gbh:BoreholeCollarType</gmlType>
		<gmlPropertyType>gbh:BoreholeCollarPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeDetails</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gbh:BoreholeDetails</gmlObjectElement>
		<gmlType>gbh:BoreholeDetailsType</gmlType>
		<gmlPropertyType>gbh:BoreholeDetailsPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeDrillingMethodCode</umlClass>
		<umlStereotype>CodeList</umlStereotype>
		<gmlObjectElement xsi:nil="true"/>
		<gmlType xsi:nil="true"/>
		<gmlPropertyType>gbh:BoreholeDrillingMethodCodeType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeInclinationCode</umlClass>
		<umlStereotype>CodeList</umlStereotype>
		<gmlObjectElement xsi:nil="true"/>
		<gmlType xsi:nil="true"/>
		<gmlPropertyType>gbh:BoreholeInclinationCodeType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeStartPointCode</umlClass>
		<umlStereotype>CodeList</umlStereotype>
		<gmlObjectElement xsi:nil="true"/>
		<gmlType xsi:nil="true"/>
		<gmlPropertyType>gbh:BoreholeStartPointCodeType</gmlPropertyType>
	</entry>
		<entry>
		<umlClass>MappedInterval</umlClass>
		<gmlObjectElement>gbh:MappedInterval</gmlObjectElement>
		<gmlType>gbh:MappedIntervalType</gmlType>
		<gmlPropertyType>gbh:MappedIntervalPropertyType</gmlPropertyType>
	</entry>
</mapping>