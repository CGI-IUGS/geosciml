<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gsml="urn:cgi:xmlns:CGI:GeoSciML:2.0"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 https://www.seegrid.csiro.au/subversion/FullMoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_borehole_GeoSciML_V2.xml">
	<description>Provides a mapping of classes from the borehole module of GeoSciML to elements from GeoSciML XML Schema Version 2.0.</description>
	<imports>
		<schema prefix="gsml" namespace="urn:cgi:xmlns:CGI:GeoSciML:2.0" schemaLocation="http://www.geosciml.org/geosciml/2.0/xsd/geosciml.xsd" />
	</imports>
	<entry>
		<umlClass>Borehole</umlClass>
		<gmlObjectElement>gsml:Borehole</gmlObjectElement>
		<gmlType>gsml:BoreholeType</gmlType>
		<gmlPropertyType>gsml:BoreholePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeCollar</umlClass>
		<gmlObjectElement>gsml:BoreholeCollar</gmlObjectElement>
		<gmlType>gsml:BoreholeCollarType</gmlType>
		<gmlPropertyType>gsml:BoreholeCollarPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeDetails</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gsml:BoreholeDetails</gmlObjectElement>
		<gmlType>gsml:BoreholeDetailsType</gmlType>
		<gmlPropertyType>gsml:BoreholeDetailsPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeDrillingMethodCode</umlClass>
		<umlStereotype>CodeList</umlStereotype>
		<gmlObjectElement xsi:nil="true"/>
		<gmlType xsi:nil="true"/>
		<gmlPropertyType>gsml:BoreholeDrillingMethodCodeType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeInclinationCode</umlClass>
		<umlStereotype>CodeList</umlStereotype>
		<gmlObjectElement xsi:nil="true"/>
		<gmlType xsi:nil="true"/>
		<gmlPropertyType>gsml:BoreholeInclinationCodeType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>BoreholeStartPointCode</umlClass>
		<umlStereotype>CodeList</umlStereotype>
		<gmlObjectElement xsi:nil="true"/>
		<gmlType xsi:nil="true"/>
		<gmlPropertyType>gsml:BoreholeStartPointCodeType</gmlPropertyType>
	</entry>
		<entry>
		<umlClass>MappedInterval</umlClass>
		<gmlObjectElement>gsml:MappedInterval</gmlObjectElement>
		<gmlType>gsml:MappedIntervalType</gmlType>
		<gmlPropertyType>gsml:MappedIntervalPropertyType</gmlPropertyType>
	</entry>
</mapping>