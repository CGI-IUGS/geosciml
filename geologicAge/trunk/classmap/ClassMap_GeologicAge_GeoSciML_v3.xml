<?xml version="1.0" encoding="UTF-8"?>
<mapping xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gsmla="http://xmlns.geosciml.org/GeologicAge/3.0"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_GeologicAge_GeoSciML_v3.xml">
	<description>Provides a mapping of classes from the GeologicAge module of GeoSciML to elements from GeoSciML XML Schema Version 3.0.0</description>
	<imports>
		<schema prefix="gsmla" namespace="http://xmlns.geosciml.org/GeologicAge/3.0" schemaLocation="http://schemas.geosciml.org/geologicage/3.0/geologicAge.xsd"/>
	</imports>
	<!-- ================================================================= -->

	<entry>
		<umlClass>GeologicDateEstimate</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gsmla:GeologicDateEstimate</gmlObjectElement>
		<gmlType>gsmla:GeologicDateEstimateType</gmlType>
		<gmlPropertyType>gsmla:GeologicDateEstimatePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>GeologicEvent</umlClass>
		<umlStereotype>FeatureType</umlStereotype>
		<gmlObjectElement>gsmla:GeologicEvent</gmlObjectElement>
		<gmlType>gsmla:GeologicEventType</gmlType>
		<gmlPropertyType>gsmla:GeologicEventPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>GeologicHistory</umlClass>
<!--		<umlStereotype>DataType</umlStereotype>-->
		<gmlObjectElement>gsmla:GeologicHistory</gmlObjectElement>
		<gmlType>gsmla:GeologicHistoryType</gmlType>
		<gmlPropertyType>gsmla:GeologicHistoryPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>NumericAgeRange</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gsmla:NumericAgeRange</gmlObjectElement>
		<gmlType>gsmla:NumericAgeRangeType</gmlType>
		<gmlPropertyType>gsmla:NumericAgeRangePropertyType</gmlPropertyType>
	</entry>

<!-- by Reference CodeList, not encoded in schema, for information only -->
	<!--	<entry>
		<umlClass>EventProcessTerm</umlClass>
