<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gpp="urn:cgi:xmlns:CGI:PhysicalProperties:3.0.0"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_PhysicalProperties_GeoSciML_V3.xml">
	<description>Provides a mapping of elements of the PhysicalProperties module of GeoSciML to GeoSciML Version 3.0.0</description>
	<imports>
		<schema prefix="gpp" namespace="urn:cgi:xmlns:CGI:PhysicalProperties:3.0.0" schemaLocation="http://www.geosciml.org/physicalProperties/3.0.0/xsd/physicalProperties.xsd" />
	</imports>
	<!-- ===================================================== -->
	<entry>
		<umlClass>PhysicalDescription</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gpp:PhysicalDescription</gmlObjectElement>
		<gmlType>gpp:PhysicalDescriptionType</gmlType>
		<gmlPropertyType>gpp:PhysicalDescriptionPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>PhysicalPropertyMeasure</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gpp:PhysicalPropertyMeasure</gmlObjectElement>
		<gmlType>gpp:PhysicalPropertyMeasureType</gmlType>
		<gmlPropertyType>gpp:PhysicalPropertyMeasurePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>PhysicalPropertyMeasureType</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gpp:PhysicalPropertyMeasureType</gmlObjectElement>
		<gmlType>gpp:PhysicalPropertyMeasureTypeType</gmlType>
		<gmlPropertyType>gpp:PhysicalPropertyMeasureTypePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>PhysicalPropertyMeasure</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gpp:PhysicalPropertyMeasure</gmlObjectElement>
		<gmlType>gpp:PhysicalPropertyMeasureType</gmlType>
		<gmlPropertyType>gpp:PhysicalPropertyMeasurePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>PhysicalPropertyValue</umlClass>
		<umlStereotype>Union</umlStereotype>
		<gmlObjectElement xsi:nil="true" /> 
		<gmlType xsi:nil="true" /> 
		<gmlPropertyType>gpp:PhysicalPropertyValuePropertyType</gmlPropertyType>
	</entry>
</mapping>
