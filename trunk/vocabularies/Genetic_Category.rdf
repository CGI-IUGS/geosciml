	<skos:ConceptScheme rdf:about="urn:cgi:classifierScheme:CGI:GeneticCategory:2008">
		<skos:prefLabel xml:lang="en">Genetic Category</skos:prefLabel>
		<skos:definition>Categories that summarize the geologic history of an Earth material. (ie, a genetic process classifier term).  Examples include igneous, sedimentary, metamorphic, shock metamorphic, volcanic, pyroclastic.</skos:definition>
		<skos:changeNote>
			<rdf:Description>
				<rdf:value>
					Converted manually from xls document https://www.seegrid.csiro.au/subversion/GeoSciML/tags/Vocabulary_200811/GeneticCategory200811.xls 
   				in order to demonstrate SKOS encoding
   			</rdf:value>
				<dc:date>2009-05-07</dc:date>
				<dc:creator>Simon Cox</dc:creator>
			</rdf:Description>
		</skos:changeNote>
		<skos:hasTopConcept
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:anthropogenic_genesis"/>
		<skos:hasTopConcept
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_genesis"/>
		<skos:hasTopConcept
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:weathering_genesis"/>
		<skos:hasTopConcept
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:sedimentary_genesis"/>
		<skos:hasTopConcept
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:metamorphic_metasomatic_or_hydrothermal_genesis"/>
		<skos:hasTopConcept
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:deformation_genesis"/>
		<skos:hasTopConcept
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:impact_genesis"/>
		<skos:hasTopConcept
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:composite_process_genesis"/>
	</skos:ConceptScheme>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:anthropogenic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by human activity.</skos:definition>
		<skos:prefLabel xml:lang="en">Anthropogenic genesis</skos:prefLabel>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by crystallisation from magma.</skos:definition>
		<skos:prefLabel xml:lang="en">Igneous genesis</skos:prefLabel>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_extrusive_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by crystallisation of magma at or immediately adjacent to Earth's surface.</skos:definition>
		<skos:prefLabel xml:lang="en">Igneous extrusive  genesis</skos:prefLabel>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_genesis"/>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:subaerial_extrusive_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by crystallisation of magma either in the open air or immediately adjacent to the land surface.</skos:definition>
		<skos:prefLabel xml:lang="en">Subaerial extrusive genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_extrusive_genesis"/>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:subaqueous_extrusive_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by crystallisation of magma under water or ice.</skos:definition>
		<skos:prefLabel xml:lang="en">Subaqueous extrusive genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_extrusive_genesis"/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_intrusive_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by crystallisation of magma within the Earth.</skos:definition>
		<skos:prefLabel xml:lang="en">Igneous intrusive genesis</skos:prefLabel>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_genesis"/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:plutonic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by crystallisation of magma far enough below Earth surface that complete crystallization of magma bodies forms holocrystalline medium to coarse grained igneous rock, wall rocks generally do not include volcanic products related to the magma, and some contact metamorphism is developed at intrusive contacts.</skos:definition>
		<skos:prefLabel xml:lang="en">Plutonic genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_intrusive_genesis"/>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:hypabyssal_intrusive_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formed by crystallisation close to the Earth's surface, characterized by more rapid cooling than plutonic setting to produce generally fine-grained intrusive igneous rock,  commonly associated with co-magmatic volcanic rocks.</skos:definition>
		<skos:prefLabel xml:lang="en">Hypabyssal intrusive genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_intrusive_genesis"/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:weathering_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by the physical, chemical or biological alteration of rock or sediment at or near the Earth surface; connotes involvement of processes related to the atmosphere (weather...)..</skos:definition>
		<skos:prefLabel xml:lang="en">Weathering genesis</skos:prefLabel>
		<skos:note>production of 'waste' (loose material) doesn't seem essential. </skos:note>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:sedimentary_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by processes of erosion, mass wasting, transportation, deposition, precipitation and biogenic production that take place in Earthâ€™s hydrosphere and atmosphere.</skos:definition>
		<skos:prefLabel xml:lang="en">Sedimentary genesis</skos:prefLabel>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:clastic_sedimentary_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by accumulation of particles (clasts) derived by weathering, erosion, or fragmentation of pre-existing rock or produced by chemical or biologically-mediated precipitation.</skos:definition>
		<skos:prefLabel xml:lang="en">Clastic sedimentary genesis</skos:prefLabel>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:sedimentary_genesis"
		/>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:biological_sedimentary_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by deposition of material produced by living organisms either as part of their body (e.g., exoskeleton, bone, pollen, wood) or through their activities (e.g., faecal pellets).</skos:definition>
		<skos:prefLabel xml:lang="en">Biological sedimentary genesis</skos:prefLabel>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:sedimentary_genesis"/>
		<skos:note>framestone, boundstone are produced by insitu biologic deposition, not particle accumulation.  'Mediated' is unclear to me, 'Produced' seems clearer and accurate.</skos:note>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:chemical_sedimentary_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by direct chemical precipitation (e.g., evaporites, exhalative deposits).</skos:definition>
		<skos:prefLabel xml:lang="en">Chemical sedimentary genesis</skos:prefLabel>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:sedimentary_genesis"
		/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:diagenetic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by chemical and physical changes subsequent to deposition of sediment, during and after lithification that occur under temperature and pressure conditions too low to  be considered metamorphic.</skos:definition>
		<skos:prefLabel xml:lang="en">Diagenetic genesis</skos:prefLabel>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:sedimentary_genesis"
		/>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:metamorphic_metasomatic_or_hydrothermal_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by changes in chemical, mineralogical, or structural properties of rocks in response to chemical and physical conditions that have been imposed below the surface zones of weathering and cementation (diagenesis), and that differ from the conditions under which the rocks in question originated.</skos:definition>
		<skos:prefLabel xml:lang="en"
			>Metamorphic Metasomatic or hydrothermal genesis</skos:prefLabel>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:regional_metamorphic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by metamorphic processes affecting a large rock volume, associated with large-scale tectonic processes. </skos:definition>
		<skos:prefLabel xml:lang="en">Regional metamorphic genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:metamorphic_metasomatic_or_hydrothermal_genesis"/>
		<skos:note>modified definition after IUGS</skos:note>
		<rdfs:isDefinedBy>Smulikowski et al, 2003</rdfs:isDefinedBy>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:local_metamorphic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by metamorphism that may be attributed to a localized cause, such as magmatic intrusion, faulting, metorite impact, combustion of naturally occuring substances (coal), or lightning.</skos:definition>
		<skos:prefLabel xml:lang="en">Local metamorphic genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:metamorphic_metasomatic_or_hydrothermal_genesis"/>
		<rdfs:isDefinedBy>Smulikowski et al, 2003</rdfs:isDefinedBy>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:contact_metamorphic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by metamorphism due to the effect of a magma body on the rocks it intrudes.</skos:definition>
		<skos:prefLabel xml:lang="en">Contact metamorphic genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:local_metamorphic_genesis"/>
		<skos:note>contact metamorphism may effect rocks not directly in contact with intrusion. Contact metamorphism is commonly metasomatic metamorphism; use of term requires information not available from an individual sample.</skos:note>
		<rdfs:isDefinedBy>Smulikowski et al, 2003</rdfs:isDefinedBy>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:dislocation_metamorphic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by metamorphism of local extent associated with fault zones or shear zones. 'Dislocation metamorphic genesis' is 'Deformation genesis' of local extent, explicitly associated with a particular fault or shear zone.</skos:definition>
		<skos:prefLabel xml:lang="en">Dislocation metamorphic genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:local_metamorphic_genesis"/>
		<skos:note>Distinction of dislocation genesis and deformation genesis requires information independent of the lithology of a particular material sample. </skos:note>
		<rdfs:isDefinedBy>Smulikowski et al, 2003</rdfs:isDefinedBy>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:metasomatic_or_hydrothermal_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by open-system changes in chemical composition by reaction with an external source, typically involving chemical transport by a fluid medium flowing through the rock. Metasomatism typically involves introduction of chemical constituents into a rock volume.</skos:definition>
		<skos:prefLabel xml:lang="en">Metasomatic or hydrothermal genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:metamorphic_metasomatic_or_hydrothermal_genesis"/>
		<rdfs:isDefinedBy>based on Neuendorf et al 2005</rdfs:isDefinedBy>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:metamorphic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by closed-system changes in mineralogy, texture, or fabric of a rock  in response to chemical and physical conditions that have been imposed below the surface zones of weathering and cementation (diagenesis), and that differ from the conditions under which the rocks in question originated. As defined here the changes in rock volume may include removal of some chemical constituents from the system (especially water and other volatile constituents). Note narrower use that Neuendorf et al. 2005; this vocabulary distinguishes metasomatism from metamorphism.</skos:definition>
		<skos:prefLabel xml:lang="en">Metamorphic genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:metamorphic_metasomatic_or_hydrothermal_genesis"/>
		<rdfs:isDefinedBy>based on Neuendorf et al 2005</rdfs:isDefinedBy>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:deformation_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by strain-related processes resulting in changes of shape of a rock body, including folding, faulting, shearing, or fabric development; deformation may be either brittle or ductile. Local deformation metamorphic genesis related to a particular fault or shear zone is categorized as 'Dislocation metamorphic genesis'</skos:definition>
		<skos:prefLabel xml:lang="en">Deformation genesis</skos:prefLabel>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:ductile_deformation_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by deformation without loss of material continuity at the scale of observation.</skos:definition>
		<skos:prefLabel xml:lang="en">Ductile deformation genesis</skos:prefLabel>
		<skos:note>rock appears to have deformed without loss of continuity a the scale of observation</skos:note>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:deformation_genesis"
		/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:cataclastic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by brittle deformation, i.e. the formation and growth of fractures and frictional sliding along fracture surfaces.</skos:definition>
		<skos:prefLabel xml:lang="en">Cataclastic genesis</skos:prefLabel>
		<skos:note>Brittle deformation: rock loses continuity; mechanisms include formation and growth of fractures, and frictional sliding on fracture surfaces.</skos:note>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:deformation_genesis"/>
		<rdfs:isDefinedBy>based on Neuendorf et al 2005</rdfs:isDefinedBy>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:impact_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation predominantly by metamorphism related to the passage of a shock wave through a body of material, typically the result of impact of a planetary body (impactor) on a planetary surface (target)</skos:definition>
		<skos:prefLabel xml:lang="en">Impact genesis</skos:prefLabel>
		<skos:note>shock metamorphism may be caused by other kinds of explosions...</skos:note>
		<rdfs:isDefinedBy>Smulikowski et al, 2003</rdfs:isDefinedBy>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:composite_process_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation in which more than one genetic process is dominant. This usually involves some combination of igneous, weathering, sedimentary, metamorphic, deformation or impact-related processes that better represents the origin of the material</skos:definition>
		<skos:prefLabel xml:lang="en">Composite process genesis</skos:prefLabel>
	</skos:Concept>
	<skos:Concept
		rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_sedimentary_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation by a combination of igneous and sedimentary processes</skos:definition>
		<skos:prefLabel xml:lang="en">Igneous-Sedimentary genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:composite_process_genesis"/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:volcaniclastic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation by a combination of extrusive igneous activity and sedimentary transport and deposition.</skos:definition>
		<skos:prefLabel xml:lang="en">Volcaniclastic genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:igneous_sedimentary_genesis"/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:metaigneous_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation by metamorphism of an igneous protolith</skos:definition>
		<skos:prefLabel xml:lang="en">Metaigneous genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:composite_process_genesis"/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:metavolcanic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation by metamorphism of an extrusive igneous protolith. </skos:definition>
		<skos:prefLabel xml:lang="en">Metavolcanic genesis</skos:prefLabel>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:metaigneous_genesis"
		/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:metaplutonic_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation by metamorphism of an plutonic igneous protolith. </skos:definition>
		<skos:prefLabel xml:lang="en">Metaplutonic genesis</skos:prefLabel>
		<skos:broader rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:metaigneous_genesis"
		/>
	</skos:Concept>
	<skos:Concept rdf:about="urn:cgi:classifier:CGI:GeneticCategory:2008:metasedimentary_genesis">
		<skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:GeneticCategory:2008"/>
		<skos:definition>Formation by metamorphism of a sedimentary protolith. </skos:definition>
		<skos:prefLabel xml:lang="en">Metasedimentary genesis</skos:prefLabel>
		<skos:broader
			rdf:resource="urn:cgi:classifier:CGI:GeneticCategory:2008:composite_process_genesis"/>
	</skos:Concept>
</rdf:RDF>
