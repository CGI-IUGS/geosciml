    <!-- urn:cgi:classifier:CGI:ParticleType:200811#accidental_pyroclastic_fragment -->

    <skos:Concept rdf:about="&ParticleType200811;accidental_pyroclastic_fragment">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.02.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >accidental_pyroclastic_fragment</skos:notation>
        <skos:definition
            >Fragment of rock generated by disruption as a direct result of volcanic action but not formed by previous activity of the magmatic system; generally derived from subvolcanic basement.</skos:definition>
        <skos:historyNote
            >Gillespie and Styles 1999</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Accidental pyroclastic fragment</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;pyroclast"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#aggregate_non-clastic_particle -->

    <owl:Thing rdf:about="&ParticleType200811;aggregate_non-clastic_particle">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >aggregate_non-clastic_particle</skos:notation>
        <skos:definition
            >Generic term for a non-clastic particle that is itself composed of an aggregation of particles;</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Aggregate non-clastic particle</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;solid_constituent_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#amygdule -->

    <skos:Concept rdf:about="&ParticleType200811;amygdule">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">amygdule</skos:notation>
        <skos:definition
            >Cavity filled with secondary minerals; denotes that cavities are filled vesicles, thus restricted to volcanic rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Amygdule</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al 2005; SMR</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;cavity"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#autoclast -->

    <skos:Concept rdf:about="&ParticleType200811;autoclast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.10</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">autoclast</skos:notation>
        <skos:prefLabel xml:lang="en">Autoclast</skos:prefLabel>
        <skos:historyNote
            >Gillespie and Styles,1999</skos:historyNote>
        <skos:definition
            >Fragments of extrusive igneous rock formed by mechanical friction of moving lava flow, breakage of chilled lava flow rinds, or gravity crumbling of active spines and domes. Gillespie and Styles (1999) include as type of juvenile pyroclastic fragment, inconsistent with definition of pyroclastic by IUGS and BGS.</skos:definition>
        <skos:broader rdf:resource="&ParticleType200811;granular_particle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#axiolite -->

    <skos:Concept rdf:about="&ParticleType200811;axiolite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">axiolite</skos:notation>
        <skos:prefLabel xml:lang="en">Axiolite</skos:prefLabel>
        <skos:historyNote
            >Williams,Turner,Gilbert 1954</skos:historyNote>
        <skos:definition
            >Elongate to irregular lense with radial aggregates of acicular and fibrous mineral coalescing along a central axis.</skos:definition>
        <skos:broader rdf:resource="&ParticleType200811;aggregate_non-clastic_particle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#bioclast -->

    <skos:Concept rdf:about="&ParticleType200811;bioclast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.06.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">bioclast</skos:notation>
        <skos:definition
            >Granular sedimentary particle that is a &quot;fragmentary piece of a shell, bone, or other hard skeletal structure of an animal, plant, or protozoan. May be fossilized or non-fossilized&quot;. Use in situations where fossil organism can not be identified, thus &#39;material fossil&#39; is inappropriate.</skos:definition>
        <skos:prefLabel xml:lang="en">Bioclast</skos:prefLabel>
        <skos:historyNote>NADM SLTTs 2004</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;biogenic_particle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#biogenic_particle -->

    <skos:Concept rdf:about="&ParticleType200811;biogenic_particle">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.06</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >biogenic_particle</skos:notation>
        <skos:definition
            >&quot;A type of granular particle formed by the physiological activities of organisms (plants, animals, protozoa) that produce body parts which subsequently are incorporated into a sediment aggregate. Contrast with nonbiogenic particle. Includes biogenic objects that are whole or fragmentary (bioclasts); can be fossilized or non-fossilized&quot;</skos:definition>
        <skos:historyNote>NADM SLTTs 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Biogenic particle</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;granular_particle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#bleb -->

    <skos:Concept rdf:about="&ParticleType200811;bleb">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.4.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">bleb</skos:notation>
        <skos:definition
            >Generic term for a small rounded particle of uncertain origin</skos:definition>
        <skos:prefLabel xml:lang="en">Bleb</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;solid_constituent_particle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#cavity -->

    <owl:Thing rdf:about="&ParticleType200811;cavity">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">cavity</skos:notation>
        <skos:definition
            >Constituent is empty space between particles in a compound material; generally filled with some sort of fluid. Cavity particle types can be used to describe porosity in a granular material.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">Cavity</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#coated_particle -->

    <owl:Thing rdf:about="&ParticleType200811;coated_particle">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.07</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">coated_particle</skos:notation>
        <skos:definition
            >&quot;A general term for a grain that has coats or layers of foreign material, usually fine mud-sized and occasionally recrystallized, that form concentric or overlapping shells around a core of rock, shell, peloidal, or intraclastic material.&quot; (eg: oolith, pisolith)</skos:definition>
        <skos:historyNote>NADM SLTTs 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en">Coated particle</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;granular_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#cognate_fragments -->

    <owl:Thing rdf:about="&ParticleType200811;cognate_fragments">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.02.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >cognate_fragments</skos:notation>
        <skos:historyNote
            >Base on Gillespie and Styles, 1999</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Cognate fragments</skos:prefLabel>
        <skos:definition
            >Pyroclast consisting of rock which formed during earlier (related) volcanic activity that have been ejected with other pyroclastic debris during a later eruption.</skos:definition>
        <skos:broader rdf:resource="&ParticleType200811;pyroclast"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#concretionary_particle -->

    <owl:Thing rdf:about="&ParticleType200811;concretionary_particle">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >concretionary_particle</skos:notation>
        <skos:historyNote
            >Neuendorf et al, 2005</skos:historyNote>
        <skos:definition
            >A hard, compact mass or aggregate of mineral matter, normally subsperical but commonly oblate, disc-shaped or irregular. Formed by precipitation of mineral from solution in the pores of a granular rock, localized around a nucleus or center, to define a discrete, sharply separated object. Size ranges from cm to meters. Particle geometry description for concretion describes the concretion size and shape, not the size and shape of particles forming the concretion.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Concretionary particle</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;aggregate_non-clastic_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#coprolite -->

    <skos:Concept rdf:about="&ParticleType200811;coprolite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.06.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">coprolite</skos:notation>
        <skos:definition
            >The fossilised excrement of vertebrate animals</skos:definition>
        <skos:prefLabel xml:lang="en">Coprolite</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;biogenic_particle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#crystal_fragment -->

    <owl:Thing rdf:about="&ParticleType200811;crystal_fragment">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.02.4</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >crystal_fragment</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >A fragment of a crystal interpreted to have been broken by pyroclastic processes.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Crystal fragment</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;pyroclast"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#crystalline_grain -->

    <owl:Thing rdf:about="&ParticleType200811;crystalline_grain">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >crystalline_grain</skos:notation>
        <skos:historyNote
            >Struik, 2002; NADM SLTTs 2004</skos:historyNote>
        <skos:definition
            >Constituent that is the product of crystallization during formation of a compound Earth material (see NADMC1 2004). Serves to distinguish crystalline from granular rocks (Struik, 2002). Includes constituents crystallized in evaporite environments, during diagenesis or hydrothermal alteration, or in other low-temperature environments.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Crystalline grain</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;solid_constituent_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#crystallite -->

    <skos:Concept rdf:about="&ParticleType200811;crystallite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.2.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">crystallite</skos:notation>
        <skos:prefLabel xml:lang="en">Crystallite</skos:prefLabel>
        <skos:historyNote
            >Williams,Turner,Gilbert, 1954</skos:historyNote>
        <skos:definition
            >Constitutent occurs as minute, spherical, rod, or hair-like forms. Typically appear isotropic in thin section.</skos:definition>
        <skos:broader rdf:resource="&ParticleType200811;crystalline_grain"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#epiclast -->

    <skos:Concept rdf:about="&ParticleType200811;epiclast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">epiclast</skos:notation>
        <skos:definition
            >An individual constituent, grain or fragment of a sediment or rock, produced by erosion of a larger rock mass. Particle whose origin as a fragment is a result of surface (sedimentary, weathering...) processes. &#39;Crystals, crystal fragments, glass and rock fragments that have been liberated from any type of pre-existing consolidated rock (volcanic or non-volcanic) by weathering or erosion and transported from the site of origin by gravity, air, water, or ice&#39; [Schmid, 1981]. Distinguished from intraclast in that epiclast is derived from a pre-existing rock from outside the basin of deposition (NADM SLTTs 2004) before introduction into a CompoundMaterial (sense of NADMC1, 2004).</skos:definition>
        <skos:prefLabel xml:lang="en">Epiclast</skos:prefLabel>
        <skos:historyNote>Schmid 1981</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;granular_particle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#fecal_pellet -->

    <skos:Concept rdf:about="&ParticleType200811;fecal_pellet">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.08.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">fecal_pellet</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >an organic excrement, mainly of invertebrates, occuring especially in modern marine sediments but also fossilised in some sedimentary rocks, usually with simple ovoid form, less commonly rod-shaped.</skos:definition>
        <skos:prefLabel xml:lang="en">Fecal pellet</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;peloid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#fenestra -->

    <owl:Thing rdf:about="&ParticleType200811;fenestra">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">fenestra</skos:notation>
        <skos:prefLabel xml:lang="en">Fenestra</skos:prefLabel>
        <skos:definition
            >Primary or penecontemporaneous gap or cavity in the framework of a sedimentary rock, larger than grain-supported intersticies. May be open space or have partial to complete fill with secondary cement or introduced sediment.</skos:definition>
        <skos:historyNote
            >Neuendorf et al 2005; SMR</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;cavity"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#fiamme -->

    <owl:Thing rdf:about="&ParticleType200811;fiamme">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.3.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">fiamme</skos:notation>
        <skos:historyNote
            >Neuendorf et al 2005; http://en.wikipedia.org/wiki/Fiamme</skos:historyNote>
        <skos:definition
            >Lens-shaped bodies, usually mm to cm thick, and centimeters to 1-2 decimeters long, typically seen on surfaces of some pyroclastic rocks. The name fiamme comes from the Italian word for flames, describing their shape. The term is descriptive and non-genetic.Generally interpreted to form by the collapse of pumice fragments during welding in a hot pyroclastic deposit.</skos:definition>
        <skos:prefLabel xml:lang="en">Fiamme</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;igneous_inclusion"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#fluid_inclusion -->

    <skos:Concept rdf:about="&ParticleType200811;fluid_inclusion">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.3.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">fluid_inclusion</skos:notation>
        <skos:definition
            >A cavity within a crystal containing liquid and/or gas, formed by the entrapment in crystal irregularities of fluid, commonly that from which the mineral crystallised.</skos:definition>
        <skos:prefLabel xml:lang="en">Fluid inclusion</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;cavity"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#fluidal_pyroclast -->

    <skos:Concept rdf:about="&ParticleType200811;fluidal_pyroclast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.02.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >fluidal_pyroclast</skos:notation>
        <skos:definition
            >Pyroclast that has a a rounded, fluidal shape indicating that it was in a wholly or partly molten state during formation and subsequent transport. Concept corresponds to bomb, but with no size denotation.</skos:definition>
        <skos:historyNote
            >Base on Gillespie and Styles, 1999</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Fluidal pyroclast</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;pyroclast"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#glomerocryst -->

    <owl:Thing rdf:about="&ParticleType200811;glomerocryst">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.3.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">glomerocryst</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >An aggregate of crystals of the same mineral</skos:definition>
        <skos:prefLabel xml:lang="en">Glomerocryst</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;igneous_inclusion"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#granular_particle -->

    <owl:Thing rdf:about="&ParticleType200811;granular_particle">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >granular_particle</skos:notation>
        <skos:definition
            >Solid constituent in a compound earth material (sense of NADMC1) that is a pre-existing object before incorporation into a particulate aggregate. &quot;...a component of solid material that has the form of grains, clasts, fragments, or whole objects of any size, shape, composition, texture, and structure.&quot; (NADMSC SLTTs, 2004)</skos:definition>
        <skos:historyNote>NADM SLTTs 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Granular particle</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;solid_constituent_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#holoblast -->

    <skos:Concept rdf:about="&ParticleType200811;holoblast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.2.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">holoblast</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A crystalline grain in a metamorphic rock that is newly and completely formed during metamorphism</skos:definition>
        <skos:prefLabel xml:lang="en">Holoblast</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;crystalline_grain"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#hydroclast -->

    <owl:Thing rdf:about="&ParticleType200811;hydroclast">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.02.5</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">hydroclast</skos:notation>
        <skos:historyNote
            >Gillespie and Styles 1999</skos:historyNote>
        <skos:prefLabel xml:lang="en">Hydroclast</skos:prefLabel>
        <skos:definition
            >Juvenile pyroclastic fragment formed by magma-water interaction during subaqueous or subglacial extrusion; typically consist of chilled glass.</skos:definition>
        <skos:broader rdf:resource="&ParticleType200811;pyroclast"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#hydrothermal_vein -->

    <owl:Thing rdf:about="&ParticleType200811;hydrothermal_vein">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.7.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >hydrothermal_vein</skos:notation>
        <skos:definition
            >a tabular or sheet-like part of a compound material formed by hydrothermal (or other metasomatic) mineral filling a fracture; may be associated with replacement of the host rock adjacent to the body</skos:definition>
        <skos:historyNote
            >Neuendorf et al 2005; this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Hydrothermal vein</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;intrusive_sheet"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#igneous_inclusion -->

    <skos:Concept rdf:about="&ParticleType200811;igneous_inclusion">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >igneous_inclusion</skos:notation>
        <skos:definition
            >Aggregate non-clastic particle that are of igneous origin. Particle types for compound material are restricted to inclusions that are typically of a size consistent with hand-sample description to characterize a kind of rock or a rock sample; larger inclusion types are included in the GeologicUnitPart role vocabulary.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Igneous inclusion</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;aggregate_non-clastic_particle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#igneous_vein -->

    <skos:Concept rdf:about="&ParticleType200811;igneous_vein">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.7.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">igneous_vein</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >a tabular or sheet like part of a compound material formed by the intrusion of magma</skos:definition>
        <skos:prefLabel xml:lang="en">Igneous vein</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;intrusive_sheet"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#intraclast -->

    <owl:Thing rdf:about="&ParticleType200811;intraclast">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.04</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">intraclast</skos:notation>
        <skos:historyNote
            >NADM SLTTs 2004; Neuendorf et al., 2005; Folk, 1959</skos:historyNote>
        <skos:definition
            >A broad, general term introduced by Folk (1959, p. 4) for a component of limestone, representing a torn-up and re-worked fragment of a penecontemporaneous sediment (usually weakly consolidated) that has been eroded within the basin of deposition...and re-deposited there...The fragment may range in size from fine sand to gravel...</skos:definition>
        <skos:prefLabel xml:lang="en">Intraclast</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;granular_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#intrusive_sheet -->

    <owl:Thing rdf:about="&ParticleType200811;intrusive_sheet">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.7</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">intrusive_sheet</skos:notation>
        <skos:definition
            >a tabular or sheet-like part of a compound material, genetic origin not specified</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">Intrusive sheet</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;aggregate_non-clastic_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#juvenile_pyroclastic_fragment -->

    <owl:Thing rdf:about="&ParticleType200811;juvenile_pyroclastic_fragment">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.02.6</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >juvenile_pyroclastic_fragment</skos:notation>
        <skos:historyNote
            >Gillespie and Styles 1999</skos:historyNote>
        <skos:definition
            >Fragment formed directly from cooling magma during transport prior to primary deposition.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Juvenile pyroclastic fragment</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;pyroclast"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#lithic_clast -->

    <skos:Concept rdf:about="&ParticleType200811;lithic_clast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.01.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">lithic_clast</skos:notation>
        <skos:definition
            >A clast derived by erosion from older, pre-existing rock materials of any kind, usually beyond the basin of accumulation but occasionally from within the basin of deposition (as from a submarine or subaqueous exposure adjacent to a depositional site). A clast that is identified as a piece of rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Lithic clast</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;epiclast"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#lithoclast -->

    <skos:Concept rdf:about="&ParticleType200811;lithoclast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.01.1.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">lithoclast</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005, p. 375</skos:historyNote>
        <skos:definition
            >&quot;A mechanically formed and deposited fragment of a carbonate rock...derived from an older, lithified limestone or dolomite within, adjacent to, or outside the depositional site&quot;</skos:definition>
        <skos:prefLabel xml:lang="en">Lithoclast</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;lithic_clast"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#lithophysae -->

    <skos:Concept rdf:about="&ParticleType200811;lithophysae">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.4.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">lithophysae</skos:notation>
        <skos:definition
            >Bubble-like cavity with concentric shells of finely crystalline minerals; open space remains in core of structure. Radiating fibrous structure may be present in secondary mineral fill. Typically in silicic volcanic rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Lithophysae</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al 2005</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;cavity"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#material_fossil -->

    <skos:Concept rdf:about="&ParticleType200811;material_fossil">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.06.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">material_fossil</skos:notation>
        <skos:historyNote
            >GeoSciML v2 RC1 model element scope note</skos:historyNote>
        <skos:prefLabel xml:lang="en">Material fossil</skos:prefLabel>
        <skos:definition
            >The preserved remains or replaced remains (casts) of plants and animals. A fossil type may have one or more described associated organisms. If particle type is materialFossil, an additional type property element may provide a reference to a Paleontologic description of the fossil.</skos:definition>
        <skos:broader rdf:resource="&ParticleType200811;biogenic_particle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#miarolitic_cavity -->

    <owl:Thing rdf:about="&ParticleType200811;miarolitic_cavity">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.5.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811"
            >miarolitic_cavity</skos:notation>
        <skos:historyNote
            >Neuendorf et al 2005</skos:historyNote>
        <skos:definition
            >An irregular cavity in a phaneritic igneous rock into which small crystals of the rock-forming minerals protrude.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Miarolitic cavity</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;vug"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#microlite -->

    <skos:Concept rdf:about="&ParticleType200811;microlite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.2.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">microlite</skos:notation>
        <skos:definition
            >Constituent occurs as minute incipient crystals that display some birefringence in thin section.</skos:definition>
        <skos:prefLabel xml:lang="en">Microlite</skos:prefLabel>
        <skos:historyNote
            >Williams,Turner,Gilbert, 1954</skos:historyNote>
        <skos:broader rdf:resource="&ParticleType200811;crystalline_grain"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#mineral_clast -->

    <owl:Thing rdf:about="&ParticleType200811;mineral_clast">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.01.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">mineral_clast</skos:notation>
        <skos:definition
            >An epiclast that is composed of a single mineral.</skos:definition>
        <skos:historyNote>NADM SLTTs 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en">Mineral clast</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;epiclast"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#nodule -->

    <owl:Thing rdf:about="&ParticleType200811;nodule">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.4</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">nodule</skos:notation>
        <skos:definition
            >An irregularly rounded mass of a mineral or mineral aggregate normally having a warty or knobby surface and no internal sructure, usually with a contrasting composition from the enclosing sediment or rock matrix in which it is embedded, and that can be separated as a discrete mass from the host material. Igneous nodules should be classified as &#39;Igneous inclusions&#39;.</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">Nodule</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;aggregate_non-clastic_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#oncoid -->

    <owl:Thing rdf:about="&ParticleType200811;oncoid">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.07.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">oncoid</skos:notation>
        <skos:definition
            >A &#39;...coated grain with a cortex of irregular, partially overlapping laminae. They are typically irregular in shape and may exhibit biogenic structures. Some forms lack a distinct nucleus. Oncoids are generally larger than 2 mm.&#39; (Hallsworth and Knox, 1999, p. 27). Synonymous with oncolite: A small, variously shaped, concentrically laminated, calcareous sedimentary structure, resembling an oolith, and formed by the accretion of successive layer[s]... It...generally does not exceed 10 cm in dimension. (Jackson, 1997, p. 446).</skos:definition>
        <skos:historyNote
            >Jackson 1997; Hallsworth and Knox 1999</skos:historyNote>
        <skos:prefLabel xml:lang="en">Oncoid</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;coated_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#ooid -->

    <owl:Thing rdf:about="&ParticleType200811;ooid">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.07.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">ooid</skos:notation>
        <skos:definition
            >A general, nongenetic term for a particle that resembles an oolith in outer appearance and size- (Jackson, 1997, p. 447). Hallsworth and Knox (1999, p. 27) describe ooids as coated grains that typically are spherical or ellipsoidal in shape, with the degree of roundness increasing outward. Concentric to semi-concentric coats are smoothly and evenly laminated. A nucleus usually is present, and may have a composition different from the coatings. Biogenic structures are not obvious. NADMSC SLTTs (2004) considers synomous with oolith.</skos:definition>
        <skos:historyNote
            >Jackson1997; Hallsworth and Knox 1999</skos:historyNote>
        <skos:prefLabel xml:lang="en">Ooid</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;coated_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#orb -->

    <skos:Concept rdf:about="&ParticleType200811;orb">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.3.3.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">orb</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">Orb</skos:prefLabel>
        <skos:definition
            >igneous constituent typically mafic, equant rounded spheroid with concentric mineralogic banding</skos:definition>
        <skos:broader rdf:resource="&ParticleType200811;igneous_inclusion"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#paleoblast -->

    <skos:Concept rdf:about="&ParticleType200811;paleoblast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.2.4</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">paleoblast</skos:notation>
        <skos:historyNote
            >Neuendorf et al. 2005</skos:historyNote>
        <skos:definition
            >A crystal or remnant of a crystal that is older than other mineral grains in the rock, especially in metamorphic rocks</skos:definition>
        <skos:prefLabel xml:lang="en">Paleoblast</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;crystalline_grain"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#paramorph -->

    <owl:Thing rdf:about="&ParticleType200811;paramorph">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.2.5.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">paramorph</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >a pseudomorph with the same composition as the original crystal (eg: calcite after aragonite)</skos:definition>
        <skos:prefLabel xml:lang="en">Paramorph</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;pseudomorph"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#peloid -->

    <owl:Thing rdf:about="&ParticleType200811;peloid">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.08</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">peloid</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >a usually rounded aggregate of clay-sized calcareous (micritic) material; origin and size is not specified</skos:definition>
        <skos:prefLabel xml:lang="en">Peloid</skos:prefLabel>
        <skos:broader rdf:resource="&ParticleType200811;granular_particle"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:ParticleType:200811#pisoid -->

    <skos:Concept rdf:about="&ParticleType200811;pisoid">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.1.07.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleType:200811">pisoid</skos:notation>
        <skos:definition
            >&quot;A round or ellipsoidal accretionary body resembling a pea in size and shape.... A pisoid...is larger and less regular in form than an ooid, although it may have the same concentric and/or radial internal structure&quot; (Jackson, 1997, p. 489).</skos:definition>
        <skos:historyNote
            >NADM SLTTs 2004; Jackson 1997</skos:historyNote>
