    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#abandoned_channel -->

    <owl:Thing rdf:about="&EventEnvironment200811;abandoned_channel">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.07.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >abandoned_channel</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A drainage channel along which runoff no longer occurs, as on an alluvial fan</skos:definition>
        <skos:prefLabel xml:lang="en"
            >abandoned channel</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;river_plain_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#above_carbonate_compensation_depth_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;above_carbonate_compensation_depth_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.01.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >above_carbonate_compensation_depth_setting</skos:notation>
        <skos:definition
            >Marine environment in which carbonate sediment does not dissolve before reaching the sea floor and can accumulate.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Above carbonate compensation depth setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;subaqueous_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#abyssal -->

    <owl:Thing rdf:about="&EventEnvironment200811;abyssal">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.02</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">abyssal</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The ocean environment at water depths between 3,500 and 6,000 metres</skos:definition>
        <skos:prefLabel xml:lang="en">abyssal</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#aeolian_process_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;aeolian_process_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.02.2.0.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >aeolian_process_setting</skos:notation>
        <skos:definition
            >Sedimentary setting in which wind is the dominant process producing, transporting, and depositing sediment. Typically has low-relief plain or piedmont slope physiography.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >aeolian process setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;subaerial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#algal_flat_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;algal_flat_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.01.0</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >algal_flat_setting</skos:notation>
        <skos:definition
            >Modern &quot;algal flats are found on rock or mud in areas flooded only by the highest tides and are often subject to high evaporation rates. Algal flats survive only when an area is salty enough to eliminate snails and other herbivorous animals that eat algae, yet is not so salty that the algae cannot survive. The most common species of algae found on algal flats are blue-green algae of the genera Scytonema and Schizothrix. These algae can tolerate the daily extremes in temperature and oxygen that typify conditions on the flats. Other plants sometimes found on algal flats include one-celled green algae, flagellates, diatoms, bacteria, and isolated scrubby red and black mangroves, as well as patches of saltwort. Animals include false cerith, cerion snails, fiddler crabs, and great land crabs. Flats with well developed algal mats are restricted for the most part to the Keys, with Sugarloaf and Crane Keys offering prime examples of algal flat habitat.&quot; (Audubon, 1991)</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Algal flat setting</skos:prefLabel>
        <skos:historyNote
            >http://www.audubonofflorida.org/main/wetlands/chp3.htm; Reading, 1978</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;low_energy_shoreline_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#alluvial_fan -->

    <owl:Thing rdf:about="&EventEnvironment200811;alluvial_fan">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.08.1..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">alluvial_fan</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A low, outspread, relatively flat to gently sloping mass of loose rock material, shaped like an open fan or a segment of a cone, deposited by a stream (esp. in a semiarid region) at the place where it issues from a narrow mountain valley upon a plain or broad valley, or where a tributary stream is near or at its junction with the main stream, or wherever a constriction in a valley abruptly ceases or the gradient of the stream suddenly decreases; it is steepest near the mouth of the valley where its apex points upstream, and it slopes gently and convexly outward with gradually decreasing gradient</skos:definition>
        <skos:prefLabel xml:lang="en">Alluvial fan</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;piedmont_slope_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#alluvial_plain -->

    <owl:Thing rdf:about="&EventEnvironment200811;alluvial_plain">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.08.2..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">alluvial_plain</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:prefLabel xml:lang="en">Alluvial plain</skos:prefLabel>
        <skos:definition
            >An assemblage landforms produced by alluvial and fluvial processes (braided streams, terraces, etc.,) that form low gradient, regional ramps along the flanks of mountains and extend great distances from their sources (e.g., High Plains of North America). (NRCS GLOSSARY OF LANDFORM AND GEOLOGIC TERMS). A level or gently sloping tract or a slightly undulating land surface produced by extensive deposition of alluvium... Synonym-- wash plain;...river plain; aggraded valley plain;... (Jackson, 1997, p. 17). May include one or more River plain systems.</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;piedmont_slope_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#anoxic_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;anoxic_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.01.0.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">anoxic_setting</skos:notation>
        <skos:definition
            >Setting depleted in oxygen, typically subaqueous.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">Anoxic setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#arid_or_semi_arid_environment_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;arid_or_semi_arid_environment_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.03.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >arid_or_semi_arid_environment_setting</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Arid or Semi Arid environment setting</skos:prefLabel>
        <skos:definition
            >Setting characterized by mean annual precipitation of 10 inches (25 cm) or less. (Jackson, 1997, p. 172). Equivalent to SLTT &#39;Desert setting&#39;, but use &#39;Arid&#39; to emphasize climatic nature of setting defintion.</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;terrestrial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#backreef -->

    <skos:Concept rdf:about="&EventEnvironment200811;backreef">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.02.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">backreef</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The landward side of a reef. The term is often used adjectivally to refer to deposits within the restricted lagoon behind a barrier reef, such as the &quot;back-reef facies&quot; of lagoonal deposits. In some places, as on a platform-edge reef tract, &quot;back reef&quot; refers to the side of the reef away from the open sea, even though no land may be nearby</skos:definition>
        <skos:prefLabel xml:lang="en">backreef</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;biological_reef_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#barrier_beach -->

    <skos:Concept rdf:about="&EventEnvironment200811;barrier_beach">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.00.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">barrier_beach</skos:notation>
        <skos:definition
            >A narrow, elongate sand or gravel ridge rising slightly above the high-tide level and extending generally parallel with the shore, but separated from it by a lagoon (Shepard, 1954, p.1904), estuary, or marsh; it is extended by longshore transport and is rarely more than several kilometers long.</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">barrier beach</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;barrier_island_coastline_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;beach"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#barrier_island_coastline_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;barrier_island_coastline_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.03</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >barrier_island_coastline_setting</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >setting meant to include all the various geographic elements typically associated with a barrier island coastline, including the barrier islands, and geomorphic/geographic elements that are linked by processes associated with the presence of the island (e.g. wash over fans, inlet channel, back barrier lagoon).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Barrier island coastline setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#barrier_lagoon -->

    <skos:Concept rdf:about="&EventEnvironment200811;barrier_lagoon">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.03.0</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">barrier_lagoon</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A lagoon that is roughly parallel to the coast and is separated from the open ocean by a strip of land or by a barrier reef. Tidal influence is typically restricted and the lagoon is commonly hypersaline.</skos:definition>
        <skos:prefLabel xml:lang="en">barrier lagoon</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;barrier_island_coastline_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;lagoonal_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;subaqueous_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#basin_plain_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;basin_plain_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.04</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >basin_plain_setting</skos:notation>
        <skos:historyNote
            >Bates &amp; Jackson, 1987; Heezen &amp; Laughton, 1963; Reading, 1978, p. 390</skos:historyNote>
        <skos:definition
            >Near flat areas of ocean floor, slope less than 1:1000; generally receive only distal turbidite and pelagic sediments.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Basin plain setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#bathyal -->

    <skos:Concept rdf:about="&EventEnvironment200811;bathyal">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.03</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">bathyal</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The ocean environment at water depths between 200 and 3500 metres</skos:definition>
        <skos:prefLabel xml:lang="en">bathyal</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#beach -->

    <owl:Thing rdf:about="&EventEnvironment200811;beach">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.00</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">beach</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The unconsolidated material at the shoreline that covers a gently sloping zone, typically with a concave profile, extending landward from the low-water line to the place where there is a definite change in material or physiographic form (such as a cliff), or to the line of permanent vegetation (usually the effective limit of the highest storm waves); at the shore of a body of water, formed and washed by waves or tides, usually covered by sand or gravel, and lacking a bare rocky surface.</skos:definition>
        <skos:prefLabel xml:lang="en">beach</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#below_carbonate_compensation_depth_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;below_carbonate_compensation_depth_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.01.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >below_carbonate_compensation_depth_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Below carbonate compensation depth setting</skos:prefLabel>
        <skos:definition
            >Marine environment in which water is deep enough that carbonate sediment goes into solution before it can accumulate on the sea floor.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;subaqueous_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#biological_reef_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;biological_reef_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.02</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >biological_reef_setting</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A ridgelike or moundlike structure, layered or massive, built by sedentary calcareous organisms, esp. corals, and consisting mostly of their remains; it is wave-resistant and stands topographically above the surrounding contemporaneously deposited sediment.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Biological reef setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#bog -->

    <owl:Thing rdf:about="&EventEnvironment200811;bog">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.03.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">bog</skos:notation>
        <skos:definition
            >Waterlogged, spongy ground, consisting primarily of mosses, containing acidic, decaying vegetation that may develop into peat.</skos:definition>
        <skos:historyNote
            >Jackson, 1997; NADM SLTTs, 2004;</skos:historyNote>
        <skos:prefLabel xml:lang="en">Bog</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;wetland_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#braided_channel -->

    <skos:Concept rdf:about="&EventEnvironment200811;braided_channel">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.07.1.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">braided_channel</skos:notation>
        <skos:definition
            >A stream that divides into or follows an interlacing or tangled network of several small branching and reuniting shallow channels separated from each other by ephemeral branch islands or channel bars, resembling in plan the strands of a complex braid. Such a stream is generally believed to indicate an inability to carry all of its load, such as an overloaded and aggrading stream flowing in a wide channel on a floodplain</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">braided channel</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;river_channel"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#carbonate_dominated_shoreline_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;carbonate_dominated_shoreline_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.05</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >carbonate_dominated_shoreline_setting</skos:notation>
        <skos:definition
            >A shoreline setting in which terrigineous input is minor compared to local carbonate sediment production. Constructional biogenic activity is an important element in geomorphic development.</skos:definition>
        <skos:historyNote
            >this vocabulary+F82</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Carbonate dominated shoreline setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#cave -->

    <owl:Thing rdf:about="&EventEnvironment200811;cave">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.08..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">cave</skos:notation>
        <skos:definition
            >A natural underground open space; it generally has a connection to the surface, is large enough for a person to enter, and extends into darkness. The most common type of cave is formed in limestone by dissolution.</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">Cave</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#coastal_dune_field -->

    <owl:Thing rdf:about="&EventEnvironment200811;coastal_dune_field">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.09</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >coastal_dune_field</skos:notation>
        <skos:definition
            >A dune field on low-lying land recently abandoned or built up by the sea; the dunes may ascend a cliff and travel inland.</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >coastal dune field</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;dunefield"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#coastal_plain_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;coastal_plain_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.04</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >coastal_plain_setting</skos:notation>
        <skos:definition
            >A low relief plain bordering a water body extending inland to the nearest elevated land, sloping very gently towards the water body. Distinguished from alluvial plain by presence of relict shoreline-related deposits or morphology.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Coastal plain setting</skos:prefLabel>
        <skos:historyNote
            >based on Neuendorf et al, 2005, p. 125</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#contact_metamorphic_origin -->

    <skos:Concept rdf:about="&EventEnvironment200811;contact_metamorphic_origin">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >contact_metamorphic_origin</skos:notation>
        <skos:historyNote
            >NADM metamorphic rock vocabulary SLTTm1.0, 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Contact metamorphic origin</skos:prefLabel>
        <skos:definition
            >Metamorphism of country rock at the contact of an igneous body.</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_interior_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#continental_borderland_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;continental_borderland_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.08</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >continental_borderland_setting</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:definition
            >&quot;An area of the continental margin between the shoreline and the continental slope that is topographically more complex than the continental shelf. It is characterized by ridges and basins, some of which are below the depth of the continental shelf. An example is the southern California continental borderland;....&quot; (Jackson, 1997, p. 138)..</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Continental borderland setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#continental_crust -->

    <owl:Thing rdf:about="&EventEnvironment200811;continental_crust">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.8.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >continental_crust</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >That type of the Earth&#39;s crust which underlies the continents and the continental shelves; it is equivalent to the</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Continental crust</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;crust"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#continental_shelf_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;continental_shelf_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.07</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >continental_shelf_setting</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:definition
            >&quot;That part of the ocean floor that is between the shoreline and the continental slope (or, when there is no noticeable continental slope, a depth of 200 m). It is characterized by its gentle slope of 0.1 degree&quot; (Jackson, 1997, p. 138). Continental shelves have a classic shoreline-shelf-slope profile termed &#39;clinoform&#39;.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Continental shelf setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#crust -->

    <owl:Thing rdf:about="&EventEnvironment200811;crust">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.8.0</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">crust</skos:notation>
        <skos:definition
            >The outermost layer or shell of the Earth, defined according to various criteria, including seismic velocity, density and composition; that part of the Earth above the Mohorovicic discontinuity, made up of the</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">crust</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_interior_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#cutoff_meander -->

    <skos:Concept rdf:about="&EventEnvironment200811;cutoff_meander">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.07.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">cutoff_meander</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The abandoned, bow- or horseshoe-shaped channel of a former meander, left when the stream formed a cutoff across a narrow meander neck. Note that these are typically lakes, thus also lacustrine.</skos:definition>
        <skos:prefLabel xml:lang="en">cutoff meander</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;river_plain_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#deep_sea_trench -->

    <owl:Thing rdf:about="&EventEnvironment200811;deep_sea_trench">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.03</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">deep_sea_trench</skos:notation>
        <skos:historyNote>Reading, 1978</skos:historyNote>
        <skos:definition
            >Deep ocean basin with steep (average 10 degrees) slope toward land, more gentle slope (average 5 degrees) towards the sea, and abundant seismic activity on landward side of trench. Does not denote water depth, but may be very deep.</skos:definition>
        <skos:prefLabel xml:lang="en">Deep sea trench</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#delta_front -->

    <owl:Thing rdf:about="&EventEnvironment200811;delta_front">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.06.02</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">delta_front</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition xml:lang="en"
            >A narrow zone where deposition in deltas is most active, consisting of a continuous sheet of sand, and occurring within the effective depth of wave erosion (10 m or less). It is the zone separating the prodelta from the delta plain, and it may or may not be steep&quot;</skos:definition>
        <skos:prefLabel xml:lang="en">delta front</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;deltaic_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#delta_plain -->

    <owl:Thing rdf:about="&EventEnvironment200811;delta_plain">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.06.06</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">delta_plain</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The level or nearly level surface composing the landward part of a large or compound delta; strictly, an alluvial plain characterized by repeated channel bifurcation and divergence, multiple distributary channels, and interdistributary flood basins</skos:definition>
        <skos:prefLabel xml:lang="en">delta plain</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;deltaic_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#deltaic_system_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;deltaic_system_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.06..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >deltaic_system_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Deltaic system setting</skos:prefLabel>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:definition
            >Environments at the mouth of a river or stream that enters a standing body of water (ocean or lake). The delta forms a triangular or fan-shaped plain of considerable area. Subaerial parts of the delta are crossed by many distributaries of the main river, and commonly extend beyond the general trend of the coast. Subaqueous parts of the delta merge with the adjacent basin floor, and are progressively influenced by non-fluvial processes. Deltas result from the accumulation of sediment supplied by the river in such quantities that it is not removed by tides, waves, and currents. Adapted from the Glossary of Geology definition for delta (Jackson, 1997, p. 167).</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:relatedHasPart rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#distributary_channel -->

    <owl:Thing rdf:about="&EventEnvironment200811;distributary_channel">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.06.03</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >distributary_channel</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A divergent stream flowing away from the main stream and not returning to it, as in a delta or on an alluvial plain</skos:definition>
        <skos:prefLabel xml:lang="en"
            >distributary channel</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;deltaic_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#distributary_mouth -->

    <skos:Concept rdf:about="&EventEnvironment200811;distributary_mouth">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.06.04</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >distributary_mouth</skos:notation>
        <skos:definition
            >The mouth of a delta distributary channel where fluvial discharge moves from confined to unconfined flow conditions</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >distributary mouth</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;deltaic_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#dunefield -->

    <owl:Thing rdf:about="&EventEnvironment200811;dunefield">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.02.2.0.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">dunefield</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >Extensive deposits on sand in an area where the supply is abundant. As a characteristic, individual dunes somewhat resemble barchans but are highly irregular in shape and crowded; erg areas of the Sahara are an example.</skos:definition>
        <skos:prefLabel xml:lang="en">dunefield</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;aeolian_process_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#earth_interior_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;earth_interior_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >earth_interior_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Earth interior setting</skos:prefLabel>
        <skos:definition
            >Geologic environments within the solid Earth.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#earth_surface_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;earth_surface_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >earth_surface_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Earth surface setting</skos:prefLabel>
        <skos:definition
            >Geologic environments on the surface of the solid Earth. Hierarchy presented here is based on assumption that a particular setting may be specified by a combination of a climatic setting with one or more process or geomorphically defined settings..</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#englacial_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;englacial_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.07.0</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >englacial_setting</skos:notation>
        <skos:definition
            >Contained, embedded, or carried within the body of a glacier or ice sheet; said of meltwater streams, till, drift, moraine</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Englacial setting</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;glacier_related_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#epicontinental_marine_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;epicontinental_marine_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.10</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >epicontinental_marine_setting</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:definition
            >Marine setting situated within the interior of the continent, rather than at the edge of a continent.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Epicontinental marine setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#estuarine_delta -->

    <skos:Concept rdf:about="&EventEnvironment200811;estuarine_delta">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.12.02</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">estuarine_delta</skos:notation>
        <skos:definition
            >A delta that has filled, or is in the process of filling, an estuary</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">estuarine delta</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;deltaic_system_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;estuary_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#estuarine_lagoon -->

    <owl:Thing rdf:about="&EventEnvironment200811;estuarine_lagoon">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.12.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >estuarine_lagoon</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A lagoon produced by the temporary sealing of a river estuary by a storm barrier. Such lagoons are usually seasonal and exist until the river breaches the barrier; they occur in regions of low or spasmodic rainfall</skos:definition>
        <skos:prefLabel xml:lang="en"
            >estuarine lagoon</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;estuary_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;lagoonal_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#estuary_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;estuary_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.12</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">estuary_setting</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:definition
            >Environments at the seaward end or the widened funnel-shaped tidal mouth of a river valley where fresh water comes into contact with seawater and where tidal effects are evident (adapted from Glossary of Geology, Jackson, 1997, p. 217).</skos:definition>
        <skos:prefLabel xml:lang="en">Estuary setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#extra_terrestrial_origin -->

    <owl:Thing rdf:about="&EventEnvironment200811;extra_terrestrial_origin">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">03..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >extra_terrestrial_origin</skos:notation>
        <skos:historyNote
            >Neuendorf et al 2005;</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Extra-terrestrial origin</skos:prefLabel>
        <skos:definition
            >Material originated outside of the Earth or its atmosphere.</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#floodplain -->

    <owl:Thing rdf:about="&EventEnvironment200811;floodplain">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.07.4</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">floodplain</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The surface or strip of relatively smooth land adjacent to a river channel, constructed by the present river in its existing regimen and covered with water when the river overflows its banks. It is built of alluvium carried by the river during floods and deposited in the sluggish water beyond the influence of the swiftest current. A river has one floodplain and may have one or more terraces representing abandoned floodplains</skos:definition>
        <skos:prefLabel xml:lang="en">floodplain</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;river_plain_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#forereef -->

    <skos:Concept rdf:about="&EventEnvironment200811;forereef">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.02.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">forereef</skos:notation>
        <skos:definition
            >The seaward side of a reef; the slope covered with deposits of coarse reef talus</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">forereef</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;biological_reef_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#gibber_plain -->

    <skos:Concept rdf:about="&EventEnvironment200811;gibber_plain">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.04..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">gibber_plain</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A desert plain strewn with wind-abraded pebbles, or gibbers; a gravelly desert.</skos:definition>
        <skos:prefLabel xml:lang="en">gibber plain</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;arid_or_semi_arid_environment_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;subaerial_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;terrestrial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#glacial_outwash_plain_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;glacial_outwash_plain_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.07.4</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >glacial_outwash_plain_setting</skos:notation>
        <skos:definition
            >Areas adjacent to glacial front dominanted by sediment and water supplied by glacial melting.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Glacial outwash plain setting</skos:prefLabel>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;glacier_related_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#glacier_lateral_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;glacier_lateral_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.07.1.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >glacier_lateral_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Glacier lateral setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Settings adjacent to edges of confined glacier.</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;proglacial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#glacier_related_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;glacier_related_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.07..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >glacier_related_setting</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Earth surface setting with geography defined by spatial relationship to glaciers (e.g. on top of a glacier, next to a glacier, in front of a glacier...). Processes related to moving ice dominate sediment transport and deposition and landform development. Includes subaqueous, shoreline, and terrestrial settings that are impacted by the presence of glaciers. Considered a geographically defined setting in that a glacier is a geographic feature.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Glacier related setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;polar_climatic_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#glacier_terminus_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;glacier_terminus_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.07.1.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >glacier_terminus_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Glacier terminus setting</skos:prefLabel>
        <skos:definition
            >Region of sediment deposition due to melting of glacier ice. ablation and flow till setting.</skos:definition>
        <skos:historyNote
            >NADM SLTTs, 2004</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;proglacial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#hadal -->

    <skos:Concept rdf:about="&EventEnvironment200811;hadal">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">hadal</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The deepest oceanic environment, i.e., over 6,000 m in depth. Always in deep sea trench.</skos:definition>
        <skos:prefLabel xml:lang="en">hadal</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#high_pressure_low_temperature_earth_interior_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;high_pressure_low_temperature_earth_interior_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.4</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >high_pressure_low_temperature_earth_interior_setting</skos:notation>
        <skos:definition
            >High pressure environment characterized by geothermal gradient significantly lower than standard continental geotherm; enviornment in which blueschist facies metamorphic rocks form. Typically associated with subduction zones.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >High pressure low temperature Earth interior setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_interior_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#hillslope_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;hillslope_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.10..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >hillslope_setting</skos:notation>
        <skos:definition
            >Earth surface setting characterized by surface slope angles high enough that gravity alone becomes a significant factor in geomorphic development, as well as base-of-slope areas influenced by hillslope processes. Hillslope activities include creep, sliding, slumping, falling, and other downslope movements caused by slope collapse induced by gravitational influence on earth materials. May be subaerial or subaqueous.</skos:definition>
        <skos:historyNote
            >NADM SLTTs, 2004; Hawley, J.W., and Parsons, R.B. 1980. Glossary of selected geomorphic and geologic terms. Mimeo. USDA Soil Conservation Service, West National Technical Center, Portland, OR. 30 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Hillslope setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#humid_temperate_climatic_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;humid_temperate_climatic_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.03.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >humid_temperate_climatic_setting</skos:notation>
        <skos:historyNote
            >ecounits (Cleland et al., 1997)</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Humid temperate climatic setting</skos:prefLabel>
        <skos:definition
            >Setting with seasonal climate having hot to cold or humid to arid seasons.</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#humid_tropical_climatic_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;humid_tropical_climatic_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.03.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >humid_tropical_climatic_setting</skos:notation>
        <skos:definition
            >Setting with hot, humid climate influenced by equatorial air masses, no winter season.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Humid tropical climatic setting</skos:prefLabel>
        <skos:historyNote
            >ecounits (Cleland et al., 1997)</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#hypabyssal_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;hypabyssal_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.6</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >hypabyssal_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Hypabyssal setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Igneous environment close to the Earth&#39;s surface, characterized by more rapid cooling than plutonic setting to produce generally fine-grained intrusive igneous rock that is commonly associated with co-magmatic volcanic rocks.</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_interior_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#inner_neritic -->

    <skos:Concept rdf:about="&EventEnvironment200811;inner_neritic">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.041</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">inner_neritic</skos:notation>
        <skos:historyNote
            >this vocabulary JL</skos:historyNote>
        <skos:definition
            >The ocean environment at depths between low tide level and 30 metres</skos:definition>
        <skos:prefLabel xml:lang="en">inner neritic</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;neritic"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#interdistributary_bay -->

    <owl:Thing rdf:about="&EventEnvironment200811;interdistributary_bay">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.06.05</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >interdistributary_bay</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A pronounced indentation of the delta front between advancing stream distributaries, occupied by shallow water, and either open to the sea or partly enclosed by minor distributaries</skos:definition>
        <skos:prefLabel xml:lang="en"
            >interdistributary bay</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;deltaic_system_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;subaqueous_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#intertidal -->

    <skos:Concept rdf:about="&EventEnvironment200811;intertidal">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.10</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">intertidal</skos:notation>
        <skos:definition
            >Pertaining to the benthic ocean environment or depth zone between high water and low water; also, pertaining to the organisms of that environment</skos:definition>
        <skos:prefLabel xml:lang="en">Intertidal</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:broader rdf:resource="&EventEnvironment200811;subaqueous_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;tidal_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#lacustrine_delta -->

    <owl:Thing rdf:about="&EventEnvironment200811;lacustrine_delta">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.02.0</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >lacustrine_delta</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The low, nearly flat, alluvial tract of land at or near the mouth of a river, commonly forming a triangular or fan-shaped plain of considerable area, crossed by many distributaries of the main river, perhaps extending beyond the general trend of the lake shore, resulting from the accumulation of sediment supplied by the river in such quantities that it is not removed by waves or currents. Most deltas are partly subaerial and partly below water.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >lacustrine delta</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;deltaic_system_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;lacustrine_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#lacustrine_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;lacustrine_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.02</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >lacustrine_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Lacustrine setting</skos:prefLabel>
        <skos:definition
            >Setting associated with a lake. Always overlaps with terrestrial, may overlap with subarial, subaqueous, or shoreline.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;terrestrial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#lagoonal_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;lagoonal_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.07</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >lagoonal_setting</skos:notation>
        <skos:definition
            >A shallow stretch of salt or brackish water, partly or completely separated from a sea or lake by an offshore reef, barrier island, sand or spit (Jackson, 1997). Water is shallow, tidal and wave-produced effects on sediments; strong light reaches sediment..</skos:definition>
        <skos:historyNote
            >generalize from Jackson 1997 and NADM SLTTs, 2004;</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Lagoonal setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:broader rdf:resource="&EventEnvironment200811;subaqueous_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#low_energy_shoreline_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;low_energy_shoreline_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >low_energy_shoreline_setting</skos:notation>
        <skos:definition
            >Settings characterized by very low surface slope and proximity to shoreline. Generally within peritidal setting, but characterized by low surface gradients and generally low-energy sedimentary processes.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Low energy shoreline setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#low_pressure_high_temperature_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;low_pressure_high_temperature_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >low_pressure_high_temperature_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Low pressure high temperature setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Setting characterized by temperatures significantly higher that those associated with normal continental geothermal gradient.</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_interior_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#lower_bathyal -->

    <owl:Thing rdf:about="&EventEnvironment200811;lower_bathyal">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.03.03</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">lower_bathyal</skos:notation>
        <skos:definition
            >The ocean environment at depths between 1000 and 3500 metres</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E., Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.; Berggren, W.A. &amp; Miller, K.G., 1989. Cenozoic bathyal and abyssal calcareous benthic foraminiferal zonation. Micropalaeontology 35, 308-320</skos:historyNote>
        <skos:prefLabel xml:lang="en">lower bathyal</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;bathyal"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#lower_continental_crust -->

    <owl:Thing rdf:about="&EventEnvironment200811;lower_continental_crust">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.8.1.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >lower_continental_crust</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Continental crustal setting characterized by upper amphibolite to granulite facies metamorphism, insitu melting, residual anhydrous metamorphic rocks, and ductile flow of rock bodies.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Lower continental crust</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;continental_crust"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#lower_delta_plain -->

    <skos:Concept rdf:about="&EventEnvironment200811;lower_delta_plain">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.06.06.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >lower_delta_plain</skos:notation>
        <skos:definition
            >The part of a delta plain which is penetrated by saline water and is subject to tidal processes</skos:definition>
        <skos:historyNote
            >Reading, H.G. &amp; Collinson, J.D., 1996. Clastic coasts. 154-231 in Reading, H.G. (ed.), Sedimentary Environments: Processses, Facies and Stratigraphy, third edition. Blackwell Science, Oxford</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >lower delta plain</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;delta_plain"/>
        <skos:broader rdf:resource="&EventEnvironment200811;tidal_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#lower_mantle -->

    <skos:Concept rdf:about="&EventEnvironment200811;lower_mantle">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.7.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">lower_mantle</skos:notation>
        <skos:definition
            >That part of the mantle that lies below a depth of about 660 km. With increasing depth, density increases from ~4.4 g/cm3-to ~5.6 g/cm3, and velocity of compressional seismic waves increases from ~10.7 km/s to ~13.7 km/s (Dziewonski and Anderson, 1981).</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">lower mantle</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;mantle"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#lower_oceanic_crustal_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;lower_oceanic_crustal_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.8.2.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >lower_oceanic_crustal_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Lower oceanic crustal setting</skos:prefLabel>
        <skos:definition
            >Setting characterized by dominantly intrusive mafic rocks, with sheeted dike complexes in upper part and gabbroic to ultramafic intrusive or metamorphic rocks in lower part.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;oceanic_crust"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#mantle -->

    <owl:Thing rdf:about="&EventEnvironment200811;mantle">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.7</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">mantle</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The zone of the Earth below the crust and above the core, which is divided into the</skos:definition>
        <skos:prefLabel xml:lang="en">mantle</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_interior_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#marginal_marine_sabkha_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;marginal_marine_sabkha_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.01.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >marginal_marine_sabkha_setting</skos:notation>
        <skos:definition
            >Setting characterized by arid to semi-arid conditions on restricted coastal plains mostly above normal high tide level, with evaporite-saline mineral, tidal-flood, and eolian deposits. Boundaries with intertidal setting and non-tidal terrestrial setting are gradational. (Jackson, 1997, p. 561).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Marginal marine sabkha setting</skos:prefLabel>
        <skos:historyNote
            >NADM SLTTs, 2004; based on Jackson 1997; Neuendorf et al. 2005</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;arid_or_semi_arid_environment_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;low_energy_shoreline_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#marine -->

    <owl:Thing rdf:about="&EventEnvironment200811;marine"/>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#marine_carbonate_platform_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;marine_carbonate_platform_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.09</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >marine_carbonate_platform_setting</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >A shallow submerged plateau separated from the continental landmass, on which high biological carbonate production rates produce enough sediment to maintain the platform surface near sea level. Grades into atoll as area becomes smaller and ringing coral reefs become more prominent part of the setting.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Marine carbonate platform setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#marine_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;marine_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">marine_setting</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004; Webster&#39;s Encyclopedic Unabridged Dictionary of the English Language, 2001, p. 1894.</skos:historyNote>
        <skos:definition
            >Setting characterized by location under the surface of the sea.</skos:definition>
        <skos:prefLabel xml:lang="en">Marine setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;subaqueous_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#meandering_channel -->

    <owl:Thing rdf:about="&EventEnvironment200811;meandering_channel">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.07.1.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >meandering_channel</skos:notation>
        <skos:definition
            >Produced by a mature stream swinging from side to side as it flows across its floodplain or shifts its course laterally toward the convex side of an original curve</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >meandering channel</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;river_channel"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#mid_ocean_ridge_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;mid_ocean_ridge_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.11.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >mid_ocean_ridge_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Mid ocean ridge setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Ocean highland associated with a divergent continental margin (spreading center). Setting is characterized by active volcanism, locally steep relief, hydrothermal activity, and pelagic sedimentation.</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;ocean_highland_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#middle_bathyal -->

    <skos:Concept rdf:about="&EventEnvironment200811;middle_bathyal">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.03.02</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">middle_bathyal</skos:notation>
        <skos:definition
            >The ocean environment at water depths between 600 and 1000 metres</skos:definition>
        <skos:historyNote
            >Berggren, W.A. &amp; Miller, K.G., 1989. Cenozoic bathyal and abyssal calcareous benthic foraminiferal zonation. Micropalaeontology 35, 308-320</skos:historyNote>
        <skos:prefLabel xml:lang="en">middle bathyal</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;bathyal"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#middle_continental_crust_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;middle_continental_crust_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.8.1.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >middle_continental_crust_setting</skos:notation>
        <skos:definition
            >Continental crustal setting characterized by greenschist to upper amphibolite facies metamorphism, plutonic igneous rocks, and ductile deformation.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Middle continental crust setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;continental_crust"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#middle_neritic -->

    <owl:Thing rdf:about="&EventEnvironment200811;middle_neritic">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.04.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">middle_neritic</skos:notation>
        <skos:prefLabel xml:lang="en">middle neritic</skos:prefLabel>
        <skos:historyNote
            >this vocabulary JL</skos:historyNote>
        <skos:definition
            >The ocean environment at depths between 30 and 100 metres</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;neritic"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#mud_flat_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;mud_flat_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.01.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >mud_flat_setting</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004; Jackson, 1997</skos:historyNote>
        <skos:definition
            >A relatively level area of fine grained material (e.g. silt) along a shore (as in a sheltered estuary or chenier-plain) or around an island, alternately covered and uncovered by the tide or covered by shallow water, and barren of vegetation. Includes most tidal flats, but lacks denotation of tidal influence..</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Mud flat setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;low_energy_shoreline_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#neritic -->

    <skos:Concept rdf:about="&EventEnvironment200811;neritic">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.04</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">neritic</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The ocean environment at depths between low-tide level and 200 metres, or between low-tide level and approximately the edge of the continental shelf</skos:definition>
        <skos:prefLabel xml:lang="en">neritic</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#ocean_highland_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;ocean_highland_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.11</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >ocean_highland_setting</skos:notation>
        <skos:definition
            >Broad category for subaqueous marine settings characterized by significant relief above adjacent sea floor.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Ocean highland setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#oceanic_crust -->

    <skos:Concept rdf:about="&EventEnvironment200811;oceanic_crust">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.8.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">oceanic_crust</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >That type of the Earth&#39;s crust which underlies the ocean basins. The oceanic crust is 5-10 km thick; it has a density of 2.9 g/cm3, and compressional seismic-wave velocities travelling through it at 4-7.2 km/sec. Setting in crust produced by submarine volcanism at a mid ocean ridge.</skos:definition>
        <skos:prefLabel xml:lang="en">oceanic crust</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;crust"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#oceanic_plateau_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;oceanic_plateau_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.11.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >oceanic_plateau_setting</skos:notation>
        <skos:definition
            >Region of elevated ocean crust that commonly rises to within 2-3 km of thre surface above an abyssal sea floor that lies several km deeper. Climate and water depths are such that a marine carbonate platform does not develop.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Oceanic plateau setting</skos:prefLabel>
        <skos:historyNote>Reading, 1978</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;ocean_highland_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#outer_neritic -->

    <skos:Concept rdf:about="&EventEnvironment200811;outer_neritic">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.04.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">outer_neritic</skos:notation>
        <skos:historyNote
            >this vocabulary JL</skos:historyNote>
        <skos:definition
            >The ocean environment at depths between 100 and 200 metres or between low-tide level and approximately the edge of the continental shelf</skos:definition>
        <skos:prefLabel xml:lang="en">outer neritic</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;neritic"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#pediment_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;pediment_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.08.3..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >pediment_setting</skos:notation>
        <skos:definition
            >A gently sloping erosional surface developed at the foot of a receding hill or mountain slope. The surface may be essentially bare, exposing earth material that extends beneath adjacent uplands; or it may be thinly mantled with alluvium and colluvium, ultimately in transit from upland front to basin or valley lowland. In hill-foot slope terrain the mantle is designated &quot;pedisediment.&quot; The term has been used in several geomorphic contexts: Pediments may be classed with respect to (a) landscape positions, for example, intermontane-basin piedmont or valley-border footslope surfaces (respectively, apron and terrace pediments (Cooke and Warren, 1973)); (b) type of material eroded, bedrock or regolith; or (c) combinations of the above. Compare - Piedmont slope..</skos:definition>
        <skos:historyNote
            >NADM SLTTs, 2004; NRCS, 2001</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Pediment setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;piedmont_slope_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#piedmont_slope_system_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;piedmont_slope_system_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.08</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >piedmont_slope_system_setting</skos:notation>
        <skos:definition
            >Location on gentle slope at the foot of a mountain; generally used in terms of intermontane-basin terrain. Main components include: (a) An erosional surface on bedrock adjacent to the receding mountain front (pediment, rock pediment); (b) A constructional surface comprising individual alluvial fans and interfan valleys, also near the mountain front; and (c) A distal complex of coalescent fans (bajada), and alluvial slopes without fan form. Piedmont slopes grade to basin-floor depressions with alluvial and temporary lake plains or to surfaces associated with through drainage.</skos:definition>
        <skos:historyNote
            >Hawley and Parsons, 1980+F98</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Piedmont slope system setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;terrestrial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#playa_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;playa_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.01..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">playa_setting</skos:notation>
        <skos:historyNote
            >Based on Hawley and Parsons, 1980</skos:historyNote>
        <skos:definition
            >The usually dry and nearly level plain that occupies the lowest parts of closed depressions, such as those occurring on intermontane basin floors. Temporary flooding occurs primarily in response to precipitation-runoff events.</skos:definition>
        <skos:prefLabel xml:lang="en">Playa setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;arid_or_semi_arid_environment_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;terrestrial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#polar_climatic_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;polar_climatic_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.03.4</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >polar_climatic_setting</skos:notation>
        <skos:historyNote
            >ecounits (Cleland et al., 1997)</skos:historyNote>
        <skos:definition
            >Setting with climate dominated by temperatures below the freezing temperature of water. Includes polar deserts because precipitation is generally scant at high latitude. Climate controlled by arctic air masses, cold dry enviornment with short summer.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Polar climatic setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#prodelta -->

    <owl:Thing rdf:about="&EventEnvironment200811;prodelta">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.06.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">prodelta</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >The part of a delta that is below the effective depth of wave erosion, lying beyond the delta front, and sloping gently down to the floor of the basin into which the delta is advancing and where clastic river sediment ceases to be a significant part of the basin-floor deposits; it is entirely below the water level</skos:definition>
        <skos:prefLabel xml:lang="en">prodelta</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;deltaic_system_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;subaqueous_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#proglacial_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;proglacial_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.07.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >proglacial_setting</skos:notation>
        <skos:definition
            >Immediately in front of or just beyond the outer limits of a glacier or ice sheet, generally at or near its lower end; said of lakes, streams, deposits, and other features produced by or derived from the glacier ice</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Proglacial setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;glacier_related_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#reef_flat -->

    <skos:Concept rdf:about="&EventEnvironment200811;reef_flat">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.02.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">reef_flat</skos:notation>
        <skos:definition
            >A stony platform of</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">reef flat</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;biological_reef_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#regional_metamorphic_origin -->

    <skos:Concept rdf:about="&EventEnvironment200811;regional_metamorphic_origin">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >regional_metamorphic_origin</skos:notation>
        <skos:historyNote
            >NADM metamorphic rock vocabulary SLTTm1.0, 2004</skos:historyNote>
        <skos:definition
            >Metamorphism not obviously localized along contacts of igneous bodies; includes burial metamorphism and ocean ridge metamorphism</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Regional metamorphic origin</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_interior_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#river_channel -->

    <skos:Concept rdf:about="&EventEnvironment200811;river_channel">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.07.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">river_channel</skos:notation>
        <skos:definition
            >The bed where a natural body of surface water flows or may flow; a natural passageway or depression of perceptible extent containing continuously or periodically flowing water, or forming a connecting link between two bodies of water; a watercourse</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">River channel</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;river_plain_system_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#river_plain_system_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;river_plain_system_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.07..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >river_plain_system_setting</skos:notation>
        <skos:definition
            >Geologic setting dominated by a river system; river plains may occur in any climatic setting. Includes active channels, abandoned channels, levees, oxbow lakes, flood plain. May be part of an alluvial plain that includes terraces composed of abandoned river plain deposits.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >River plain system setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;terrestrial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#rocky_coast_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;rocky_coast_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.08</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >rocky_coast_setting</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:definition
            >Shoreline with significant relief and abundant rock outcrop.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Rocky coast setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#sand_plain -->

    <skos:Concept rdf:about="&EventEnvironment200811;sand_plain">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.02.2.0.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">sand_plain</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A sand-covered plain dominated by aeolian processes.</skos:definition>
        <skos:prefLabel xml:lang="en">sand plain</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;aeolian_process_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;arid_or_semi_arid_environment_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#seamount_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;seamount_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.11.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >seamount_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Seamount setting</skos:prefLabel>
        <skos:definition
            >Setting that consists of a conical mountain on the ocean floor (guyot). Typically characterized by active volcanism, pelagic sedimentation. If the mountain is high enough to reach the photic zone, carbonate production may result in reef building to produce a carbonate platform or atoll setting.</skos:definition>
        <skos:historyNote
            >Reading, 1978+F63</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;ocean_highland_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#shoreline_settings -->

    <owl:Thing rdf:about="&EventEnvironment200811;shoreline_settings">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >shoreline_settings</skos:notation>
        <skos:definition
            >Geologic settings characterized by location adjacent to the ocean, or a lake. A strip of land of indefinite width (may be many kilometers), bordering a body of water that extends from the water line inland to the first major change in landform features. Marine or non-marine not specified. Includes settings that may be subarial or subaqueous, but are intrinsically associated with the interface between land areas and water bodies..</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Shoreline settings</skos:prefLabel>
        <skos:historyNote
            >based on Neuendorf et al, 2005</skos:historyNote>
        <rdfs:comment xml:lang="en"
            >need to revise definition to include subaerial and subaqueous zone at interface between body of water and land where the two interact.</rdfs:comment>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#slope_rise_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;slope_rise_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.05</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >slope_rise_setting</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Slope-rise setting</skos:prefLabel>
        <skos:historyNote
            >based on NADM SLTTs, 2004</skos:historyNote>
        <skos:definition
            >The part of a subaqueous basin that is between a bordering shelf setting, which separate the basin from an adjacent landmass, and a very low-relief basin plain setting.</skos:definition>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#strandplain_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;strandplain_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.09..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >strandplain_setting</skos:notation>
        <skos:historyNote
            >NADM SLTTs, 2004; based on Jackson 1997</skos:historyNote>
        <skos:definition
            >A prograded shore built seaward by waves and currents, and continuous for some distance along the coast. It is characterized by subparallel beach ridges and swales, in places with associated dunes.&quot; Glossary of Geology (Jackson, 1997, p. 626).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Strandplain setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#subaerial_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;subaerial_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.02.2..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >subaerial_setting</skos:notation>
        <skos:definition
            >Setting at the interface between the solid earth and the atmosphere, includes some shallow subaqueous settings in river channels and playas. Characterized by conditions and processes, such as erosion, that exist or op.erate in the open air on or immediately adjacent to the land surface</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Subaerial setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:broader rdf:resource="&EventEnvironment200811;terrestrial_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#subaqueous_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;subaqueous_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.02.1..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >subaqueous_setting</skos:notation>
        <skos:historyNote
            >based on NADM SLTTs, 2004;</skos:historyNote>
        <skos:definition
            >Setting situated in or under permanent, standing water. Used for marine and lacustrine settings, but not for fluvial settings.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Subaqueous setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#subglacial_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;subglacial_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.07.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >subglacial_setting</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >Formed or accumulated in or by the bottom parts of a glacier or ice sheet; said of meltwater streams, till, moraine, etc.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Subglacial setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;glacier_related_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#submarine_fan_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;submarine_fan_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.06</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >submarine_fan_setting</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Large fan-shaped cones of sediment on the ocean floor, generally associated with submarine canyons that provide sediment supply to build the fan..</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Submarine fan setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;marine"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#supraglacial_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;supraglacial_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.07.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >supraglacial_setting</skos:notation>
        <skos:definition
            >&quot;Carried upon, deposited from, or pertaining to the top surface of a glacier or ice sheet; said of meltwater streams, till, drift, etc. &quot; (Jackson, 1997, p. 639). Dreimanis (1988, p. 39) recommendation that &quot;supraglacial&quot; supercede &quot;superglacial&quot; is followed.</skos:definition>
        <skos:historyNote
            >NADM SLTTs, 2004;</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Supraglacial setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;glacier_related_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#supratidal -->

    <skos:Concept rdf:about="&EventEnvironment200811;supratidal">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.02</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">supratidal</skos:notation>
        <skos:definition
            >Pertaining to the shore area marginal to the littoral zone, just above high-tide level</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">supratidal</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;shoreline_settings"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#swamp_or_marsh_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;swamp_or_marsh_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00.03.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >swamp_or_marsh_setting</skos:notation>
        <skos:definition
            >A water-saturated, periodically wet or continually flooded area with the surface not deeply submerged, essentially without the formation of peat. Marshes are characterized by sedges, cattails, rushes, or other aquatic and grasslike vegetation. Swamps are characterized by tree and brush vegetation.</skos:definition>
        <skos:historyNote
            >Neuendorf et al 2005; Soil Science Society of America, 1997.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Swamp or marsh setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;wetland_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#terrestrial_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;terrestrial_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.00..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >terrestrial_setting</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Setting characterized by absence of direct marine influence. Most of the subaerial settings are also terrestrial, but lacustrine settings, while terrestrial, are not subaerial, so the subaerial settings are not included as subcategories.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Terrestrial setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#tidal_channel -->

    <owl:Thing rdf:about="&EventEnvironment200811;tidal_channel">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.11.01.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">tidal_channel</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A major channel followed by the tidal currents, extending from offshore into a tidal marsh or a tidal flat.</skos:definition>
        <skos:prefLabel xml:lang="en">tidal channel</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;tidal_flat_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#tidal_flat_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;tidal_flat_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.11.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >tidal_flat_setting</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >An extensive, nearly horizontal, barren tract of land that is alternately covered and uncovered by the tide, and consisting of unconsolidated sediment (mostly mud and sand). It may form the top surface of a deltaic deposit.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Tidal flat setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;tidal_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#tidal_marsh -->

    <skos:Concept rdf:about="&EventEnvironment200811;tidal_marsh">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.11.01.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">tidal_marsh</skos:notation>
        <skos:definition
            >A marsh bordering a coast (as in a shallow lagoon or sheltered bay), formed of mud and of the resistant mat of roots of salt-tolerant plants, and regularly inundated during high tides; a marshy tidal flat.</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">tidal marsh</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;tidal_flat_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#tidal_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;tidal_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.04.11</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">tidal_setting</skos:notation>
        <skos:definition
            >Setting subject to tidal processes</skos:definition>
        <skos:prefLabel xml:lang="en">Tidal setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_surface_setting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#transitional_crust -->

    <skos:Concept rdf:about="&EventEnvironment200811;transitional_crust">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.8.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >transitional_crust</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >Crust formed in the transition zone between continental and oceanic crust, during the history of continental rifting that culminates in the formation of a new ocean.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Transitional crust</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;crust"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#ultra_high_pressure_crustal_setting -->

    <owl:Thing rdf:about="&EventEnvironment200811;ultra_high_pressure_crustal_setting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.5</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >ultra_high_pressure_crustal_setting</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Setting characterized by pressures characteristic of upper mantle, but indicated by mineral assemblage in crustal composition rocks.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Ultra high pressure crustal setting</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;earth_interior_setting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#upper_bathyal -->

    <skos:Concept rdf:about="&EventEnvironment200811;upper_bathyal">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.05.01.03.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811">upper_bathyal</skos:notation>
        <skos:definition
            >The ocean environment at water depths between 200 and 600 metres</skos:definition>
        <skos:historyNote
            >Berggren, W.A. &amp; Miller, K.G., 1989. Cenozoic bathyal and abyssal calcareous benthic foraminiferal zonation. Micropalaeontology 35, 308-320</skos:historyNote>
        <skos:prefLabel xml:lang="en">upper bathyal</skos:prefLabel>
        <skos:broader rdf:resource="&EventEnvironment200811;bathyal"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#upper_continental_crustal_setting -->

    <skos:Concept rdf:about="&EventEnvironment200811;upper_continental_crustal_setting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">02.0.8.1.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >upper_continental_crustal_setting</skos:notation>
        <skos:definition
            >Continental crustal setting dominated by non metamorphosed to low greenschist facies metamorphic rocks, and brittle deformation.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Upper continental crustal setting</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventEnvironment200811;continental_crust"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventEnvironment:200811#upper_delta_plain -->

    <owl:Thing rdf:about="&EventEnvironment200811;upper_delta_plain">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">01.06.06.02</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventEnvironment:200811"
            >upper_delta_plain</skos:notation>
        <skos:historyNote
            >Reading, H.G. &amp; Collinson, J.D., 1996. Clastic coasts. 154-231 in Reading, H.G. (ed.), Sedimentary Environments: Processses, Facies and Stratigraphy, third edition. Blackwell Science, Oxford</skos:historyNote>
        <skos:definition
            >The part of a delta plain essentially unaffected by basinal processes. They do not differ substantially from alluvial environments except that areas of swamp, marsh and lakes are usually more widespread and channels may bifurcate downstream</skos:definition>
        <skos:prefLabel xml:lang="en"
            >upper delta plain</skos:prefLabel>
