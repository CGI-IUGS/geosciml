    <!-- urn:cgi:classifier:CGI:ParticleShape:200811#angular -->

    <skos:Concept rdf:about="&ParticleShape200811;angular">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleShape:200811">angular</skos:notation>
        <skos:historyNote
            >Powers, M. C., 1953 A new roundness scale for sedimentary particles. Journal of Sedimentary Petrology, v. 23, p. 117-119</skos:historyNote>
        <skos:definition
            >A clastic sedimentary particle showing very little or no evidence of abrasion, with all of its edges and corners sharp, such as blocks with numerous (15-30) secondary corners and a roundness value between 0.17 and 0.25 (midpoint at 0.21).</skos:definition>
        <skos:prefLabel xml:lang="en">angular</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:ParticleShape:200811#anhedral -->

    <owl:Thing rdf:about="&ParticleShape200811;anhedral">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ParticleShape:200811">anhedral</skos:notation>
        <skos:historyNote
            >MacKenzie, W.S., Donaldson, C.H. &amp; Guilford, C., 1982. Atlas of igneous rocks and their textures. John Wiley &amp; Sons, New York</skos:historyNote>
        <skos:definition
            >Crystalline particles in a rock lack well-developed crystal faces; usually referring to igneous or metamorphic grains</skos:definition>
        <skos:prefLabel xml:lang="en">anhedral</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


