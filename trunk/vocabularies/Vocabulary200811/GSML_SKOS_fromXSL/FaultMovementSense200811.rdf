  <skos:ConceptScheme rdf:about="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">
    <skos:prefLabel xml:lang="en">FaultMovementSense</skos:prefLabel>
    <skos:changeNote>
      <rdf:Description>
        <rdf:value>Converted from Excel document.</rdf:value>
        <dc:date>2009-07-14</dc:date>
        <dc:creator>Stephen M Richard</dc:creator>
      </rdf:Description>
    </skos:changeNote>
    <skos:editorialNote>This file contains the 200811 version of CGI Fault Movement Sense vocabulary. Terms to specify the relative direction of movement of bodies of rock across a fault (GeoSciML ShearDisplacementStructure) surface. Examples include 'sinistral', 'dextral', 'normal', 'reverse'.</skos:editorialNote>
  </skos:ConceptScheme>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:dextral">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>Right-lateral separation sense; in plan view, the side opposite the observer appears displaced to the right.</skos:definition>
    <skos:prefLabel xml:lang="en">dextral</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">dextral</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">01..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:sinistral">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>Left-lateral separation sense; in plan view, the side opposite the observer appears displaced to the left.</skos:definition>
    <skos:prefLabel xml:lang="en">sinistral</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">sinistral</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">02..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:normal">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>The hanging wall appears to have moved down relative to the footwall; dip of fault usually 45-90 degrees.</skos:definition>
    <skos:prefLabel xml:lang="en">normal</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">normal</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">03..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:detachment">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>A regional-scale low-angle normal fault.</skos:definition>
    <skos:prefLabel xml:lang="en">detachment</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">detachment</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">03.1.</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:reverse">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>The hanging wall appears to have moved down relative to the footwall; dip of fault usually greater than 45 degrees.</skos:definition>
    <skos:prefLabel xml:lang="en">reverse</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">reverse</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">04..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:thrust">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>Reverse fault with dip typically less than 45 degrees; horizontal compression, rather than vertical displacement is characteristic.</skos:definition>
    <skos:prefLabel xml:lang="en">thrust</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">thrust</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">05..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:thrust_decollement">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>A regional-scale low-angle thrust fault.</skos:definition>
    <skos:prefLabel xml:lang="en">thrust decollement</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">thrust_decollement</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">05.1.</skos:notation>
    <skos:historyNote>GeoScience Victoria and others</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:normal_dextral">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>The movement sense includes both normal and dextral components.</skos:definition>
    <skos:prefLabel xml:lang="en">normal dextral</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">normal_dextral</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">06..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:normal_sinistral">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>The movement sense includes both normal and sinistral components.</skos:definition>
    <skos:prefLabel xml:lang="en">normal sinistral</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">normal_sinistral</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">07..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:reverse_dextral">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>The movement sense includes both reverse and dextral components.</skos:definition>
    <skos:prefLabel xml:lang="en">reverse dextral</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">reverse_dextral</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">08..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:reverse_sinistral">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>The movement sense includes both reverse and sinistral components.</skos:definition>
    <skos:prefLabel xml:lang="en">reverse sinistral</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">reverse_sinistral</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">09..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:generic_decollement">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>A large-displacement (kilometers or tens of kilometers) shallowly dipping to subhorizontal fault or shear zone.</skos:definition>
    <skos:prefLabel xml:lang="en">generic decollement</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">generic_decollement</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">10..</skos:notation>
    <skos:historyNote>Neuendorf et al. 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementSense:200811:no_movement_sense">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811" />
    <skos:definition>The fault-parallel displacement is effectively zero, as in an extraction fault.</skos:definition>
    <skos:prefLabel xml:lang="en">no movement sense</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementSense:200811">no_movement_sense</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">11..</skos:notation>
    <skos:historyNote>Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</skos:historyNote>
  </skos:Concept>
</rdf:RDF>