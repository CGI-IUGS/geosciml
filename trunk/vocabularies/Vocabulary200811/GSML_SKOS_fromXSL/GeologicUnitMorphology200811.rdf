  <skos:ConceptScheme rdf:about="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">
    <skos:prefLabel xml:lang="en">GeologicUnitMorphology</skos:prefLabel>
    <skos:changeNote>
      <rdf:Description>
        <rdf:value>Converted from Excel document.</rdf:value>
        <dc:date>2009-07-14</dc:date>
