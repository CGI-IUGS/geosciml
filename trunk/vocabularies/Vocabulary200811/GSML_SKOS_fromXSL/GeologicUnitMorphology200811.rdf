    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#arcuate_sheet -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;arcuate_sheet">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.01.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">arcuate_sheet</skos:notation>
        <skos:prefLabel xml:lang="en">arcuate sheet</skos:prefLabel>
        <skos:definition
            >sheet in which one of L or W is a curved arc</skos:definition>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;sheet"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#blob -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;blob">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.07.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">blob</skos:notation>
        <skos:definition
            >Body with smooth surface, broadly equant shape, irregular protuberances, no symmetry axes, and a boundary that can not be separated into sides.</skos:definition>
        <skos:prefLabel xml:lang="en">blob</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;three_dimensional_body"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#block -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;block">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.04.3.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">block</skos:notation>
        <skos:definition
            >Equant polygonal cylinder, L=W=H, definable edges separate bounding surfaces. Grades to tabular prism as one horizontal dimension becomes distinctly longer than the other, and tablet or polygonal cylinder as one vertical axis becomes distinctly shorter or longer than the horizontal axes.</skos:definition>
        <skos:prefLabel xml:lang="en">block</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;polygonal_cylinder"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#bread_loaf_ellipsoid -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;bread_loaf_ellipsoid">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.6.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811"
            >bread_loaf_ellipsoid</skos:notation>
        <skos:definition
            >Ellipsoid with L 10 times W or H</skos:definition>
        <skos:prefLabel xml:lang="en"
            >bread loaf ellipsoid</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;ellipsoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#circular_cylinder -->

    <owl:Thing rdf:about="&GeologicUnitMorphology200811;circular_cylinder">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.04.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811"
            >circular_cylinder</skos:notation>
        <skos:definition
            >Elliptical cylinder that has circular symmetry about long axis; distinct side surfaces are not defined. Cross section is circular</skos:definition>
        <skos:prefLabel xml:lang="en"
            >circular cylinder</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;cylinder"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#circular_profile -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;circular_profile">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.3.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811"
            >circular_profile</skos:notation>
        <skos:definition
            >equant profile with arcuate boundary</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Circular profile</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;equant_profile"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#cone -->

    <owl:Thing rdf:about="&GeologicUnitMorphology200811;cone">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.05.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">cone</skos:notation>
        <skos:prefLabel xml:lang="en">cone</skos:prefLabel>
        <skos:definition
            >discus with one planar side, and circular symmetry on axis; geologic cones typically have height &lt;(&lt;) diameter</skos:definition>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;discus"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#cone_segment -->

    <owl:Thing rdf:about="&GeologicUnitMorphology200811;cone_segment">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.10.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">cone_segment</skos:notation>
        <skos:definition
            >Body that is section of a cone formed by splitting the cone along a surface parallel to the cone axis, that interesect approximately along the axis.</skos:definition>
        <skos:prefLabel xml:lang="en">cone segment</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;three_dimensional_body"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#conical_sheet -->

    <owl:Thing rdf:about="&GeologicUnitMorphology200811;conical_sheet">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.01.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">conical_sheet</skos:notation>
        <skos:definition
            >sheet that conforms to surface of a cone.</skos:definition>
        <skos:prefLabel xml:lang="en">conical sheet</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;sheet"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#cylinder -->

    <owl:Thing rdf:about="&GeologicUnitMorphology200811;cylinder">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.04..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">cylinder</skos:notation>
        <skos:definition
            >Body defined by translation of a generating plane figure with approximately equant cross section (circle, ellipsoid, polygon) along a straight axial line. H&gt;=W=L. Axis is direction normal to generating plane figure, which forms parallel top and bottom bounding surface.</skos:definition>
        <skos:prefLabel xml:lang="en">cylinder</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;three_dimensional_body"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#dart_ellipsoid -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;dart_ellipsoid">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.7.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">dart_ellipsoid</skos:notation>
        <skos:definition
            >Ellipsoid with L more than 50 times W or H</skos:definition>
        <skos:prefLabel xml:lang="en">dart ellipsoid</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;ellipsoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#disc -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;disc">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.04.5.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">disc</skos:notation>
        <skos:definition
            >Cylinder with height less than diameter. L=W&gt;H; edges separates side from top and bottom. Grades to oblate ellipsoid as edges lose definition, .</skos:definition>
        <skos:prefLabel xml:lang="en">disc</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;cylinder"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#discoid_anulus -->

    <owl:Thing rdf:about="&GeologicUnitMorphology200811;discoid_anulus">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.11.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">discoid_anulus</skos:notation>
        <skos:definition
            >Cone with central part removed along cylindrical volume approximately along the cone axis</skos:definition>
        <skos:prefLabel xml:lang="en">discoid anulus</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;three_dimensional_body"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#discus -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;discus">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.05..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">discus</skos:notation>
        <skos:definition
            >Body with two sides that converge to define an edge that is circular in profile; edge separates definable top and bottom. L=W&gt;H. Grades to oblate ellipsoid as edge separating sides loses definition, cone as one side becomes distinctly planar.</skos:definition>
        <skos:prefLabel xml:lang="en">discus</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;three_dimensional_body"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#dome_sheet -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;dome_sheet">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.01.4.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">dome_sheet</skos:notation>
        <skos:definition
            >sheet in which L and W are curved arcs.</skos:definition>
        <skos:prefLabel xml:lang="en">dome sheet</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;sheet"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#ellipsoid -->

    <owl:Thing rdf:about="&GeologicUnitMorphology200811;ellipsoid">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">ellipsoid</skos:notation>
        <skos:definition
            >Body that contains three orthogonal axes defining three surfaces of reflection symmetry, profiles along the surfaces are elliptical. Boundary can not be separated into sides (no edges). Becomes cylinder or disc when bounding surface can be considered parallel to one of the axis. L&gt;=W&gt;=H</skos:definition>
        <skos:prefLabel xml:lang="en">ellipsoid</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;three_dimensional_body"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#elliptical_cylinder -->

    <owl:Thing rdf:about="&GeologicUnitMorphology200811;elliptical_cylinder">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.04.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811"
            >elliptical_cylinder</skos:notation>
        <skos:definition
            >Cylinder that has elliptical cross section; distinct side surfaces are not defined.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >elliptical cylinder</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;cylinder"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#elliptical_profile -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;elliptical_profile">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811"
            >elliptical_profile</skos:notation>
        <skos:definition
            >Profile approximates an ellipse; ratio of long axis to short axis is greater than 1.4</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Elliptical profile</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;planar_profile"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#elongate_discus -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;elongate_discus">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.05.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">elongate_discus</skos:notation>
        <skos:definition
            >discus for which the profile defined by the bounding edge is longer in one direction than other. L&gt;W&gt;&gt;H</skos:definition>
        <skos:prefLabel xml:lang="en">elongate discus</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;discus"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#equant_polygonal_profile -->

    <owl:Thing rdf:about="&GeologicUnitMorphology200811;equant_polygonal_profile">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.3.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811"
            >equant_polygonal_profile</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Equant polygonal profile</skos:prefLabel>
        <skos:definition
            >equant profile with boundary that can be divided into distinct straight sides separated by higher curvature corners.</skos:definition>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;equant_profile"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#equant_profile -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;equant_profile">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.3.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">equant_profile</skos:notation>
        <skos:prefLabel xml:lang="en">Equant profile</skos:prefLabel>
        <skos:definition
            >profile for which the ratio of length of longest chord to shortest chord is less than 1.4.</skos:definition>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;planar_profile"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#football_ellipsoid -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;football_ellipsoid">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.5.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811"
            >football_ellipsoid</skos:notation>
        <skos:definition
            >Ellipsoid with L 2 to 5 times W or H</skos:definition>
        <skos:prefLabel xml:lang="en"
            >football ellipsoid</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;ellipsoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#hemispheroid -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;hemispheroid">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.12.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeologicUnitMorphology:200811">hemispheroid</skos:notation>
        <skos:definition
            >Section of a spheroid formed by removing everything above a surface that intersects the spheroid. Grades to cone as suface generator (line rotated about axis to generate volume above lower planar boundary) becomes straight.</skos:definition>
        <skos:prefLabel xml:lang="en">hemispheroid</skos:prefLabel>
        <skos:broader rdf:resource="&GeologicUnitMorphology200811;three_dimensional_body"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeologicUnitMorphology:200811#irregular_profile -->

    <skos:Concept rdf:about="&GeologicUnitMorphology200811;irregular_profile">
        <rdf:type rdf:resource="&owl;Thing"/>
