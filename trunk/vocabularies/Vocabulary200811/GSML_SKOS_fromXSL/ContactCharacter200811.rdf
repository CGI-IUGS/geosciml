  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ContactCharacter:200811:gradational">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ContactCharacter:200811" />
    <skos:definition>contact between geologic units is clearly defined by a gradual change in one or more physical characteristics</skos:definition>
    <skos:prefLabel xml:lang="en">gradational</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ContactCharacter:200811">gradational</skos:notation>
