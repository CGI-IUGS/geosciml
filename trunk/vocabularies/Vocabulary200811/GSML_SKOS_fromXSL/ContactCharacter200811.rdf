  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ContactCharacter:200811:sharp">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ContactCharacter:200811" />
    <skos:definition>contact between geologic units is sharply defined by a change in one or more physical characteristics</skos:definition>
    <skos:prefLabel xml:lang="en">sharp</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ContactCharacter:200811">sharp</skos:notation>
