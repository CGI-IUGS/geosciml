    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#argillic -->

    <owl:Thing rdf:about="&CompositionCategory200811;argillic">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.06.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">argillic</skos:notation>
        <skos:prefLabel xml:lang="en">Argillic</skos:prefLabel>
        <skos:definition
            >Greater than accessory amount (greater than 5 percent by volume) of clay is present</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#arkosic_detrital_mode -->

    <owl:Thing rdf:about="&CompositionCategory200811;arkosic_detrital_mode">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.07.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >arkosic_detrital_mode</skos:notation>
        <skos:definition
            >Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the feldspar to lithic ratio is greater than 3 to 1. This corresponds to the arkose field of Folk (1968). For clastic rocks with little or no matrix and cement, rocks in this detrital mode composition category will also have quartz-feldspathic mineralogical composition.</skos:definition>
        <skos:historyNote>Folk1968</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Arkosic detrital mode</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;feldspathic_detrital_mode"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#asphaltic -->

    <owl:Thing rdf:about="&CompositionCategory200811;asphaltic">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.00.1.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">asphaltic</skos:notation>
        <skos:definition
            >Material contains a significant amount of asphalt, which is &#8220;A dark brown to black viscous liquid or low-melting solid bitumen that consists almost entirely of carbon and hydrogen...[and] formed in oil-bearing rocks by the evaporation of volatiles...&#8221; (Jackson, 1997, p. 39).</skos:definition>
        <skos:historyNote>Jackson 1997</skos:historyNote>
        <skos:prefLabel xml:lang="en">asphaltic</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;carbonaceous"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#barium_bearing_carbonate_chemistry -->

    <owl:Thing rdf:about="&CompositionCategory200811;barium_bearing_carbonate_chemistry">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.0</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >barium_bearing_carbonate_chemistry</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Barium-bearing carbonate chemistry</skos:prefLabel>
        <skos:definition
            >CO3 is the predominant anion, and greater than 5 mole percent of cations are Ba.</skos:definition>
        <skos:historyNote>SMR definition</skos:historyNote>
        <skos:broader rdf:resource="&CompositionCategory200811;carbonate_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#bioclastic -->

    <owl:Thing rdf:about="&CompositionCategory200811;bioclastic">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.01.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">bioclastic</skos:notation>
        <skos:prefLabel xml:lang="en">Bioclastic</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Carbonate sedimentary material contains significant broken skeletal material</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#calcareous -->

    <skos:Concept rdf:about="&CompositionCategory200811;calcareous">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.01.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">calcareous</skos:notation>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en">Calcareous</skos:prefLabel>
        <skos:definition
            >Calcium carbonate minerals are present, carbonate minerals form less than 50 percent of rock, material contains sufficient CaCO3 to react forming bubbles when hydrochloric acid is applied.</skos:definition>
        <skos:broader rdf:resource="&CompositionCategory200811;carbonate_bearing"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#calcareous_monomeralic_carbonate -->

    <skos:Concept rdf:about="&CompositionCategory200811;calcareous_monomeralic_carbonate">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.03.2.1.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >calcareous_monomeralic_carbonate</skos:notation>
        <skos:definition
            >Carbonate minerals form greater than 75 percent of rock, and calcium carbonate minerals forms greater than 75 percent of carbonate minerals.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Calcareous monomeralic carbonate</skos:prefLabel>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:broader rdf:resource="&CompositionCategory200811;monomineralic_carbonate"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#calcium_rich_carbonate_chemistry -->

    <skos:Concept rdf:about="&CompositionCategory200811;calcium_rich_carbonate_chemistry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >calcium_rich_carbonate_chemistry</skos:notation>
        <skos:historyNote
            >Struik etal 2002; this vocabulary</skos:historyNote>
        <skos:definition
            >CO3 is the predominante anion, and the Ca is the most abundant (mole percent) cation.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Calcium-rich carbonate chemistry</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;carbonate_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#calcsilicate -->

    <owl:Thing rdf:about="&CompositionCategory200811;calcsilicate">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.03.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">calcsilicate</skos:notation>
        <skos:definition
            >Material consists of greater than or equal to 50 percent calcsilicate or carbonate minerals and carbonate minerals less than or equal to calcsilicate minerals in mineral mode.</skos:definition>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en">Calcsilicate</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;impure_calcsilicate_or_carbonate"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#calcsilicate_bearing -->

    <owl:Thing rdf:about="&CompositionCategory200811;calcsilicate_bearing">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.02.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >calcsilicate_bearing</skos:notation>
        <skos:definition
            >Calcsilicate minerals of any sort are present, but less than 50 percent of rock</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >calcsilicate-bearing</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#carbonaceous -->

    <owl:Thing rdf:about="&CompositionCategory200811;carbonaceous">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.00.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">carbonaceous</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Material containing significant carbon or hydrocarbon mineral phases, including graphite, coal, oil, gas, lignite, peat.</skos:definition>
        <skos:prefLabel xml:lang="en">carbonaceous</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#carbonaceous_chemistry -->

    <skos:Concept rdf:about="&CompositionCategory200811;carbonaceous_chemistry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.05.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >carbonaceous_chemistry</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Carbonaceous chemistry</skos:prefLabel>
        <skos:historyNote
            >Struik2002; SLTTs2004; This vocabulary</skos:historyNote>
        <skos:definition
            >Containing greater than 5 percent reduced carbon in compounds that are typically the diagenetic products of dead organisms (e.g. coal, bitumen and petroleum). 5 percent value is chosen for consistency with SLTTs usage of term carbonaceous.</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#carbonate -->

    <owl:Thing rdf:about="&CompositionCategory200811;carbonate">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.03.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">carbonate</skos:notation>
        <skos:prefLabel xml:lang="en">Carbonate</skos:prefLabel>
        <skos:definition
            >Material consists of greater than 50 percent calcsilicate or carbonate minerals and carbonate minerals greater than calcsilicate minerals in mineral mode. Metacarbonate of NADMSC SLTTm (2004) terminology, adopted here for non-genetic use.</skos:definition>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:broader rdf:resource="&CompositionCategory200811;impure_calcsilicate_or_carbonate"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#carbonate_bearing -->

    <owl:Thing rdf:about="&CompositionCategory200811;carbonate_bearing">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.01.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >carbonate_bearing</skos:notation>
        <skos:definition
            >Carbonate minerals of any sort are present, but less than 50 percent of rock.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Carbonate bearing</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#carbonate_chemistry -->

    <owl:Thing rdf:about="&CompositionCategory200811;carbonate_chemistry">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >carbonate_chemistry</skos:notation>
        <skos:definition
            >The carbonate (CO3) anion is the predominant anion (by mole percent) in chemical analysis. Sub-categories may be defined by dominant cation. Material in this chemical class overlaps with the monomineralic carbonate class, and probably overlaps with the Carbonate class in the mineralogical composition classification.</skos:definition>
        <skos:historyNote
            >Struik etal 2002; this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Carbonate chemistry</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#chloride_chemistry -->

    <skos:Concept rdf:about="&CompositionCategory200811;chloride_chemistry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.07.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >chloride_chemistry</skos:notation>
        <skos:definition
            >Chloride is the most abundant (mole percent) anion</skos:definition>
        <skos:historyNote
            >Struik etal 2002; this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Chloride chemistry</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;halide_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#chloritic -->

    <skos:Concept rdf:about="&CompositionCategory200811;chloritic">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.07.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">chloritic</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">Chloritic</skos:prefLabel>
        <skos:definition
            >Greater than accessory amount (greater than 5 percent by volume) of chlorite group minerals are present</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#composition_not_specified -->

    <owl:Thing rdf:about="&CompositionCategory200811;composition_not_specified">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">4.02.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >composition_not_specified</skos:notation>
        <skos:definition
            >Composition not specified. Use in normative descriptions where any composition property is allowed.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Composition not specified</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#dolomite_monomineralic_carbonate -->

    <skos:Concept rdf:about="&CompositionCategory200811;dolomite_monomineralic_carbonate">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.03.2.1.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >dolomite_monomineralic_carbonate</skos:notation>
        <skos:definition
            >Carbonate minerals form greater than 75 percent of rock, and dolomite forms greater than 75 percent of carbonate minerals.</skos:definition>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Dolomite monomineralic carbonate</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;monomineralic_carbonate"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#dolomitic -->

    <skos:Concept rdf:about="&CompositionCategory200811;dolomitic">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.01.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">dolomitic</skos:notation>
        <skos:definition
            >Dolomite is present, carbonate minerals form less than 50 percent of rock.</skos:definition>
        <skos:prefLabel xml:lang="en">dolomitic</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&CompositionCategory200811;carbonate_bearing"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#feldspathic -->

    <owl:Thing rdf:about="&CompositionCategory200811;feldspathic">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.05.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">feldspathic</skos:notation>
        <skos:definition
            >Sedimentary material contains significant feldspar mineral content</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">Feldspathic</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#feldspathic_detrital_mode -->

    <owl:Thing rdf:about="&CompositionCategory200811;feldspathic_detrital_mode">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.07.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >feldspathic_detrital_mode</skos:notation>
        <skos:historyNote
            >Dott1964; Folk1968</skos:historyNote>
        <skos:definition
            >General sandstone petrography term for a sandstone in which the ratio of feldspar to lithic fragments is greater than 1:1 and less than 75 percent quartz is present in the detrital mode. Subsumes lithic arkosic and arkosic sandstone petrographic composition classes. Usage of modifier feldspathic is consistent with petrographic subdivisions of Dott (1964) and Williams, Turner and Gilbert (1982)</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Feldspathic detrital mode</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#feldspathic_lithic_detrital_mode -->

    <skos:Concept rdf:about="&CompositionCategory200811;feldspathic_lithic_detrital_mode">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.08.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >feldspathic_lithic_detrital_mode</skos:notation>
        <skos:definition
            >Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the feldspar to lithic ratio is between 1 to 1 and 1 to 3. This corresponds to the feldspathic litharenite field of Folk (1968). Rocks in this class are likely to be quartzo-feldspathic in terms of mineralogical composition terms in this vocablary, but because the mineralogical composition of the lithic fraction is not constrainged, this correlation is not necessary.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Feldspathic lithic detrital mode</skos:prefLabel>
        <skos:historyNote>Folk1968</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#ferromagnesian -->

    <owl:Thing rdf:about="&CompositionCategory200811;ferromagnesian">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.05.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">ferromagnesian</skos:notation>
        <skos:prefLabel xml:lang="en">Ferromagnesian</skos:prefLabel>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:definition
            >Material consists of greater than 40 percent dark ferromagnesian silicate minerals. Standard term defined by Bates and Jackson (1987) to mean &#39;containing iron and magnesium&#39;. Subsumes mafic and ultramafic.</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#ferruginous -->

    <skos:Concept rdf:about="&CompositionCategory200811;ferruginous">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.08.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">ferruginous</skos:notation>
        <skos:prefLabel xml:lang="en">Ferruginous</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Greater than accessory amount (greater than 5 percent by volume) of non-silicate iron minerals (hematite, magnetite, siderite...) are present. Rocks containing abundant iron-rich silicate minerals would use the &#39;Ferromagnesian&#39; terms.</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#fluoride_chemistry -->

    <skos:Concept rdf:about="&CompositionCategory200811;fluoride_chemistry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.07.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >fluoride_chemistry</skos:notation>
        <skos:definition
            >Fluorine is the most abundant (mole percent) anion</skos:definition>
        <skos:historyNote
            >Struik etal 2002; this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Fluoride chemistry</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;halide_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#halide_chemistry -->

    <owl:Thing rdf:about="&CompositionCategory200811;halide_chemistry">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.07.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >halide_chemistry</skos:notation>
        <skos:historyNote
            >Struik etal 2002; this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Halide chemistry</skos:prefLabel>
        <skos:definition
            >One or more of halide group elements (F, Cl, Br, I) are most abundant (mole percent) anion</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#high_silica_chemistry -->

    <skos:Concept rdf:about="&CompositionCategory200811;high_silica_chemistry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.10.0</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >high_silica_chemistry</skos:notation>
        <skos:historyNote
            >Struik2002; SLTTs2004; this vocabulary</skos:historyNote>
        <skos:definition
            >Containing 65 percent or more SiO2 by weight. Syn acid.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >High silica chemistry</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;silicate_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#impure_calcsilicate_or_carbonate -->

    <owl:Thing rdf:about="&CompositionCategory200811;impure_calcsilicate_or_carbonate">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.03.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >impure_calcsilicate_or_carbonate</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Impure calcsilicate or carbonate</skos:prefLabel>
        <skos:definition
            >Material consists of greater than 50 percent calcsilicate or carbonate minerals and relative proportion of calcsilicate and carbonate minerals is unknown or not specified.</skos:definition>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#intermediate_silica_chemistry -->

    <skos:Concept rdf:about="&CompositionCategory200811;intermediate_silica_chemistry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.10.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >intermediate_silica_chemistry</skos:notation>
        <skos:definition
            >Containing SiO2 in the range 52-65 percent, sometimes described as &quot;intermediate&quot; rocks.</skos:definition>
        <skos:historyNote
            >Struik2002; SLTTs2004; this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Intermediate silica chemistry</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;silicate_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#intraclastic -->

    <owl:Thing rdf:about="&CompositionCategory200811;intraclastic">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.02.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">intraclastic</skos:notation>
        <skos:definition
            >Carbonate sedimentary material contains signficant intraclast content</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">Intraclastic</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#iron_bearing_carbonate_chemistry -->

    <skos:Concept rdf:about="&CompositionCategory200811;iron_bearing_carbonate_chemistry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >iron_bearing_carbonate_chemistry</skos:notation>
        <skos:definition
            >CO3 is the predominant anion, and iron is greater than 5 mole percent of the cations (includes Fe, Mg, Mn, Ba, Sr, Ca...).</skos:definition>
        <skos:historyNote>SMR definition</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Iron-bearing carbonate chemistry</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;carbonate_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#iron_rich_carbonate_chemistry -->

    <owl:Thing rdf:about="&CompositionCategory200811;iron_rich_carbonate_chemistry">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.2.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >iron_rich_carbonate_chemistry</skos:notation>
        <skos:historyNote>SMR definition</skos:historyNote>
        <skos:definition
            >CO3 is the predominant anion, and iron is the most abundant (mole percent) cation</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Iron-rich carbonate chemistry</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;iron_bearing_carbonate_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#lithic -->

    <skos:Concept rdf:about="&CompositionCategory200811;lithic">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.06.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">lithic</skos:notation>
        <skos:definition
            >Sedimentary material contains significant detrital rock fragments derived by erosion from older, pre-existing rock materials</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">Lithic</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#lithic_arkosic_detrital_mode -->

    <skos:Concept rdf:about="&CompositionCategory200811;lithic_arkosic_detrital_mode">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.07.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >lithic_arkosic_detrital_mode</skos:notation>
        <skos:historyNote>Folk1968</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Lithic arkosic detrital mode</skos:prefLabel>
        <skos:definition
            >Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the ratio of lithic fragments to feldspar is between 1:1 and 1:3. This corresponds to the lithic arkose field of Folk (1968); the 75 percent quartz boundary is chosen to be consistent with quartzsose composition as defined in the mineralogic compostion terms vocabulary. Because of the abundance of lithic fragments for which the mineralogic composition is not specified, and the presence of undiscernible matrix and cement that are not incuded in the detrital mode, corresondence with the mineralogical composition terms is very approximate.</skos:definition>
        <skos:broader rdf:resource="&CompositionCategory200811;feldspathic_detrital_mode"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#lithic_detrital_mode -->

    <owl:Thing rdf:about="&CompositionCategory200811;lithic_detrital_mode">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.08.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >lithic_detrital_mode</skos:notation>
        <skos:historyNote>Folk1968</skos:historyNote>
        <skos:definition
            >Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 75 percent quartz and the lithic to feldspar ratio is greater than 3 to 1. This corresponds to the litharenite field of Folk (1968). Because the mineralogy of the lithic fragments is not constrained, the mineralogical composition is largely independent of the detrital mode composition for this class.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Lithic detrital mode</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#lithic_rich_detrital_mode -->

    <owl:Thing rdf:about="&CompositionCategory200811;lithic_rich_detrital_mode">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.08.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >lithic_rich_detrital_mode</skos:notation>
        <skos:historyNote
            >Dott1964; Folk1968</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Lithic rich detrital mode</skos:prefLabel>
        <skos:definition
            >Sandstone petrographic composition term indicating that the ratio of lithic fragments to feldspar is greater than 1:1, and less than 75 percent quartz is present in the detrital mode. Because the mineralogy of the lithic fragments is not constrained, the mineralogical composition is largely independent of the detrital mode composition for this class.</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#low_silica_chemistry -->

    <owl:Thing rdf:about="&CompositionCategory200811;low_silica_chemistry">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.10.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >low_silica_chemistry</skos:notation>
        <skos:definition
            >Containing SiO2 in the range 44-52 percent. Syn basic.</skos:definition>
        <skos:historyNote
            >Struik2002; SLTTs2004; this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Low silica chemistry</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;silicate_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#mafic -->

    <owl:Thing rdf:about="&CompositionCategory200811;mafic">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.05.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">mafic</skos:notation>
        <skos:definition
            >Material consists of greater than or equal to 40 percent and less than 90 percent ferromagnesian silicate minerals.</skos:definition>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en">Mafic</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;ferromagnesian"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#magnesium_rich_carbonate_chemistry -->

    <skos:Concept rdf:about="&CompositionCategory200811;magnesium_rich_carbonate_chemistry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >magnesium_rich_carbonate_chemistry</skos:notation>
        <skos:historyNote
            >Struik etal 2002; this vocabulary</skos:historyNote>
        <skos:definition
            >CO3 is the predominant anion, and Mg is the most abundant (mole percent) cation.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Magnesium-rich carbonate chemistry</skos:prefLabel>
        <skos:broader rdf:resource="&CompositionCategory200811;carbonate_chemistry"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#metallic_oxide_bearing -->

    <skos:Concept rdf:about="&CompositionCategory200811;metallic_oxide_bearing">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.10.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >metallic_oxide_bearing</skos:notation>
        <skos:historyNote
            >Struik2002; SLTTs2004; this vocabulary</skos:historyNote>
        <skos:definition
            >Greater than accessory amounts (greater than 5 percent by volume) of base metal oxide minerals such as magnetite (Fe3O4) or pyrolusite (MnO2) are present.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Metallic oxide bearing</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#metaluminous -->

    <skos:Concept rdf:about="&CompositionCategory200811;metaluminous">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.01</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">metaluminous</skos:notation>
        <skos:definition
            >Chemical composition term used for igneous rocks, denoting that content of aluminum oxide is greater than sodium oxide plus potassium oxide, but aluminum oxide less than sum of Na, K and Ca oxide.</skos:definition>
        <skos:prefLabel xml:lang="en">Metaluminous</skos:prefLabel>
        <skos:historyNote>Shand 1947</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#micaceous -->

    <owl:Thing rdf:about="&CompositionCategory200811;micaceous">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.09.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">micaceous</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Greater than accessory amount (greater than 5 percent by volume) of mica group mineral are present</skos:definition>
        <skos:prefLabel xml:lang="en">Micaceous</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#mono_mineralic -->

    <owl:Thing rdf:about="&CompositionCategory200811;mono_mineralic">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.11.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">mono_mineralic</skos:notation>
        <skos:definition
            >Greater than 75 percent of the material consists of a single mineral species.</skos:definition>
        <skos:prefLabel xml:lang="en">Mono mineralic</skos:prefLabel>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#monomictic -->

    <owl:Thing rdf:about="&CompositionCategory200811;monomictic">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.03.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811">monomictic</skos:notation>
        <skos:historyNote>Jackson 1997</skos:historyNote>
        <skos:definition
            >Grains or clasts are composed of a single rock or mineral type (Jackson, 1997)</skos:definition>
        <skos:prefLabel xml:lang="en">Monomictic</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#monomineralic_carbonate -->

    <owl:Thing rdf:about="&CompositionCategory200811;monomineralic_carbonate">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.03.2.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >monomineralic_carbonate</skos:notation>
        <skos:historyNote>NADM SLTTm 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Monomineralic carbonate</skos:prefLabel>
        <skos:definition
            >Rock in which carbonate minerals form greater than 75 percent of rock.</skos:definition>
        <skos:broader rdf:resource="&CompositionCategory200811;carbonate"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#nitrate_chemistry -->

    <skos:Concept rdf:about="&CompositionCategory200811;nitrate_chemistry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.07.3</skos:notation>
