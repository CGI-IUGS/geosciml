    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#quartzose_detrital_mode -->

    <owl:Thing rdf:about="&CompositionCategory200811;quartzose_detrital_mode">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.11.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >quartzose_detrital_mode</skos:notation>
        <skos:historyNote
            >Folk1968; this vocabulary</skos:historyNote>
        <skos:definition
            >Detrital mineral grains in a granular rock consist of greater than 95 percent quartz. This petrographic criteria follows the most restrictive definition of the composition field at the quartz vertex of the sandstone QFL triangle (Folk, 1968). This use of term quartzose is more restrictive that that used in the mineralogical composition term vocabulary, reflecting the interpretive significance of pure quartz sandstones in sedimentary petrology.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Quartzose detrital mode</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


