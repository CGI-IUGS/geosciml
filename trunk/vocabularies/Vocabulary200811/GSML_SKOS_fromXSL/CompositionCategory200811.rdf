        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#sub_feldspathic_detrital_mode -->

    <skos:Concept rdf:about="&CompositionCategory200811;sub_feldspathic_detrital_mode">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.09.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompositionCategory:200811"
            >sub_feldspathic_detrital_mode</skos:notation>
        <skos:historyNote>Folk1968</skos:historyNote>
        <skos:definition
            >Sandstone petrographic composition term indicating that detrital mode for clastic rock (Q-F-L) includes less than 50 percent lithic fragments, between 75 and 95 percent quartz (thus less than 25 percent feldspar). This corresponds to the subarkose field of Folk (1968); the 75 percent quartz boundary is consistent with the quartzsose composition as defined in the mineralogic compostion terms vocabulary, but if more than a few percent &#39;matrix&#39; or cement is present, this is not necessarily equivalent to the mineralogical composition term.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Sub feldspathic detrital mode</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompositionCategory:200811#sub_lithic_detrital_mode -->

