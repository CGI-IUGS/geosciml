    <!-- urn:cgi:classifier:CGI:ConventionCode:200811#dip_dip_direction -->

    <skos:Concept rdf:about="&ConventionCode200811;dip_dip_direction">
        <rdf:type rdf:resource="&owl;Thing"/>
