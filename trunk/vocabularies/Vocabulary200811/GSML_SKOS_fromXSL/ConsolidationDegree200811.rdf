    <skos:editorialNote>This file contains the 200811 version of CGI Consolidation degree vocabulary. Use for specifying degree of consolidation of Compound material, distinguishes rock from unconsolidated material.</skos:editorialNote>
  </skos:ConceptScheme>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:unconsolidated">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Particulate constituents of a compound material do not adhere to each other strongly enough that the aggregate can be considered a solid in its own right.</skos:definition>
    <skos:prefLabel xml:lang="en">Unconsolidated</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">unconsolidated</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">1</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:unconsolidated_very_loose">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Easily indented with fingers; Relative density 0.0-0.2.</skos:definition>
    <skos:prefLabel xml:lang="en">Unconsolidated, very loose</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">unconsolidated_very_loose</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:ConsolidationDegree:200811:unconsolidated" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.1</skos:notation>
    <skos:historyNote>NADM SLTTs 2004 after Bowles 1984</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:unconsolidated_loose">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Easily shoveled, can be indented with fingers; Relative density 0.2-0.4.</skos:definition>
    <skos:prefLabel xml:lang="en">Unconsolidated, loose</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">unconsolidated_loose</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:ConsolidationDegree:200811:unconsolidated" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.2</skos:notation>
    <skos:historyNote>NADM SLTTs 2004 after Bowles 1985</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:incipient_consolidation">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Shoveled with difficulty; relative density 0.4 - 0.7.</skos:definition>
    <skos:prefLabel xml:lang="en">Incipient consolidation</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">incipient_consolidation</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:ConsolidationDegree:200811:unconsolidated" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.3</skos:notation>
    <skos:historyNote>NADM SLTTs 2004 after Bowles 1986</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:consolidated">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Particulate constituents of a compound material adhere to each other strongly enough that the aggregate can be considered a solid material in its own right.</skos:definition>
    <skos:prefLabel xml:lang="en">Consolidated</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">consolidated</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">2</skos:notation>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:well_consolidated">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Requires pick to loosen for shoveling; relative density 0.7-0.9.</skos:definition>
    <skos:prefLabel xml:lang="en">Well consolidated</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">well_consolidated</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:ConsolidationDegree:200811:consolidated" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.1</skos:notation>
    <skos:historyNote>NADM SLTTs 2004 after Bowles 1986</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:indurated">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Requires blasting or heavy equipment to loosen; Relative density 0.9-1.0. Rings to blow of hammer.</skos:definition>
    <skos:prefLabel xml:lang="en">Indurated</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">indurated</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:ConsolidationDegree:200811:consolidated" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.2</skos:notation>
    <skos:historyNote>NADM SLTTs 2004 after Bowles 1987</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:slightly_indurated">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Rock can be broken with single blow from standard rock hammer (less than 1 kg mass).</skos:definition>
    <skos:prefLabel xml:lang="en">Slightly indurated</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">slightly_indurated</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:ConsolidationDegree:200811:indurated" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.2.1</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:moderately_indurated">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Multiple blows with standard rock hammer (less than 1 kg) are required to break rock.</skos:definition>
    <skos:prefLabel xml:lang="en">Moderately indurated</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">moderately_indurated</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:ConsolidationDegree:200811:indurated" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.2.2</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:well_indurated">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Particles in the rock are strongly bound together such that rock surface can only be broken with great difficulty using standard rock hammer (less than 1 kg mass).</skos:definition>
    <skos:prefLabel xml:lang="en">Well indurated</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">well_indurated</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:ConsolidationDegree:200811:indurated" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.2.3</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:variable_induration">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Material is lithified, but induration varies at scale of description.</skos:definition>
    <skos:prefLabel xml:lang="en">Variable induration</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">variable_induration</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:ConsolidationDegree:200811:indurated" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.2.4</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:consolidation_variable">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>Consolidation ranges from unconsolidated to indurated on scale of description</skos:definition>
    <skos:prefLabel xml:lang="en">Consolidation variable</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">consolidation_variable</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">3</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ConsolidationDegree:200811:consolidation_not_specified">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811" />
    <skos:definition>In normative descriptions, indicates that consolidation state is not a determining factor in identification, it may have any value.</skos:definition>
    <skos:prefLabel xml:lang="en">Consolidation not specified</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ConsolidationDegree:200811">consolidation_not_specified</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">4</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
</rdf:RDF>