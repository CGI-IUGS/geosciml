    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#armoured_relict_crystal -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;armoured_relict_crystal">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.04.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >armoured_relict_crystal</skos:notation>
        <skos:historyNote
            >base on Neuendorf et al 2005</skos:historyNote>
        <skos:definition
            >a crystal of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Armoured relict crystal</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;particulate_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#armoured_relict_inclusion -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;armoured_relict_inclusion">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.10.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >armoured_relict_inclusion</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Armoured relict inclusion</skos:prefLabel>
        <skos:definition
            >an inclusion of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products</skos:definition>
        <skos:historyNote
            >base on Neuendorf et al 2005</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;inclusion"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#chadacryst -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;chadacryst">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.06.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">chadacryst</skos:notation>
        <skos:definition
            >the enclosed crystal in a poikolitic texture</skos:definition>
        <skos:prefLabel xml:lang="en">Chadacryst</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;particulate_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#clast_supporting_orthomatrix -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;clast_supporting_orthomatrix">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.02.4.1</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >clast_supporting_orthomatrix</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Clast supporting orthomatrix</skos:prefLabel>
        <skos:definition
            >Orthomatrix in matrix supported sedimentary rock.</skos:definition>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;orthomatrix"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#clot -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;clot">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.06.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">clot</skos:notation>
        <skos:definition
            >A cluster of ferromagnesian minerals in an igneous rock, from several centimeters to decimeters in diameter, that may be a segregation or an altered xenolith.</skos:definition>
        <skos:prefLabel xml:lang="en">Clot</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al, 2005</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#constituent_role_not_specified -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;constituent_role_not_specified">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">4..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >constituent_role_not_specified</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Constituent role not specified</skos:prefLabel>
        <skos:definition
            >For descriptions in which role of constituent in compound material is not specified.</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#corona -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;corona">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.02..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">corona</skos:notation>
        <skos:definition
            >Constituent occurs as a concentric envelope enclosing another constituent. Corona is a non-genetic term.</skos:definition>
        <skos:historyNote
            >Williams, Turner and Gilbert, 1954; Spry, 1969; Fettes and Desmons, 2007</skos:historyNote>
        <skos:prefLabel xml:lang="en">Corona</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#corrosion_rim -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;corrosion_rim">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.02.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">corrosion_rim</skos:notation>
        <skos:prefLabel xml:lang="en">Corrosion rim</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >A corona formed by a secondary mineral around an orginal igneous crystal, formed by modification of the crystal by the corrosive action of its parent magma</skos:definition>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;corona"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#crystalline_framework_constituent -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;crystalline_framework_constituent">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.02.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >crystalline_framework_constituent</skos:notation>
        <skos:definition
            >Constituent forms an interconnected network of discernible crystals.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Crystalline framework constituent</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;particulate_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#dropstone -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;dropstone">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.08.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">dropstone</skos:notation>
        <skos:prefLabel xml:lang="en">Dropstone</skos:prefLabel>
        <skos:historyNote
            >NADM SLTTs 2004; Jackson, 1997, p 193</skos:historyNote>
        <skos:definition
            >&quot;An oversized stone in laminated sediment that depresses the underlying laminae and may be covered by -draped laminae. Most dropstones originate through ice-rafting; other sources are floating tree roots and kelp holdfasts....&quot;</skos:definition>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#epimatrix -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;epimatrix">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.02.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">epimatrix</skos:notation>
        <skos:definition
            >A type of matrix consisting of inhomogeneous interstitial materials grown in originally open interstices during diagenesis, but lacking the homogeneity and clear textural evidence of pore-filling needed to classify as phyllosilicate cement.</skos:definition>
        <skos:prefLabel xml:lang="en">Epimatrix</skos:prefLabel>
        <skos:historyNote
            >NADM SLTTs 2004; Dickinson, 1970</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;matrix"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#floating_clast -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;floating_clast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.07.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">floating_clast</skos:notation>
        <skos:historyNote
            >Pettijohn et al., 1972, p. 90; SMR</skos:historyNote>
        <skos:definition
            >Constituent is a clast that has no visible contacts with other clasts. Interpreted to be largely or completly immersed in matrix or cement.</skos:definition>
        <skos:prefLabel xml:lang="en">Floating clast</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#granular_framework_constituent -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;granular_framework_constituent">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.03.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >granular_framework_constituent</skos:notation>
        <skos:definition
            >&quot;Constituent forms a rigid arrangement of particles that support one another at their points of contact...constituting a mechanically firm structure capable of supporting open pore spaces, although interstices may be occupied by cement or matrix&quot;</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Granular framework constituent</skos:prefLabel>
        <skos:historyNote
            >Jackson, 1997, p. 250</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;particulate_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#groundmass_constituent -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;groundmass_constituent">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.01..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >groundmass_constituent</skos:notation>
        <skos:definition
            >Constituent forms an interconnected network of material enclosing other constituents that are disguished by larger grain size.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Groundmass constituent</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;interstitial_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#inclusion -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;inclusion">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.10.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">inclusion</skos:notation>
        <skos:definition
            >a fragment of older material within an igneous rock to which it may or may not be genetically related</skos:definition>
        <skos:prefLabel xml:lang="en">Inclusion</skos:prefLabel>
        <skos:historyNote
            >Neuendorf et al, 2005</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#interstitial_constituent -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;interstitial_constituent">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >interstitial_constituent</skos:notation>
        <skos:definition
            >Constituent is distributed through the material between other constituent particles</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Interstitial constituent</skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#irregular_part -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;irregular_part">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.05.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">irregular_part</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >A constituent with irregular distribution and geometry, as in pseudobreccia or patch migmatite.</skos:definition>
        <skos:prefLabel xml:lang="en">Irregular part</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#kelyphitic_rim -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;kelyphitic_rim">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.02.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">kelyphitic_rim</skos:notation>
        <skos:definition
            >Corona that consists of concentric bands with radial fibrous texture.</skos:definition>
        <skos:historyNote
            >Williams, Turner and Gilbert, 1954; Fettes and Desmons, 2007</skos:historyNote>
        <skos:prefLabel xml:lang="en">Kelyphitic rim</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;corona"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#layer -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;layer">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.03.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">layer</skos:notation>
        <skos:definition
            >A thin sheet compositionally distinct from the surrounding material, related to primary genesis of rock, e.g. sedimentary layers, metamorphic segregation</skos:definition>
        <skos:prefLabel xml:lang="en">Layer</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#matrix -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;matrix">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.02..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">matrix</skos:notation>
        <skos:historyNote
            >NADM SLTTs 2004; Jackson, 1997</skos:historyNote>
        <skos:definition
            >Constituent forms finer-grained material interstitial to a framework constituent. &quot;The finer-grained material enclosing, or filling the interstices between, the larger grains or particles of a sediment or sedimentary rock....The term refers to the relative size and disposition of the particles, and no particular particle size is implied&quot; (Jackson, 1997, p. 393). May be classifiable into orthomatrix, protomatrix, epimatrix, pseudomatrix, and unclassified matrix.</skos:definition>
        <skos:prefLabel xml:lang="en">Matrix</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;interstitial_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#megacryst -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;megacryst">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.01.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">megacryst</skos:notation>
        <skos:historyNote
            >Neuendorf et al 2005</skos:historyNote>
        <skos:definition
            >any crystal in an igneous or metamorphic rock that is sgnificantly larger than the surounding groundmass. May be a phenocryst, xenocryst, porphyroblast or porphyroclast</skos:definition>
        <skos:prefLabel xml:lang="en">Megacryst</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;particulate_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#oikocryst -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;oikocryst">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.05.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">oikocryst</skos:notation>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:definition
            >Constituent occurs as crystals that poikilitically enclose crystals of other phases in an igneous rock.</skos:definition>
        <skos:prefLabel xml:lang="en">Oikocryst</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;particulate_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#orthomatrix -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;orthomatrix">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.02.4.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">orthomatrix</skos:notation>
        <skos:definition
            >A type of matrix consisting of recrystallized detrital lutum (clay fraction, less than 2 micron) or protomatrix.</skos:definition>
        <skos:historyNote
            >NADM SLTTs 2004; Dickinson, 1970</skos:historyNote>
        <skos:prefLabel xml:lang="en">Orthomatrix</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;matrix"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#overgrowth -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;overgrowth">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.01..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">overgrowth</skos:notation>
        <skos:definition
            >Constituent crystallized in crystallographic continuity with some other mineral constituent, typically quartz or calcite. In a clastic rock, commonly forms cement as well, but this should be represented using two role attribute links, &#39;overgrowth&#39; and cement, because overgrowth does not necessarily imply cement.</skos:definition>
        <skos:historyNote
            >NADM SLTTs 2004; Neuendorf et al. 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">Overgrowth</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#particulate_constituent -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;particulate_constituent">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >particulate_constituent</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Particulate constituent</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >constituent occurs as a collection of particles that are characterized by average properties of the individual particles</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#phenoclast -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;phenoclast">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.07.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">phenoclast</skos:notation>
        <skos:definition
            >a relatively large and conspicuous fragment in a sediment or sedimentary rock</skos:definition>
        <skos:historyNote
            >Neuendorf et al., 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">Phenoclast</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;particulate_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#phenocryst -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;phenocryst">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.01.2</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">phenocryst</skos:notation>
        <skos:definition
            >Constituent occurs as crystals that are distinctly larger than a groundmass of enclosing crystals. Used only in igneous rocks.</skos:definition>
        <skos:historyNote
            >Neuendorf et al. 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">Phenocryst</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;megacryst"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#porphyroblast -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;porphyroblast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.01.1.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">porphyroblast</skos:notation>
        <skos:definition
            >Constituent occurs as crystals formed by metamorphic crystallization, set in a finer-grained groundmass.</skos:definition>
        <skos:historyNote
            >Fettes and Desmons, 2007</skos:historyNote>
        <skos:prefLabel xml:lang="en">Porphyroblast</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;megacryst"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#porphyroclast -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;porphyroclast">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">1.01.3</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">porphyroclast</skos:notation>
        <skos:prefLabel xml:lang="en">Porphyroclast</skos:prefLabel>
        <skos:historyNote>Spry, 1969</skos:historyNote>
        <skos:definition
            >Relict crystal in metamorphic rock, in groundmass of relatively finer-grained material. Connotes that groundmass is result of tectonic reduction in grain size.</skos:definition>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;megacryst"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#protomatrix -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;protomatrix">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.02.3.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">protomatrix</skos:notation>
        <skos:definition
            >A type of matrix consisting of un-recrystallized detrital clayey lutum (clay fraction, less than 2 micron) in weakly consolidated rocks.</skos:definition>
        <skos:historyNote
            >NADM SLTTs 2004; Dickinson, 1970</skos:historyNote>
        <skos:prefLabel xml:lang="en">Protomatrix</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;matrix"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#pseudomatrix -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;pseudomatrix">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.02.2.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">pseudomatrix</skos:notation>
        <skos:definition
            >A type of matrix consisting of soft deformable framework grains that are squeezed and flattened between stronger framework grains.</skos:definition>
        <skos:historyNote
            >NADM SLTTs 2004; Dickinson, 1970</skos:historyNote>
        <skos:prefLabel xml:lang="en">Pseudomatrix</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;matrix"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#sedimentary_rock_cement -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;sedimentary_rock_cement">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">2.03..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >sedimentary_rock_cement</skos:notation>
        <skos:definition
            >A constituent that occupies space between individual grains of a consolidated sedimentary rock, and binds the grains together as a rigid, coherent mass; it may be derived from the sediment or its entrapped waters, or it may be brought in by solution from outside sources. Material is usually chemically precipitated (Jackson, 1997, p. 103). Distinguished from matrix by clearly secondary origin and generally monomineralic character.</skos:definition>
        <skos:historyNote
            >NADM SLTTs 2004; Jackson, 1997</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Sedimentary rock cement</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;interstitial_constituent"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#small_concretion -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;small_concretion">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.09.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811"
            >small_concretion</skos:notation>
        <skos:definition
            >A hard, compact mass or aggregate of mineral matter, normally subsperical but commonly oblate, disc-shaped or irregular. Formed by precipitation of mineral from solution in the pores of a granular rock, localized around a nucleus or center, to define a discrete, sharply separated object. Size ranges from cm to decimeter for application as a compoundMaterialConstituentPart; larger concretions should be considered GeologicUnit parts.</skos:definition>
        <skos:historyNote
            >Neuendorf et al, 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Small concretion</skos:prefLabel>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#structural_part -->

    <skos:Concept rdf:about="&CompoundMaterialConstituentPartRole200811;structural_part">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3..</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">structural_part</skos:notation>
        <skos:definition
            >Constituent occurs in a structural configuration integral to the rock, such as layering, veinlets, overgrowths. The &#39;material&#39; composition of these parts will often be other rock materials, not minerals, and &#39;ParticleGeometryDescription&#39; associated with these describes the geometry of the constituent, not the particles the it is made of. These roles are mostly useful for RockMaterial descriptions that apply to individual samples, because their distribution is unlikely to be pervasive enought to be considered characteristic of a large mass of material.</skos:definition>
        <skos:prefLabel xml:lang="en">Structural part</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811#vein -->

    <owl:Thing rdf:about="&CompoundMaterialConstituentPartRole200811;vein">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="LocalHierarchyKey">3.04.</skos:notation>
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">vein</skos:notation>
        <skos:definition
            >Thin sheet of material intruded into the rock. May be hydrothermal, magmatic, or sedimentary.</skos:definition>
        <skos:prefLabel xml:lang="en">Vein</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&CompoundMaterialConstituentPartRole200811;structural_part"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


