  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:particulate_constituent">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>constituent occurs as a collection of particles that are characterized by average properties of the individual particles</skos:definition>
    <skos:prefLabel xml:lang="en">Particulate constituent</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">particulate_constituent</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">1..</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:megacryst">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>any crystal in an igneous or metamorphic rock that is sgnificantly larger than the surounding groundmass. May be a phenocryst, xenocryst, porphyroblast or porphyroclast</skos:definition>
    <skos:prefLabel xml:lang="en">Megacryst</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">megacryst</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:particulate_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.01.</skos:notation>
    <skos:historyNote>Neuendorf et al 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:porphyroblast">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent occurs as crystals formed by metamorphic crystallization, set in a finer-grained groundmass.</skos:definition>
    <skos:prefLabel xml:lang="en">Porphyroblast</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">porphyroblast</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:megacryst" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.01.1.</skos:notation>
    <skos:historyNote>Fettes and Desmons, 2007</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:phenocryst">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent occurs as crystals that are distinctly larger than a groundmass of enclosing crystals. Used only in igneous rocks.</skos:definition>
    <skos:prefLabel xml:lang="en">Phenocryst</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">phenocryst</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:megacryst" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.01.2</skos:notation>
    <skos:historyNote>Neuendorf et al. 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:porphyroclast">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Relict crystal in metamorphic rock, in groundmass of relatively finer-grained material. Connotes that groundmass is result of tectonic reduction in grain size.</skos:definition>
    <skos:prefLabel xml:lang="en">Porphyroclast</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">porphyroclast</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:megacryst" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.01.3</skos:notation>
    <skos:historyNote>Spry, 1969</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:crystalline_framework_constituent">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent forms an interconnected network of discernible crystals.</skos:definition>
    <skos:prefLabel xml:lang="en">Crystalline framework constituent</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">crystalline_framework_constituent</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:particulate_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.02.</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:granular_framework_constituent">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>"Constituent forms a rigid arrangement of particles that support one another at their points of contact...constituting a mechanically firm structure capable of supporting open pore spaces, although interstices may be occupied by cement or matrix"</skos:definition>
    <skos:prefLabel xml:lang="en">Granular framework constituent</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">granular_framework_constituent</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:particulate_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.03.</skos:notation>
    <skos:historyNote>Jackson, 1997, p. 250</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:armoured_relict_crystal">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>a crystal of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products</skos:definition>
    <skos:prefLabel xml:lang="en">Armoured relict crystal</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">armoured_relict_crystal</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:particulate_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.04.</skos:notation>
    <skos:historyNote>base on Neuendorf et al 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:oikocryst">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent occurs as crystals that poikilitically enclose crystals of other phases in an igneous rock.</skos:definition>
    <skos:prefLabel xml:lang="en">Oikocryst</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">oikocryst</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:particulate_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.05.</skos:notation>
    <skos:historyNote>Neuendorf et al., 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:chadacryst">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>the enclosed crystal in a poikolitic texture</skos:definition>
    <skos:prefLabel xml:lang="en">Chadacryst</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">chadacryst</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:particulate_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.06.</skos:notation>
    <skos:historyNote>Neuendorf et al., 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:phenoclast">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>a relatively large and conspicuous fragment in a sediment or sedimentary rock</skos:definition>
    <skos:prefLabel xml:lang="en">Phenoclast</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">phenoclast</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:particulate_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.07.</skos:notation>
    <skos:historyNote>Neuendorf et al., 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:interstitial_constituent">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent is distributed through the material between other constituent particles</skos:definition>
    <skos:prefLabel xml:lang="en">Interstitial constituent</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">interstitial_constituent</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">2..</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:groundmass_constituent">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent forms an interconnected network of material enclosing other constituents that are disguished by larger grain size.</skos:definition>
    <skos:prefLabel xml:lang="en">Groundmass constituent</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">groundmass_constituent</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:interstitial_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.01..</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:matrix">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent forms finer-grained material interstitial to a framework constituent. "The finer-grained material enclosing, or filling the interstices between, the larger grains or particles of a sediment or sedimentary rock....The term refers to the relative size and disposition of the particles, and no particular particle size is implied" (Jackson, 1997, p. 393). May be classifiable into orthomatrix, protomatrix, epimatrix, pseudomatrix, and unclassified matrix.</skos:definition>
    <skos:prefLabel xml:lang="en">Matrix</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">matrix</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:interstitial_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.02..</skos:notation>
    <skos:historyNote>NADM SLTTs 2004; Jackson, 1997</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:sedimentary_rock_cement">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A constituent that occupies space between individual grains of a consolidated sedimentary rock, and binds the grains together as a rigid, coherent mass; it may be derived from the sediment or its entrapped waters, or it may be brought in by solution from outside sources. Material is usually chemically precipitated (Jackson, 1997, p. 103). Distinguished from matrix by clearly secondary origin and generally monomineralic character.</skos:definition>
    <skos:prefLabel xml:lang="en">Sedimentary rock cement</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">sedimentary_rock_cement</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:interstitial_constituent" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.03..</skos:notation>
    <skos:historyNote>NADM SLTTs 2004; Jackson, 1997</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:epimatrix">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A type of matrix consisting of inhomogeneous interstitial materials grown in originally open interstices during diagenesis, but lacking the homogeneity and clear textural evidence of pore-filling needed to classify as phyllosilicate cement.</skos:definition>
    <skos:prefLabel xml:lang="en">Epimatrix</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">epimatrix</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:matrix" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.02.1.</skos:notation>
    <skos:historyNote>NADM SLTTs 2004; Dickinson, 1970</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:pseudomatrix">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A type of matrix consisting of soft deformable framework grains that are squeezed and flattened between stronger framework grains.</skos:definition>
    <skos:prefLabel xml:lang="en">Pseudomatrix</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">pseudomatrix</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:matrix" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.02.2.</skos:notation>
    <skos:historyNote>NADM SLTTs 2004; Dickinson, 1970</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:protomatrix">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A type of matrix consisting of un-recrystallized detrital clayey lutum (clay fraction, less than 2 micron) in weakly consolidated rocks.</skos:definition>
    <skos:prefLabel xml:lang="en">Protomatrix</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">protomatrix</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:matrix" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.02.3.</skos:notation>
    <skos:historyNote>NADM SLTTs 2004; Dickinson, 1970</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:orthomatrix">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A type of matrix consisting of recrystallized detrital lutum (clay fraction, less than 2 micron) or protomatrix.</skos:definition>
    <skos:prefLabel xml:lang="en">Orthomatrix</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">orthomatrix</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:matrix" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.02.4.</skos:notation>
    <skos:historyNote>NADM SLTTs 2004; Dickinson, 1970</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:clast_supporting_orthomatrix">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Orthomatrix in matrix supported sedimentary rock.</skos:definition>
    <skos:prefLabel xml:lang="en">Clast supporting orthomatrix</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">clast_supporting_orthomatrix</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:orthomatrix" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.02.4.1</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent occurs in a structural configuration integral to the rock, such as layering, veinlets, overgrowths. The 'material' composition of these parts will often be other rock materials, not minerals, and 'ParticleGeometryDescription' associated with these describes the geometry of the constituent, not the particles the it is made of. These roles are mostly useful for RockMaterial descriptions that apply to individual samples, because their distribution is unlikely to be pervasive enought to be considered characteristic of a large mass of material.</skos:definition>
    <skos:prefLabel xml:lang="en">Structural part</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">structural_part</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">3..</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:overgrowth">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent crystallized in crystallographic continuity with some other mineral constituent, typically quartz or calcite. In a clastic rock, commonly forms cement as well, but this should be represented using two role attribute links, 'overgrowth' and cement, because overgrowth does not necessarily imply cement.</skos:definition>
    <skos:prefLabel xml:lang="en">Overgrowth</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">overgrowth</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.01..</skos:notation>
    <skos:historyNote>NADM SLTTs 2004; Neuendorf et al. 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:corona">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent occurs as a concentric envelope enclosing another constituent. Corona is a non-genetic term.</skos:definition>
    <skos:prefLabel xml:lang="en">Corona</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">corona</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.02..</skos:notation>
    <skos:historyNote>Williams, Turner and Gilbert, 1954; Spry, 1969; Fettes and Desmons, 2007</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:kelyphitic_rim">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Corona that consists of concentric bands with radial fibrous texture.</skos:definition>
    <skos:prefLabel xml:lang="en">Kelyphitic rim</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">kelyphitic_rim</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:corona" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.02.1.</skos:notation>
    <skos:historyNote>Williams, Turner and Gilbert, 1954; Fettes and Desmons, 2007</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:corrosion_rim">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A corona formed by a secondary mineral around an orginal igneous crystal, formed by modification of the crystal by the corrosive action of its parent magma</skos:definition>
    <skos:prefLabel xml:lang="en">Corrosion rim</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">corrosion_rim</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:corona" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.02.2.</skos:notation>
    <skos:historyNote>Neuendorf et al., 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:layer">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A thin sheet compositionally distinct from the surrounding material, related to primary genesis of rock, e.g. sedimentary layers, metamorphic segregation</skos:definition>
    <skos:prefLabel xml:lang="en">Layer</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">layer</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.03.</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:vein">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Thin sheet of material intruded into the rock. May be hydrothermal, magmatic, or sedimentary.</skos:definition>
    <skos:prefLabel xml:lang="en">Vein</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">vein</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.04.</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:irregular_part">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A constituent with irregular distribution and geometry, as in pseudobreccia or patch migmatite.</skos:definition>
    <skos:prefLabel xml:lang="en">Irregular part</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">irregular_part</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.05.</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:clot">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A cluster of ferromagnesian minerals in an igneous rock, from several centimeters to decimeters in diameter, that may be a segregation or an altered xenolith.</skos:definition>
    <skos:prefLabel xml:lang="en">Clot</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">clot</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.06.</skos:notation>
    <skos:historyNote>Neuendorf et al, 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:floating_clast">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>Constituent is a clast that has no visible contacts with other clasts. Interpreted to be largely or completly immersed in matrix or cement.</skos:definition>
    <skos:prefLabel xml:lang="en">Floating clast</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">floating_clast</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.07.</skos:notation>
    <skos:historyNote>Pettijohn et al., 1972, p. 90; SMR</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:dropstone">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>"An oversized stone in laminated sediment that depresses the underlying laminae and may be covered by -draped laminae. Most dropstones originate through ice-rafting; other sources are floating tree roots and kelp holdfasts...."</skos:definition>
    <skos:prefLabel xml:lang="en">Dropstone</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">dropstone</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.08.</skos:notation>
    <skos:historyNote>NADM SLTTs 2004; Jackson, 1997, p 193</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:small_concretion">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>A hard, compact mass or aggregate of mineral matter, normally subsperical but commonly oblate, disc-shaped or irregular. Formed by precipitation of mineral from solution in the pores of a granular rock, localized around a nucleus or center, to define a discrete, sharply separated object. Size ranges from cm to decimeter for application as a compoundMaterialConstituentPart; larger concretions should be considered GeologicUnit parts.</skos:definition>
    <skos:prefLabel xml:lang="en">Small concretion</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">small_concretion</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.09.</skos:notation>
    <skos:historyNote>Neuendorf et al, 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:inclusion">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>a fragment of older material within an igneous rock to which it may or may not be genetically related</skos:definition>
    <skos:prefLabel xml:lang="en">Inclusion</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">inclusion</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:structural_part" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.10.</skos:notation>
    <skos:historyNote>Neuendorf et al, 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:armoured_relict_inclusion">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>an inclusion of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products</skos:definition>
    <skos:prefLabel xml:lang="en">Armoured relict inclusion</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">armoured_relict_inclusion</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:inclusion" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.10.1</skos:notation>
    <skos:historyNote>base on Neuendorf et al 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:CompoundMaterialConstituentPartRole:200811:constituent_role_not_specified">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811" />
    <skos:definition>For descriptions in which role of constituent in compound material is not specified.</skos:definition>
    <skos:prefLabel xml:lang="en">Constituent role not specified</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:CompoundMaterialConstituentPartRole:200811">constituent_role_not_specified</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">4..</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
</rdf:RDF>