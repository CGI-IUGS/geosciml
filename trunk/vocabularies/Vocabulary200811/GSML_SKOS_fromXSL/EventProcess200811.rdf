    <skos:definition>Process of removal of oxygen from from the deep part of a body of water.</skos:definition>
    <skos:prefLabel xml:lang="en">deep water oxygen depletion</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">deep_water_oxygen_depletion</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">03..</skos:notation>
    <skos:historyNote>this vocabulary</skos:historyNote>
  </skos:Concept>
