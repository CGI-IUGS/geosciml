  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ValueQualifier:200811:quadratic_mean">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ValueQualifier:200811" />
    <skos:definition>Root means square, or the square root of the mean of the squares of a collection of measured values.</skos:definition>
    <skos:prefLabel xml:lang="en">quadratic mean</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ValueQualifier:200811">quadratic_mean</skos:notation>
