  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ValueQualifier:200811:harmonic_mean">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ValueQualifier:200811" />
    <skos:definition>The reciprocal of the arithmetic mean of the reciprocals of a collection of measured values.</skos:definition>
    <skos:prefLabel xml:lang="en">harmonic mean</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ValueQualifier:200811">harmonic_mean</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">5.2.</skos:notation>
    <skos:historyNote>GeoSciML UML model; http://en.wikipedia.org/wiki/Harmonic_mean</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ValueQualifier:200811:geometric_mean">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ValueQualifier:200811" />
    <skos:definition>The geometric mean of a collection of positive data is defined as the nth root of the product of all the members of the data set, where n is the number of members.</skos:definition>
    <skos:prefLabel xml:lang="en">geometric mean</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ValueQualifier:200811">geometric_mean</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">5.3.</skos:notation>
    <skos:historyNote>GeoSciML UML model; definition this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ValueQualifier:200811:arithmetic_mean">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ValueQualifier:200811" />
    <skos:definition>The average of a set of numbers. Found by adding all the numbers in a set and dividing the sum by the number of entries.</skos:definition>
    <skos:prefLabel xml:lang="en">arithmetic mean</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ValueQualifier:200811">arithmetic_mean</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">5.4.</skos:notation>
    <skos:historyNote>GeoSciML UML model; definition this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ValueQualifier:200811:mode">
