    <skos:prefLabel xml:lang="en">mode</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ValueQualifier:200811">mode</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">5.5.</skos:notation>
    <skos:historyNote>GeoSciML UML model; definition this vocabulary</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:ValueQualifier:200811:median">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:ValueQualifier:200811" />
    <skos:definition>Half of the values in a collection of measured values for the property are greater than the reported value and half are less than the reported value.</skos:definition>
    <skos:prefLabel xml:lang="en">median</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:ValueQualifier:200811">median</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">5.6.</skos:notation>
    <skos:historyNote>GeoSciML UML model; definition this vocabulary</skos:historyNote>
  </skos:Concept>
