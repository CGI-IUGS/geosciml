  <skos:ConceptScheme rdf:about="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">
    <skos:prefLabel xml:lang="en">FaultMovementType</skos:prefLabel>
    <skos:changeNote>
      <rdf:Description>
        <rdf:value>Converted from Excel document.</rdf:value>
        <dc:date>2009-07-14</dc:date>
        <dc:creator>Stephen M Richard</dc:creator>
      </rdf:Description>
    </skos:changeNote>
    <skos:editorialNote>This file contains the 200811 version of CGI Fault movement type vocabulary. Includes terms used to define the type of movement (eg dip-slip, strike-slip) for a DisplacementValue on a ShearDisplacementStructure.</skos:editorialNote>
  </skos:ConceptScheme>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:dip_separation_sense">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>A fault along which there is some separation parallel to the dip of the fault.</skos:definition>
    <skos:prefLabel xml:lang="en">dip separation sense</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">dip_separation_sense</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">1..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:dip_slip">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>The net slip of the fault lies in the dip direction of the fault.</skos:definition>
    <skos:prefLabel xml:lang="en">dip slip</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">dip_slip</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:FaultMovementType:200811:dip_separation_sense" />
    <skos:notation rdf:datatype="LocalHierarchyKey">1.1.</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:oblique_slip">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>The net slip of the fault lies between the strike and dip directions of the fault; the slip vector rakes between 10 and 80 degrees in the plane of the fault.</skos:definition>
    <skos:prefLabel xml:lang="en">oblique slip</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">oblique_slip</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">2..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia); SMR</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:transpressional">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>A fault along which strike-slip deformation is accompanied by a component of shortening transverse to the fault.</skos:definition>
    <skos:prefLabel xml:lang="en">transpressional</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">transpressional</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:FaultMovementType:200811:oblique_slip" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.1.</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:transtensional">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>A fault along which strike-slip deformation is accompanied by a component of extension transverse to the fault.</skos:definition>
    <skos:prefLabel xml:lang="en">transtensional</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">transtensional</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:FaultMovementType:200811:oblique_slip" />
    <skos:notation rdf:datatype="LocalHierarchyKey">2.2.</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:strike_separation_sense">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>A fault along which there is some separation parallel to the strike of the fault.</skos:definition>
    <skos:prefLabel xml:lang="en">strike separation sense</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">strike_separation_sense</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">3..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:strike_slip">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>The net slip of the fault (slip vector) is parallel to the strike of the fault.</skos:definition>
    <skos:prefLabel xml:lang="en">strike slip</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">strike_slip</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:FaultMovementType:200811:strike_separation_sense" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.1.</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia); SMR</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:transcurrent">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>A large scale strike-slip fault in which the fault surface is steeply inclined.</skos:definition>
    <skos:prefLabel xml:lang="en">transcurrent</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">transcurrent</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:FaultMovementType:200811:strike_slip" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.1.1</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:transform">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>A variety of strike-slip fault along which the displacement suddenly stops or changes form; typically associated with mid-ocean ridges.</skos:definition>
    <skos:prefLabel xml:lang="en">transform</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">transform</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:FaultMovementType:200811:strike_slip" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.1.2</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:wrench">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>A strike slip fault in which the faut plane is more or less vertical.</skos:definition>
    <skos:prefLabel xml:lang="en">wrench</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">wrench</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:FaultMovementType:200811:strike_slip" />
    <skos:notation rdf:datatype="LocalHierarchyKey">3.1.3</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:horizontal">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>The fault is horizontal.</skos:definition>
    <skos:prefLabel xml:lang="en">horizontal</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">horizontal</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">4..</skos:notation>
    <skos:historyNote>GGIC vocabularies working group (Australia)</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:scissor">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>A fault on which there is increasing offset or separation along the strike from an initial point of no offset, with reverse offset in the opposite direction.</skos:definition>
    <skos:prefLabel xml:lang="en">scissor</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">scissor</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">5..</skos:notation>
    <skos:historyNote>Neuendorf et al 2005</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:extraction">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>A fault whose two sides have approached each other substantially in the direction perpendicular to the fault.</skos:definition>
    <skos:prefLabel xml:lang="en">extraction</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">extraction</skos:notation>
    <skos:notation rdf:datatype="LocalHierarchyKey">6..</skos:notation>
    <skos:historyNote>Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:pure_extraction">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>An extraction fault with no discernible displacement within the fault plane.</skos:definition>
    <skos:prefLabel xml:lang="en">pure extraction</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">pure_extraction</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:FaultMovementType:200811:extraction" />
    <skos:notation rdf:datatype="LocalHierarchyKey">6.1.</skos:notation>
    <skos:historyNote>Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</skos:historyNote>
  </skos:Concept>
  <skos:Concept rdf:about="urn:cgi:classifier:CGI:FaultMovementType:200811:mixed_extraction">
    <skos:inScheme rdf:resource="urn:cgi:classifierScheme:CGI:FaultMovementType:200811" />
    <skos:definition>An extraction fault with some displacement within the fault plane.</skos:definition>
    <skos:prefLabel xml:lang="en">mixed extraction</skos:prefLabel>
    <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:FaultMovementType:200811">mixed_extraction</skos:notation>
    <skos:broader rdf:resource="urn:cgi:classifier:CGI:FaultMovementType:200811:extraction" />
    <skos:notation rdf:datatype="LocalHierarchyKey">6.2.</skos:notation>
    <skos:historyNote>Froitzheim, N., Pleuger, J. &amp; Nagel, T.J., 2006. Extraction faults. Journal of Structural Geology v28, pp. 1388-1395.</skos:historyNote>
  </skos:Concept>
</rdf:RDF>