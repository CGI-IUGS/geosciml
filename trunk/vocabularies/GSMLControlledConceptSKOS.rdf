<?xml version="1.0"?>
<rdf:RDF
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:gsml-r="http://geosciml.org/geosciml/3.0_alpha/rdf/gsml#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#">
	<owl:Ontology
		rdf:about="">
		<dc:creator>Simon Cox</dc:creator>
		<dc:description
			xml:lang="en">An RDF vocabulary for describing concept schemes for classifying
			geoscience features. 
			
			This context adds the notion of &quot;prototype&quot; to the SKOS
			vocabulary, to carry a pointer to an instance that is considered to exemplify the
			concept.</dc:description>
		<dc:title
			xml:lang="en">GeoSciML Controlled Concept vocabulary</dc:title>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
		<skos:changeNote>
			<rdf:Description>
				<rdf:value>
					Cleaned up in response to comments from Luis Bermudez: 
						1. deleted the redundant rdf:Description (individual)
				</rdf:value>
				<dc:date>2009-03-19</dc:date>
				<dc:creator>Simon Cox</dc:creator>
			</rdf:Description>
		</skos:changeNote>
	</owl:Ontology>

	<!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

	<!-- http://geosciml.org/geosciml/3.0_alpha/rdf/gsml#prototype -->

	<rdf:Property
		rdf:about="#prototype">
		<rdfs:label
			xml:lang="en">prototype</rdfs:label>
		<rdfs:comment
			xml:lang="en">This property may be used directly.</rdfs:comment>
		<rdfs:isDefinedBy
			rdf:resource="http://geosciml.org/geosciml/2.0/doc/"/>
		<rdfs:subPropertyOf
			rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
		<skos:definition
			xml:lang="en">
				prototype links from a ControlledConcept to one geologic entity 
				(e.g. GeologicFeature or EarthMaterial) 
				which exemplifies  this ControlledConcept. 
			
				N.B. domain and range are each skos:Concept because it is subPropertyOf skos:semanticRelation </skos:definition>
		<skos:changeNote>
			<rdf:Description>
				<rdf:value>Initial description.</rdf:value>
				<dc:date>2008-10-27</dc:date>
				<dc:creator>Simon Cox</dc:creator>
			</rdf:Description>
		</skos:changeNote>
		<skos:changeNote>
			<rdf:Description>
				<rdf:value>
					Cleaned up in response to comments from Luis Bermudez: 
						1. changed type to rdf:Property (owl:ObjectProperty is implied by subPropertyOf skos:semanticRelation)
						2. fixed isDefinedBy link to point to GeoSciML 2.0 release</rdf:value>
				<dc:date>2009-03-19</dc:date>
				<dc:creator>Simon Cox</dc:creator>
			</rdf:Description>
		</skos:changeNote>
	</rdf:Property>

</rdf:RDF>
