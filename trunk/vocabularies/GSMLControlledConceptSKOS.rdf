<?xml version="1.0"?>
<rdf:RDF
	xmlns="http://geosciml.org/release/3.0_alpha/rdf/gsmlCC#"
	xml:base="file:///C:/projects/SVN_xmml/ontologies/GSML/GSMLControlledConceptSKOS.xml"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:gsmlCC="http://geosciml.org/release/3.0_alpha/rdf/gsmlCC#"
	xmlns:terms="http://purl.org/dc/terms/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#">
	<owl:Ontology
		rdf:about="">
		<dc:creator>Simon Cox</dc:creator>
		<dc:description
			xml:lang="en">An RDF vocabulary for describing concept schemes for classifying
			geoscience features. This adds the notion of &quot;prototype&quot; to the SKOS
			vocabulary, to carry a pointer to an instance that is considered to exemplify the
			concept.</dc:description>
		<dc:title
			xml:lang="en">GeoSciML Controlled Concept vocabulary</dc:title>
		<owl:imports
			rdf:resource="file:///C:/projects/SVN_xmml/w3c/skos/2004_02_core.rdf"/>
	</owl:Ontology>

	<!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

	<!-- http://geosciml.org/release/3.0_alpha/rdf/gsmlCC#prototype -->

	<owl:ObjectProperty
		rdf:about="#prototype">
		<rdfs:label
			xml:lang="en">prototype</rdfs:label>
		<rdfs:isDefinedBy
			rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://geosciml.org/release/2.0_rc3/doc</rdfs:isDefinedBy>
		<rdfs:comment
			xml:lang="en">This property may be used directly.</rdfs:comment>
		<rdfs:domain
			rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
		<rdfs:subPropertyOf
			rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
	</owl:ObjectProperty>

	<!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

	<!-- http://geosciml.org/release/3.0_alpha/rdf/gsmlCC#prototype -->

	<rdf:Description
		rdf:about="#prototype">
		<terms:issued>2008-10-27</terms:issued>
		<skos:definition
			xml:lang="en">The prototype links the ControlledConcept to one special geologic entity
			(e.g. GeologicFeature or EarthMaterial) that defines the
			ControlledConcept.</skos:definition>
		<skos:changeNote>
			<rdf:Description>
				<rdf:value>Initial description.</rdf:value>
			</rdf:Description>
			<rdf:Description>
				<dc:date>2008-10-27</dc:date>
			</rdf:Description>
			<rdf:Description>
				<dc:creator>Simon Cox</dc:creator>
			</rdf:Description>
		</skos:changeNote>
	</rdf:Description>

</rdf:RDF>
