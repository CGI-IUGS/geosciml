<?xml version="1.0"?>
<rdf:RDF
	xml:base="file:///C:/projects/SVN_xmml/ontologies/GSML/GSMLControlledConceptSKOS.xml"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:gsmlCC="http://geosciml.org/release/3.0_alpha/rdf/gsmlCC#"
	xmlns:terms="http://purl.org/dc/terms/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#">
	<owl:Ontology
		rdf:about="">
		<dc:creator>Simon Cox</dc:creator>
		<dc:description
			xml:lang="en">An RDF vocabulary for describing concept schemes for classifying
			geoscience features. This adds the notion of &quot;prototype&quot; to the SKOS
			vocabulary, to carry a pointer to an instance that is considered to exemplify the
			concept.</dc:description>
		<dc:title
			xml:lang="en">GeoSciML Controlled Concept vocabulary</dc:title>
		<owl:imports
			rdf:resource="http://www.w3.org/2004/02/skos/core"/>
	</owl:Ontology>

	<!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

	<!-- http://geosciml.org/release/3.0_alpha/rdf/gsmlCC#prototype -->

	<rdf:Property
		rdf:about="#prototype">
		<rdfs:label
			xml:lang="en">prototype</rdfs:label>
		<rdfs:isDefinedBy
			rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.geosciml.org/geosciml/2.0/doc/</rdfs:isDefinedBy>
		<rdfs:comment
			xml:lang="en">This property may be used directly.</rdfs:comment>
		<rdfs:subPropertyOf
			rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
		<skos:definition
			xml:lang="en">The prototype links a ControlledConcept to one geologic entity
			(e.g. GeologicFeature or EarthMaterial) that is the exemplar of this
			ControlledConcept.</skos:definition>
		<skos:changeNote>
			<rdf:Description>
				<rdf:value>Initial description.</rdf:value>
			</rdf:Description>
			<rdf:Description>
				<dc:date>2008-10-27</dc:date>
			</rdf:Description>
			<rdf:Description>
				<dc:creator>Simon Cox</dc:creator>
			</rdf:Description>
		</skos:changeNote>
		<skos:changeNote>
			<rdf:Description>
				<rdf:value>Cleaned up in response to comments from Luis Bermudez:
				1. changed to an rdf:Property (owl:ObjectProperty is implied by subPropertyOf semanticRelation)
				2. deleted the redundant rdf:Description (individual)
				3. fixed isDefinedBy link to point to GeoSciML 2.0 release</rdf:value>
			</rdf:Description>
			<rdf:Description>
				<dc:date>2009-03-18</dc:date>
			</rdf:Description>
			<rdf:Description>
				<dc:creator>Simon Cox</dc:creator>
			</rdf:Description>
		</skos:changeNote>
	</rdf:Property>

</rdf:RDF>
