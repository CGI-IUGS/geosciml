<?xml version="1.0" encoding="UTF-8"?>
<Gsml xmlns="http://www.cgi-iugs.org/xml/GeoSciML/1" xmlns:gml="http://www.opengis.net/gml" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cgi-iugs.org/xml/GeoSciML/1 ../schema/GeoSciML.xsd">
  <!-- GSV Mount Leinster CompoundMaterials  -->
  <geologicObjectMember>
    <CompoundMaterial gml:id="GSV1">
      <gml:description>grey, equigranular to strongly porphyritic in quartz and feldspar, miarolitic cavities often lined with smoky quartz</gml:description>
      <gml:name>Biotite Granite</gml:name>
      <color>
        <CGI_TermValue>
          <value codeSpace="http://www.iugs-cgi.org/colorVocabulary">grey</value>
        </CGI_TermValue>
      </color>
      <consolidationDegree>
        <CGI_TermValue>
          <value codeSpace="http://www.iugs-cgi.org/consolidationDegreeList">well indurated</value>
        </CGI_TermValue>
      </consolidationDegree>
      <metamorphicGrade>
        <CGI_TermValue>
          <value codeSpace="http://www.iugs-cgi.org/metamorphicGradeVocabulary">Not metamorphosed</value>
        </CGI_TermValue>
      </metamorphicGrade>
      <particleGeometry>
        <CGI_PhysicalDescription>
          <property codeSpace="http://www.iugs-cgi.org/physicalPropertyList">Grain size</property>
          <value>
            <CGI_TermValue>
              <value codeSpace="http://www.iugs-cgi.org/grainSizeVocabulary">coarse</value>
            </CGI_TermValue>
          </value>
        </CGI_PhysicalDescription>
      </particleGeometry>
      <fabric>
        <CGI_TermValue>
          <value codeSpace="http://www.iugs-cgi.org/fabricVocabulary">equigranular</value>
        </CGI_TermValue>
      </fabric>
      <grossComposition>
        <CGI_TermValue>
          <value codeSpace="http://www.iugs-cgi.org/grossCompositionVocabulary">Alkalic Igneous</value>
        </CGI_TermValue>
      </grossComposition>
      <compositionClass>
        <CGI_TermValue>
          <value codeSpace="http://www.iugs-cgi.org/lithologicVocabulary">granite</value>
        </CGI_TermValue>
      </compositionClass>
      <genesis>
        <CGI_TermValue>
          <value codeSpace="http://www.iugs-cgi.org/genesisVocabulary">Intrusive igneous</value>
        </CGI_TermValue>
      </genesis>
    </CompoundMaterial>
  </geologicObjectMember>
</Gsml>
