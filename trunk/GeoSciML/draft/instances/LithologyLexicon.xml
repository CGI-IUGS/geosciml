	<!-- example earth material prototype -->
	<geologicObjectMember>
		<EarthMaterial xsi:type="CompoundMaterialType" gml:id="CM0001">
			<gml:description>Example of prototype/ defining entity instance for 'Sedimentary rock' in litholgoy vocabulary</gml:description>
			<gml:name>urn:x-cgi:def:material:AZGS:2005:matLex1:SedimentaryRock</gml:name>
			<purpose>identifyingNorm</purpose>
			<classifier xlink:href="#vocab1"/> <!-- this is reference to 'Sedimentary Rock' controlled concept in urn:x-cgi:def:material:AzGS:2005:matLex1-->
			<color>
				<CGI_TermValue>
					<value codeSpace="http://pubs.usgs.gov/Colors">"Any Color"</value>
				</CGI_TermValue>
			</color>
			<consolidationDegree>
				<CGI_TermValue>
					<qualifier>always</qualifier>
					<value codeSpace="http://pubs.usgs.gov/Vocab">"consolidated"</value>
				</CGI_TermValue>
			</consolidationDegree>
			<metamorphicGrade>
				<CGI_TermValue>
					<qualifier>always</qualifier>
					<value codeSpace="http://pubs.usgs.gov/Vocab">"not metamorphosed"</value>
				</CGI_TermValue>
			</metamorphicGrade>
			<particleGeometry/>
			<fabric/>
			<grossChemistry>unknown</grossChemistry>
			<genesis>
				<CGI_TermValue>
					<qualifier>always</qualifier>
					<value codeSpace="http://pubs.usgs.gov/Vocab">"Sedimentary genesis"</value>
				</CGI_TermValue>
			</genesis>
		</EarthMaterial>
