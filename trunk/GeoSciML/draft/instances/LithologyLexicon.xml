					<preferredName>Sandstone _sandy_rock_of SLTTs</preferredName>
					<prototype xlink:href="http://pubs.usgs.gov/Geolex/GeoSciML_lith_prototypes/Sandstone _sandy_rock_of SLTTs.xml"/>
					<vocabulary xlink:href="#ExampleLithologyVocabulary"/>
					<metadata/>
				</ControlledConcept>
			</member>
			<!-- -->
			<!-- -->
			<member>
				<ControlledConcept gml:id="vocab11">
					<gml:description>A carbonate rock with a dolomite:calcite ratio greater than 9:1, and having non-carbonate impurities between 10 and 50 percent.</gml:description>
					<gml:name>urn:x-cgi:def:lithostratigraphy:USGS:2006:AzSLTTs:Calcareous_dolostone</gml:name>
					<preferredName>Calcareous dolostone</preferredName>
