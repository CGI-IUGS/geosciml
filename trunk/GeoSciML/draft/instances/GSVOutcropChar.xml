<?xml version="1.0" encoding="UTF-8"?>
<Gsml xmlns="http://www.cgi-iugs.org/xml/GeoSciML/1.0" xmlns:gml="http://www.opengis.net/gml" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cgi-iugs.org/xml/GeoSciML/1.0 ../schema/GeoSciML.xsd">
  <!-- Concepts  -->
  <vocabularyMember>
    <GeologicVocabulary gml:id="gsv1.0">
      <gml:description>Describes the nature of outcrops formed by a Geological Unit.</gml:description>
      <gml:name>OutcropCharacter</gml:name>
      <conceptMember>
        <ControlledConcept gml:id="OutcropCharacter-1">
          <!-- simple in-line definition -->
          <gml:description>geological unit forms bouldery outcrops</gml:description>
          <gml:name>bouldery</gml:name>
          <preferredName>bouldery</preferredName>
          <prototype/>
        </ControlledConcept>
      </conceptMember>
      <conceptMember>
        <ControlledConcept gml:id="OutcropCharacter-2">
          <gml:description>outcrops of geological unit form cliffs</gml:description>
          <gml:name>cliff-forming</gml:name>
          <preferredName>cliff-forming</preferredName>
          <prototype/>
        </ControlledConcept>
      </conceptMember>
      <conceptMember>
        <ControlledConcept gml:id="OutcropCharacter-3">
          <gml:description>outcrops of geological unit form ledges</gml:description>
          <gml:name>ledge-forming</gml:name>
          <preferredName>ledge-forming</preferredName>
          <prototype/>
        </ControlledConcept>
      </conceptMember>
      <conceptMember>
        <ControlledConcept gml:id="OutcropCharacter-4">
          <gml:description>outcrops of geological unit form slopes</gml:description>
          <gml:name>slope-forming</gml:name>
          <preferredName>slope-forming</preferredName>
          <prototype/>
        </ControlledConcept>
      </conceptMember>
      <conceptMember>
        <ControlledConcept gml:id="OutcropCharacter-5">
          <gml:description>outcrops of geological unit are recessive relative to adjacent units</gml:description>
          <gml:name>recessive</gml:name>
          <preferredName>recessive</preferredName>
          <prototype/>
        </ControlledConcept>
      </conceptMember>
      <conceptMember>
        <ControlledConcept gml:id="OutcropCharacter-6">
          <gml:description>applied to lava flows that have a surface of angular blocks; fragments more regular, smoother and less vesicular than aa</gml:description>
          <gml:name>blocky</gml:name>
          <preferredName>blocky</preferredName>
          <prototype/>
        </ControlledConcept>
      </conceptMember>
      <conceptMember>
        <ControlledConcept gml:id="OutcropCharacter-7">
          <gml:description>geological unit is poorly exposed</gml:description>
          <gml:name>poorly exposed</gml:name>
          <preferredName>poorly exposed</preferredName>
          <prototype/>
        </ControlledConcept>
      </conceptMember>
    </GeologicVocabulary>
  </vocabularyMember>
</Gsml>
