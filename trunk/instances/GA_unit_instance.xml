<?xml version="1.0" encoding="UTF-8"?>
<LithostratigraphicUnit xmlns="http://www.cgi-iugs.org/xml/GeoSciML/2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gml="http://www.opengis.net/gml"
    xsi:schemaLocation="http://www.cgi-iugs.org/xml/GeoSciML/2 http://www.cgi-iugs.org/xml/GeoSciML/2/geologicUnit.xsd">
    <gml:name codeSpace="http://www.ga.gov.au">Kabadah Formation</gml:name>
    <observationMethod>
        <CGI_TermValue>
            <value codeSpace="http://www.ga.gov.au/GeoloigicalVocabs">field mapping</value>
        </CGI_TermValue>
    </observationMethod>
    <purpose>instance</purpose>
    <preferredAge>
        <GeologicEvent>
            <eventAge>
                <CGI_Value xsi:type="CGI_TermValueType">
                    <value codeSpace="http://www.ga.gov.au/ICS_timescale">Silurian</value>
                </CGI_Value>
            </eventAge>
            <eventProcess>
                <CGI_TermValue>
                    <value codeSpace="http://www.ga.gov.au/GeologicalVocabs">deposition</value>
                </CGI_TermValue>
            </eventProcess>
            <eventEnvironment>
                <CGI_TermValue>
                    <value codeSpace="http://www.ga.gov.au/GeologicalVocabs">marine</value>
                </CGI_TermValue>
            </eventEnvironment>
        </GeologicEvent>
    </preferredAge>
    <rank codeSpace="http://www.ga.gov.au/GeologicalVocabs">formation</rank>
    <classifier>
        <ControlledConcept>
            <preferredName>Kabadah Formation</preferredName>
            <vocabulary xlink:type="simple" xlink:href="www.ga.gov.au/GA_stratigraphic_lexicon"/>
        </ControlledConcept>
    </classifier>
</LithostratigraphicUnit>
