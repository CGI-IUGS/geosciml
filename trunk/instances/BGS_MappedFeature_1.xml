<?xml version="1.0" encoding="UTF-8"?>
<!--Example mapped feature from BGS data.-->
<MappedFeature xmlns="urn:cgi:xmlns:CGI:GeoSciML:2.0" gml:id="mf.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="
	urn:cgi:xmlns:CGI:GeoSciML:2.0	https://www.seegrid.csiro.au/subversion/GeoSciML/trunk/schema/GeoSciML/geosciml.xsd"
	>
	<positionalAccuracy>
		<CGI_TermValue>
			<value codeSpace="http://www.bgs.ac.uk/positionalAccuracyVocab">unknown</value>
		</CGI_TermValue>
	</positionalAccuracy>
	<observationMethod>
		<CGI_TermValue>
			<value codeSpace="http://www.bgs.ac.uk/observationMethodVocab">fieldObservation</value>
		</CGI_TermValue>
	</observationMethod>
	<observationMethod>
		<CGI_TermValue>
			<value codeSpace="http://www.bgs.ac.uk/observationMethodVocab">Boreholes</value>
		</CGI_TermValue>
	</observationMethod>
	<samplingFrame xlink:href="urn:cgi:feature:CGI:EarthNaturalSurface"/>
	<shape><gml:Solid><!-- complex content here --></gml:Solid></shape>
	<specification>
	<LithostratigraphicUnit>
	<gml:name codeSpace="http://www.bgs.ac.uk/LithostratLexicon">DEEP CONGLOMERATE FORMATION</gml:name>
		<observationMethod>
			<CGI_TermValue>
				<value codeSpace="http://www.bgs.ac.uk/observationMethodVocab">Summary of published description</value>
			</CGI_TermValue>
		</observationMethod>
		<purpose>instance</purpose>
		<preferredAge>
		<GeologicEvent>
			<eventAge>
			<CGI_TermRange>
				<lower>
