<?xml version="1.0" encoding="UTF-8"?>
<!--Example mapped feature from BGS data.-->
<GSML xmlns="urn:cgi:xmlns:CGI:GeoSciML:2.0" gml:id="mf.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml" xmlns:swe="http://www.opengis.net/swe/1.0" xmlns:om="http://www.opengis.net/om/1.0" xmlns:sa="http://www.opengis.net/sampling/1.0" xmlns:cv="http://www.opengis.net/cv/0.2" xmlns:geox="http://www.opengis.net/geomExt/0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
	http://www.opengis.net/gml 		http://schemas.opengis.net/gml/3.1.1/base/gml.xsd
	urn:cgi:xmlns:CGI:GeoSciML:2.0	http://www.geosciml.org/schemas/geosciml/2.0/geosciml.xsd
	http://www.opengis.net/swe/1.0		http://schemas.opengis.net/swe/1.0.0/swe.xsd 
	http://www.opengis.net/cv/0.2 		http://schemas.opengis.net/cv/0.2.0/cv.xsd 
	http://www.opengis.net/om/1.0 		http://schemas.opengis.net/om/1.0.0/om.xsd 
	http://www.opengis.net/sampling/1.0 	http://schemas.opengis.net/sampling/1.0.0/sampling.xsd">
	<member>
		<MappedFeature>
			<observationMethod>
				<CGI_TermValue>
					<value codeSpace="urn:cgi:classifierScheme:BGS:ObservationMethod">fieldObservation</value>
				</CGI_TermValue>
			</observationMethod>
			<observationMethod>
				<CGI_TermValue>
					<value codeSpace="urn:cgi:classifierScheme:BGS:ObservationMethod">Boreholes</value>
				</CGI_TermValue>
			</observationMethod>
			<positionalAccuracy>
				<CGI_TermValue>
					<value codeSpace="urn:cgi:classifierScheme:BGS:PositionalAccuracy">unknown</value>
				</CGI_TermValue>
			</positionalAccuracy>
			<samplingFrame xlink:href="urn:cgi:classifier:BGS:SurfaceOfMapping:Rockhead"/>
			<specification>
				<LithostratigraphicUnit>
					<observationMethod>
						<CGI_TermValue>
							<value codeSpace="urn:cgi:classifierScheme:BGS:ObservationMethod">Summary of published description</value>
						</CGI_TermValue>
					</observationMethod>
					<purpose>instance</purpose>
					<classifier>
						<ControlledConcept gml:id="LEXREF1" >
							<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:StratigraphicLexicon:DECO</gml:name>
							<identifier codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:StratigraphicLexicon:DECO</identifier>
							<name xml:lang="en" codeSpace="urn:cgi:classifierScheme:BGS:StratigraphicLexicon"  >DEEP CONGLOMERATE FORMATION</name>
							<vocabulary xlink:href="urn:cgi:classifierScheme:BGS:StratigraphicLexicon" />
						</ControlledConcept>
					</classifier>
				</LithostratigraphicUnit>
			</specification>
			<shape></shape>
		</MappedFeature>
	</member>
	<member>
		<StratigraphicLexicon gml:id="LEX">
			<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifierScheme:BGS:StratigraphicLexicon</gml:name>
			<gml:definitionMember>
				<ControlledConcept gml:id="LEXREF2">
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:StratigraphicLexicon:DECO</gml:name>
					<identifier codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:StratigraphicLexicon:DECO</identifier>
					<name xml:lang="en" codeSpace="urn:cgi:classifierScheme:BGS:StratigraphicLexicon"  >DEEP CONGLOMERATE FORMATION</name>
					<prototype>
						<LithostratigraphicUnit>
							<observationMethod>
								<CGI_TermValue>
									<value codeSpace="urn:cgi:classifierScheme:BGS:ObservationMethod">Summary of published description</value>
								</CGI_TermValue>
							</observationMethod>
							<purpose>definingNorm</purpose>
							<preferredAge>
								<GeologicEvent>
									<eventAge>
										<CGI_TermRange>
											<lower>
												<CGI_TermValue>
													<value codeSpace="urn:cgi:classifierScheme:ICS:InternationalStratigraphicChart:2004">LOCHKOVIAN</value>
												</CGI_TermValue>
											</lower>
											<upper>
												<CGI_TermValue>
													<value codeSpace="urn:cgi:classifierScheme:ICS:InternationalStratigraphicChart:2004">PRAGHIAN</value>
												</CGI_TermValue>
											</upper>
										</CGI_TermRange>
									</eventAge>
									<eventProcess>
										<CGI_TermValue>
											<value codeSpace="urn:cgi:classifierScheme:BGS:GeologicProcess">DEPOSITION</value>
										</CGI_TermValue>
									</eventProcess>
								</GeologicEvent>
							</preferredAge>
							<composition>
								<CompositionPart>
									<role codeSpace="urn:cgi:classifierScheme:BGS:RoleVocab">dominantConstituent</role>
									<lithology>
										<ControlledConcept gml:id="RCS1" >
											<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:RCS:CONG</gml:name>
											<identifier codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:RCS:CONG</identifier>
											<name xml:lang="en" codeSpace="urn:cgi:classifierScheme:BGS:RCS">CONGLOMERATE</name>
											<vocabulary xlink:href="urn:cgi:classifierScheme:BGS:RCS" />
										</ControlledConcept>
									</lithology>
									<proportion>
										<CGI_NumericValue>
											<principalValue uom="percent">100</principalValue>
										</CGI_NumericValue>
									</proportion>
								</CompositionPart>
							</composition>
						</LithostratigraphicUnit>
					</prototype>
					<vocabulary xlink:href="urn:cgi:classifierScheme:BGS:StratigraphicLexicon" />
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="LEXREF3">
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:StratigraphicLexicon:SCN</gml:name>
					<identifier codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:StratigraphicLexicon:SCN</identifier>
					<name xml:lang="en" codeSpace="urn:cgi:classifierScheme:BGS:StratigraphicLexicon">SCONE SANDSTONE FORMATION</name>
					<prototype>
						<LithostratigraphicUnit>
							<observationMethod>
								<CGI_TermValue>
									<value codeSpace="urn:cgi:classifierScheme:BGS:ObservationMethod">Summary of published description</value>
								</CGI_TermValue>
							</observationMethod>
							<purpose>definingNorm</purpose>
							<preferredAge>
								<GeologicEvent>
									<eventAge>
									<CGI_TermValue>
										<value codeSpace="urn:cgi:classifierScheme:ICS:InternationalStratigraphicChart:2004">EARLY DEVONIAN</value>
									</CGI_TermValue>
									</eventAge>
									<eventProcess>
										<CGI_TermValue>
											<value codeSpace="urn:cgi:classifierScheme:BGS:GeologicProcess">DEPOSITION</value>
										</CGI_TermValue>
									</eventProcess>
								</GeologicEvent>
							</preferredAge>
							<composition>
								<CompositionPart>
									<role codeSpace="urn:cgi:classifierScheme:BGS:RoleVocab">dominantConstituent</role>
									<lithology>
										<ControlledConcept gml:id="RCS2" >
											<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:RCS:SDST</gml:name>
											<identifier codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:BGS:RCS:SDST</identifier>
											<name xml:lang="en" codeSpace="urn:cgi:classifierScheme:BGS:RCS">SANDSTONE</name>
											<vocabulary xlink:href="urn:cgi:classifierScheme:BGS:RCS"/>
										</ControlledConcept>
									</lithology>
									<proportion>
										<CGI_NumericValue>
											<principalValue uom="percent">100</principalValue>
										</CGI_NumericValue>
									</proportion>
								</CompositionPart>
							</composition>
						</LithostratigraphicUnit>
					</prototype>
					<vocabulary xlink:href="urn:cgi:classifierScheme:BGS:StratigraphicLexicon"/>
				</ControlledConcept>
			</gml:definitionMember>
		</StratigraphicLexicon>
	</member>
</GSML>