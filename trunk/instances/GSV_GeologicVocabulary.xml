xmlns:cgu="urn:cgi:xmlns:CGI:Utilities:1.0"
xsi:schemaLocation="urn:cgi:xmlns:CGI:GeoSciML:2.0 https://www.seegrid.csiro.au/subversion/GeoSciML/trunk/schema/GeoSciML/geosciml.xsd">
	<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifierScheme:GSV:Vocabularies:2007></gml:name>
	<gml:definitionMember>
		<gsml:GeologicVocabulary gml:id="id_gv01">
			<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifierScheme:GSV:Lithology:2007</gml:name> 
<!-- The name of this vocabulary.  Is the vocabulary a "classifierScheme" or an "object"? -->
			<gml:definitionMember> <!-- Use definitionMember or dictionaryEntry? -->
				<gsml:ControlledConcept gml:id="id_cc01"> <!-- gml:id is internal document identifier, just has to be unique, don't use anything that's required to have external significance -->							
					<gml:description> QAPF Field 3 or TAS Field R; silicic volcanic rock consisting of phenocrysts of quartz and alkali feldspar, often with minor plagioclase and biotite, in a microcrystalline or glassy groundmass and having the chemical composition of granite </gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:Lithology:2007:rhyolite</gml:name> 
<!-- this is the GSV identifier for the concept -->
					<gsml:identifier codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:Lithology:2007:rhyolite</gsml:identifier> 
<!-- the identifier attribute appears redundant -->
					<gsml:name codeSpace="urn:cgi:classifierScheme:GSV:Lithology:2007" xml:lang="en">rhyolite</gsml:name>
					<gsml:vocabulary xlink:href="urn:cgi:classifierScheme:GSV:Lithology:2007"/>
<!-- If there is a CGI definedControlledConcept that can be related to this concept then use the VocabRelation class -->
					<gsml:linkToTarget>
						<gsml:VocabRelation gml:id="id_vr01">
							<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:Relationships:2007:NarrowerTerm</gml:name> 
<!-- The name of the relationship -->
							<gsml:role  codeSpace="urn:cgi:classifierScheme:GSV:Roles:2007">narrower term</gsml:role> 
<!-- The role the source plays in the relationship -->
							<gsml:source xlink:href="urn:cgi:classifier:GSV:Lithology:2007:rhyolite"/>
<!-- The source is the identifier for source term -->
							<gsml:target xlink:href="urn:cgi:classifier:CGI:Lithology:2007:aphaniticvolcanicrock"/> 
<!-- The target is the language independent identifier -->
							<gsml:vocabulary xlink:href="urn:cgi:classifierScheme:GSV:Relationships:2007"/> 
<!-- The vocabulary that contains the VocabRelation terms.-->
						</gsml:VocabRelation>
					</gsml:linkToTarget>
				</gsml:ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember> <!-- next entry in the vocabulary. -->
				<gsml:ControlledConcept gml:id="id_cc02"> <!-- gml:id is internal document identifier, just has to be unique, don't use anything that's required to have external significance -->							
					<gml:description> clastic sedimentary rock composed predominantly of fragments 0.032-2 mm in diameter </gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:Lithology:2007:Sandstone</gml:name> 
<!-- this is the GSV identifier for the concept -->
					<gsml:identifier codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:Lithology:2007:Sandstone</gsml:identifier> 
<!-- the identifier attribute appears redundant -->
					<gsml:name codeSpace="urn:cgi:classifierScheme:GSV:Lithology:2007" xml:lang="en">sandstone</gsml:name>
					<gsml:vocabulary xlink:href="urn:cgi:classifierScheme:GSV:Lithology:2007"/>
				</gsml:ControlledConcept>
			</gml:definitionMember>
