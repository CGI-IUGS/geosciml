	<gml:definitionMember>
		<gsml:GeologicVocabulary gml:id="id_gv02">
			<gml:name codeSpace="rn:ietf:rfc:2141">urn:cgi:classifierScheme:GSV:OutcropCharacter:2007</gml:name> 
<!-- The name of this vocabulary.   -->
			<gml:definitionMember> 
				<gsml:ControlledConcept gml:id="id_cc03"> <!-- gml:id is internal document identifier, just has to be unique, don't use anything that's required to have external significance -->							
					<gml:description> the geological unit overall is well exposed </gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:OutcropCharacter:2007:WellExposed</gml:name> 
<!-- this is the GSV identifier for the concept -->
					<gsml:identifier codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:OutcropCharacter:2007:WellExposed</gsml:identifier> 
<!-- the identifier attribute appears redundant -->
					<gsml:name codeSpace="urn:cgi:classifierScheme:GSV:OutcropCharacter:2007" xml:lang="en">well exposed</gsml:name>
					<gsml:vocabulary xlink:href="urn:cgi:classifierScheme:GSV:OutcropCharacter:2007"/>
				</gsml:ControlledConcept>
			</gml:definitionMember>
		</gsml:GeologicVocabulary>
	</gml:definitionMember>
</gml:Dictionary>
