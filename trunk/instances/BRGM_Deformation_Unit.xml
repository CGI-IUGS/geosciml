<?xml version="1.0" encoding="UTF-8"?>
<gsml:DeformationUnit xmlns="http://www.cgi-iugs.org/xml/GeoSciML/2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gml="http://www.opengis.net/gml"
	xmlns:gsml="http://www.cgi-iugs.org/xml/GeoSciML/2"
	xsi:schemaLocation="http://www.cgi-iugs.org/xml/GeoSciML/2 http://schemas.cgi-iugs.net/GeoSciML/GeoSciML.xsd">
		<gml:name codeSpace="http://www.brgm.fr">Grès vosgien mylonitisé</gml:name>
		<gsml:observationMethod>
			<gsml:CGI_TermValue>
				<gsml:value codeSpace="http://www.brgm.fr/VocabGeol">à partir de la carte géologique</gsml:value>
			</gsml:CGI_TermValue>
		</gsml:observationMethod>
		<gsml:purpose>instance</gsml:purpose>
		<gsml:preferredAge>
			<gsml:GeologicEvent>
				<gsml:eventAge>
					<gsml:CGI_Value xsi:type="gsml:CGI_TermValueType">
						<gsml:value codeSpace="http://www.brgm.fr/age/2006">Trias</gsml:value>
					</gsml:CGI_Value>
				</gsml:eventAge>
				<gsml:eventProcess>
					<gsml:CGI_TermValue>
						<gsml:value codeSpace="http://www.brgm.fr/TypeMiseEnPlace">dépôt</gsml:value>
					</gsml:CGI_TermValue>
				</gsml:eventProcess>
				<gsml:eventEnvironment>
					<gsml:CGI_TermValue>
						<gsml:value codeSpace="http://www.brgm.fr/Lexique_Environnement">fluviatile</gsml:value>
					</gsml:CGI_TermValue>
				</gsml:eventEnvironment>
			</gsml:GeologicEvent>
		</gsml:preferredAge>
		<gsml:metamorphicCharacter>
			<gsml:MetamorphicDescription>
				<gsml:metamorphicFacies>
					<gsml:CGI_TermValue>
						<gsml:value codeSpace="http://www.brgm.fr/Lexique_Facies_Métamorphiques">schiste vert</gsml:value>
					</gsml:CGI_TermValue>
				</gsml:metamorphicFacies>
				<gsml:metamorphicGrade>
					<gsml:CGI_TermValue>
						<gsml:value codeSpace="http://www.brgm.fr/Lexique_Type_Métamorphisme">barrowien</gsml:value>
					</gsml:CGI_TermValue>
				</gsml:metamorphicGrade>
			</gsml:MetamorphicDescription>
		</gsml:metamorphicCharacter>
		<gsml:definingStructure>
			<gsml:Fault>
				<gml:name codeSpace="http://www.brgm.fr">Faille de Colmar</gml:name>
				<gsml:observationMethod>
					<gsml:CGI_TermValue>
						<gsml:value codeSpace="http://www.brgm.fr/VocabGeol">à partir de la carte géologique</gsml:value>
					</gsml:CGI_TermValue>
				</gsml:observationMethod>
				<gsml:purpose>instance</gsml:purpose>
				<gsml:preferredAge>
					<gsml:DisplacementEvent>
						<gsml:eventAge>
							<gsml:CGI_Value xsi:type="gsml:CGI_TermValueType">
								<gsml:value codeSpace="http://www.brgm.fr/age/2006">Pliocène</gsml:value>
							</gsml:CGI_Value>
						</gsml:eventAge>
						<gsml:eventProcess>
							<gsml:CGI_TermValue>
								<gsml:value codeSpace="http://www.brgm.fr/ProcessType">extension</gsml:value>
							</gsml:CGI_TermValue>
						</gsml:eventProcess>
						<gsml:eventEnvironment>
							<gsml:CGI_TermValue>
								<gsml:value codeSpace="http://www.brgm.fr/Lexique_Structures_Geol_Regionales">Graben du Rhin</gsml:value>
							</gsml:CGI_TermValue>
						</gsml:eventEnvironment>
					</gsml:DisplacementEvent>
				</gsml:preferredAge>
			</gsml:Fault>
		</gsml:definingStructure>
</gsml:DeformationUnit>
