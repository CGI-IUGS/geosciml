<?xml version="1.0" encoding="UTF-8"?>
<gsml:ChronostratigraphicUnit xmlns="http://www.cgi-iugs.org/xml/GeoSciML/2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gml="http://www.opengis.net/gml"
	xmlns:gsml="http://www.cgi-iugs.org/xml/GeoSciML/2"
	xsi:schemaLocation="http://www.cgi-iugs.org/xml/GeoSciML/2 http://schemas.cgi-iugs.net/GeoSciML/GeoSciML.xsd">
	<gml:name codeSpace="http://www.brgm.fr">Lutétien moyen : calcaires grossiers et glauconie de base</gml:name>
	<gsml:observationMethod>
		<gsml:CGI_TermValue>
			<gsml:value codeSpace="http://www.brgm.fr/VocabGeol">à partir de la carte géologique</gsml:value>
		</gsml:CGI_TermValue>
	</gsml:observationMethod>
	<gsml:purpose>instance</gsml:purpose>
	<gsml:preferredAge>
		<gsml:GeologicEvent>
			<gsml:eventAge>
				<gsml:CGI_Value xsi:type="gsml:CGI_TermValueType">
					<gsml:value codeSpace="http://www.brgm.fr/age/2006">Lutétien</gsml:value>
				</gsml:CGI_Value>
			</gsml:eventAge>
			<gsml:eventProcess>
				<gsml:CGI_TermValue>
					<gsml:value codeSpace="http://www.brgm.fr/TypeMiseEnPlace">dépôt</gsml:value>
				</gsml:CGI_TermValue>
			</gsml:eventProcess>
			<gsml:eventEnvironment>
				<gsml:CGI_TermValue>
					<gsml:value codeSpace="http://www.brgm.fr/Lexique_Environnement">lagunaire</gsml:value>
				</gsml:CGI_TermValue>
			</gsml:eventEnvironment>
		</gsml:GeologicEvent>
	</gsml:preferredAge>
	<gsml:weatheringCharacter>
		<gsml:WeatheringDescription>
			<gsml:weatheringProcess>
				<gsml:CGI_TermValue>
					<gsml:value codeSpace="http://www.brgm.fr/Type_Altération">altération par lessivage</gsml:value>
				</gsml:CGI_TermValue>
			</gsml:weatheringProcess>
			<gsml:weatheringProduct>
				<gsml:CGI_TermValue>
					<gsml:value codeSpace="http://www.brgm.fr/Produit_Altération">silcrète</gsml:value>
				</gsml:CGI_TermValue>
			</gsml:weatheringProduct>
		</gsml:WeatheringDescription>
	</gsml:weatheringCharacter>
	<gsml:classifier>
		<gsml:ControlledConcept>
			<gsml:preferredName>Lutétien moyen : calcaires grossiers et glauconie de base</gsml:preferredName>
			<gsml:vocabulary xlink:type="simple" xlink:href="www.brgm.fr/BRGM_appellation_lexique"/>
		</gsml:ControlledConcept>
	</gsml:classifier>
	<gsml:definingAge>
		<gsml:CGI_Value xsi:type="gsml:CGI_TermValueType">
			<gsml:value codeSpace="http://www.brgm.fr/age/2006">Lutétien moyen</gsml:value>
		</gsml:CGI_Value>
	</gsml:definingAge>
	<gsml:unitThickness>
		<gsml:CGI_NumericRange>
			<gsml:lower>
				<gsml:CGI_NumericValue>
					<gsml:principalValue uom="urn:ogc:def:uom:UCUM:m">30</gsml:principalValue>
					<gsml:plusDelta uom="urn:ogc:def:uom:UCUM:m">0</gsml:plusDelta>
					<gsml:minusDelta uom="urn:ogc:def:uom:UCUM:m">0</gsml:minusDelta>
				</gsml:CGI_NumericValue>
			</gsml:lower>
			<gsml:upper>
				<gsml:CGI_NumericValue>
					<gsml:principalValue uom="urn:ogc:def:uom:UCUM:m">40</gsml:principalValue>
					<gsml:plusDelta uom="urn:ogc:def:uom:UCUM:m">0</gsml:plusDelta>
					<gsml:minusDelta uom="urn:ogc:def:uom:UCUM:m">0</gsml:minusDelta>
				</gsml:CGI_NumericValue>
			</gsml:upper>
		</gsml:CGI_NumericRange>
	</gsml:unitThickness>
</gsml:ChronostratigraphicUnit>