<?xml version="1.0" encoding="UTF-8"?>
<gsml:LithostratigraphicUnit xmlns="http://www.cgi-iugs.org/xml/GeoSciML/2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:gml="http://www.opengis.net/gml"
	xmlns:gsml="http://www.cgi-iugs.org/xml/GeoSciML/2"
	xsi:schemaLocation="http://www.cgi-iugs.org/xml/GeoSciML/2 http://schemas.cgi-iugs.net/GeoSciML/GeoSciML.xsd">
		<gml:name codeSpace="http://www.brgm.fr">Grès armoricain</gml:name>
		<gsml:observationMethod>
			<gsml:CGI_TermValue>
				<gsml:value codeSpace="http://www.brgm.fr/VocabGeol">sur le terrain</gsml:value>
			</gsml:CGI_TermValue>
		</gsml:observationMethod>
		<gsml:purpose>instance</gsml:purpose>
		<gsml:preferredAge>
			<gsml:GeologicEvent>
				<gsml:eventAge>
					<gsml:CGI_Value xsi:type="gsml:CGI_TermValueType">
						<gsml:value codeSpace="http://www.brgm.fr/age/2006">Trias</gsml:value>
					</gsml:CGI_Value>
				</gsml:eventAge>
				<gsml:eventProcess>
					<gsml:CGI_TermValue>
						<gsml:value codeSpace="http://www.brgm.fr/TypeMiseEnPlace">dépôt</gsml:value>
					</gsml:CGI_TermValue>
				</gsml:eventProcess>
				<gsml:eventEnvironment>
					<gsml:CGI_TermValue>
						<gsml:value codeSpace="http://www.brgm.fr/Lexique_Environnement">fluviatile</gsml:value>
					</gsml:CGI_TermValue>
				</gsml:eventEnvironment>
			</gsml:GeologicEvent>
		</gsml:preferredAge>
		<gsml:rank codeSpace="http://www.brgm.fr/VocabGeol">formation</gsml:rank>
		<gsml:metamorphicCharacter>
			<gsml:MetamorphicDescription>
				<gsml:metamorphicFacies>
					<gsml:CGI_TermValue>
						<gsml:value codeSpace="http://www.brgm.fr/Lexique_Facies_Métamorphiques">schiste vert</gsml:value>
					</gsml:CGI_TermValue>
				</gsml:metamorphicFacies>
				<gsml:metamorphicGrade>
					<gsml:CGI_TermValue>
						<gsml:value codeSpace="http://www.brgm.fr/Lexique_Type_Métamorphisme">barrowien</gsml:value>
					</gsml:CGI_TermValue>
				</gsml:metamorphicGrade>
			</gsml:MetamorphicDescription>
		</gsml:metamorphicCharacter>
</gsml:LithostratigraphicUnit>
