			<gsml:mineralElement>
				<gsml:Mineral>
					<gsml:purpose>instance</gsml:purpose>
					<gsml:mineralName xlink:href="id_cc06"></gsml:mineralName>
				</gsml:Mineral>
			</gsml:mineralElement>
			<gsml:orientation>
				<gsml:CGI_LinearOrientation>
					<gsml:plunge>
						<gsml:CGI_TermValue>
							<gsml:value codeSpace="urn:cgi:classifierScheme:GSV:Inclination">subhorizontal</gsml:value>
						</gsml:CGI_TermValue>
