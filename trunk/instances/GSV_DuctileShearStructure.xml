<!-- lineation alignment -->
	<gsml:member>
		<gsml:Lineation gml:id="id_lineation01">
			<gsml:observationMethod>
				<gsml:CGI_TermValue>
					<gsml:value codeSpace="urn:cgi:classifierScheme:GSV:ObservationMethod">field mapping</gsml:value>
				</gsml:CGI_TermValue>
			</gsml:observationMethod>
