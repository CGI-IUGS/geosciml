    <observationMethod>
        <CGI_TermValue>
            <value codeSpace="http://www.ga.gov.au/GeologicalVocabs">field mapping</value>
        </CGI_TermValue>
    </observationMethod>
    <purpose>instance</purpose>
    <preferredAge>
        <GeologicEvent>
            <eventAge>
                <CGI_Value xsi:type="CGI_TermValueType">
                    <value codeSpace="http://www.ga.gov.au/ICS_timescale">Silurian</value>
                </CGI_Value>
            </eventAge>
            <eventProcess>
                <CGI_TermValue>
                    <value codeSpace="http://www.ga.gov.au/GeologicalVocabs">deposition</value>
                </CGI_TermValue>
            </eventProcess>
        </GeologicEvent>
    </preferredAge>
    <foliationType>
        <ControlledConcept>
            <preferredName>bedding</preferredName>
            <vocabulary xlink:type="simple" xlink:href="http://www.ga.gov.au/GA_structures_vocabulary"/>
        </ControlledConcept>
    </foliationType>
    <orientation>
        <CGI_PlanarOrientation>
            <determinationMethod>
                <CGI_TermValue>
                    <value codeSpace="http://www.ga.gov.au/GeologicalVocabs">compass and clinometer</value>
                </CGI_TermValue>
            </determinationMethod>
            <azimuth>
                <CGI_Value xsi:type="CGI_NumericValueType">
                    <principalValue uom="degrees">270</principalValue>
                </CGI_Value>
            </azimuth>
            <convention>dip dip direction</convention>
            <dip>  
                <CGI_Value xsi:type="CGI_NumericValueType">
                    <principalValue uom="degrees">40</principalValue>
                </CGI_Value>
            </dip>
            <polarity>upright</polarity>
       </CGI_PlanarOrientation>
    </orientation>
   </Layering>
</member>
    
