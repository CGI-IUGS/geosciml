<?xml version="1.0" encoding="UTF-8"?>
<!--Example mapped feature from BGS data.-->
<GSML xmlns="urn:cgi:xmlns:CGI:GeoSciML:2.0" gml:id="mf.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml" xmlns:sa="http://www.opengis.net/sampling/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
	urn:cgi:xmlns:CGI:GeoSciML:2.0	https://www.seegrid.csiro.au/subversion/GeoSciML/trunk/schema/GeoSciML/geosciml.xsd
">
	<member>
		<MappedFeature>
			<observationMethod>
				<CGI_TermValue>
					<value codeSpace="http://www.bgs.ac.uk/observationMethodVocab">fieldObservation</value>
				</CGI_TermValue>
			</observationMethod>
			<observationMethod>
				<CGI_TermValue>
					<value codeSpace="http://www.bgs.ac.uk/observationMethodVocab">Boreholes</value>
				</CGI_TermValue>
			</observationMethod>
			<positionalAccuracy>
				<CGI_TermValue>
					<value codeSpace="http://www.bgs.ac.uk/positionalAccuracyVocab">unknown</value>
				</CGI_TermValue>
			</positionalAccuracy>
			<samplingFrame>
				<sa:SamplingSurface>
				<!--Need MapHorizon specialisation here-->
					<sa:sampledFeature></sa:sampledFeature>
					<sa:shape></sa:shape>
				</sa:SamplingSurface>
			</samplingFrame>
			<specification>
				<LithostratigraphicUnit>
					<observationMethod>
						<CGI_TermValue>
							<value codeSpace="http://www.bgs.ac.uk/observationMethodVocab">Summary of published description</value>
						</CGI_TermValue>
					</observationMethod>
					<purpose>instance</purpose>
					<preferredAge>
						<GeologicEvent>
							<eventAge>
								<CGI_TermRange>
									<lower>
										<CGI_TermValue>
											<value codeSpace="http://www.bgs.ac.uk/geochronVocab">LOCHKOVIAN</value>
										</CGI_TermValue>
									</lower>
									<upper>
										<CGI_TermValue>
											<value codeSpace="http://www.bgs.ac.uk/geochronVocab">PRAGHIAN</value>
										</CGI_TermValue>
									</upper>
								</CGI_TermRange>
							</eventAge>
						</GeologicEvent>
					</preferredAge>
					<classifier>
						<ControlledConcept>
							<gml:name> codeSpace="#LEXUNIT1">DECO</gml:name>
							<preferredName>DEEP CONGLOMERATE FORMATION</preferredName>
							<vocabulary xlink:href="#LEX" />
						</ControlledConcept>
					</classifier>
			</LithostratigraphicUnit>
			</specification>
			<shape></shape>
		</MappedFeature>
	</member>
	<member>
		<StratigraphicLexicon gml:id="LEX">
			<member>
				<ControlledConcept gml:id="LEXUNIT1">
					<preferredName>DEEP CONGLOMERATE FORMATION</preferredName>
					<prototype>
						<LithostratigraphicUnit>
							<observationMethod>
								<CGI_TermValue>
									<value codeSpace="http://www.bgs.ac.uk/observationMethodVocab">Summary of published description</value>
								</CGI_TermValue>
							</observationMethod>
							<purpose>definingNorm</purpose>
							<preferredAge>
								<GeologicEvent>
									<eventAge>
										<CGI_TermRange>
											<lower>
												<CGI_TermValue>
													<value codeSpace="http://www.bgs.ac.uk/geochronVocab">LOCHKOVIAN</value>
												</CGI_TermValue>
											</lower>
											<upper>
												<CGI_TermValue>
													<value codeSpace="http://www.bgs.ac.uk/geochronVocab">PRAGHIAN</value>
												</CGI_TermValue>
											</upper>
										</CGI_TermRange>
									</eventAge>
								</GeologicEvent>
							</preferredAge>
							<composition>
								<CompositionPart>
									<lithology>
										<ControlledConcept>
											<preferredName>CONGLOMERATE</preferredName>
											<vocabulary xlink:href="http://www.bgs.ac.uk/RCSVocab" />
										</ControlledConcept>
									</lithology>
									<proportion>
										<CGI_NumericValue>
											<principalValue uom="percent">100</principalValue>
										</CGI_NumericValue>
									</proportion>
									<role codeSpace="http://www.bgs.ac.uk/roleVocab">dominantConstituent</role>
								</CompositionPart>
							</composition>
						</LithostratigraphicUnit>
					</prototype>
					<vocabulary xlink:href="#LEX" />
				</ControlledConcept>
			</member>
			<member>
				<ControlledConcept gml:id="LEXUNIT2">
					<preferredName>SCONE SANDSTONE FORMATION</preferredName>
					<prototype>
						<LithostratigraphicUnit>
							<observationMethod>
								<CGI_TermValue>
									<value codeSpace="http://www.bgs.ac.uk/observationMethodVocab">Summary of published description</value>
								</CGI_TermValue>
							</observationMethod>
							<purpose>definingNorm</purpose>
							<preferredAge>
								<GeologicEvent>
									<eventAge>
									<CGI_TermValue>
										<value codeSpace="">http://www.bgs.ac.uk/geochronVocab">EARLY DEVONIAN</value>
									</CGI_TermValue>
									</eventAge>
								</GeologicEvent>
							</preferredAge>
							<composition>
								<CompositionPart>
									<lithology>
										<ControlledConcept>
											<preferredName>SANDSTONE</preferredName>
											<vocabulary xlink:href="http://www.bgs.ac.uk/RCSVocab" />
										</ControlledConcept>
									</lithology>
									<proportion>
										<CGI_NumericValue>
											<principalValue uom="percent">100</principalValue>
										</CGI_NumericValue>
									</proportion>
									<role codeSpace="http://www.bgs.ac.uk/roleVocab">dominantConstituent</role>
								</CompositionPart>
							</composition>
						</LithostratigraphicUnit>
					</prototype>
					<vocabulary xlink:href="#LEX" />
				</ControlledConcept>
			</member>
		</StratigraphicLexicon>
	</member>
</GSML>