				<gsml:CGI_NumericValue>
					<gsml:principalValue uom="m">240</gsml:principalValue>
				</gsml:CGI_NumericValue>
			</mo:depth>
			<mo:length>
				<gsml:CGI_NumericValue>
					<gsml:principalValue uom="m">300</gsml:principalValue>
				</gsml:CGI_NumericValue>
			</mo:length>
			<mo:width>
				<gsml:CGI_NumericRange>
					<gsml:lower>
						<gsml:CGI_NumericValue>
							<gsml:principalValue uom="m">0</gsml:principalValue>
						</gsml:CGI_NumericValue>
					</gsml:lower>
					<gsml:upper>
						<gsml:CGI_NumericValue>
							<gsml:principalValue uom="m">37</gsml:principalValue>
						</gsml:CGI_NumericValue>
					</gsml:upper>
				</gsml:CGI_NumericRange>
			</mo:width>
		</mo:EarthResourceDimension>
	</mo:dimension>
	<!-- form comes from GSV VicMine t_md_site: deposit_sub_type_cd -->
	<mo:form>
		<gsml:CGI_TermValue>
			<gsml:value codeSpace="http://www.gsv.vic.gov.au/GeologicalVocabulary">stratiform</gsml:value>
		</gsml:CGI_TermValue>
