				<gsml:CGI_NumericValue>
					<gsml:principalValue uom="degree">45</gsml:principalValue>
				</gsml:CGI_NumericValue>
			</gsml:dip>
			<gsml:polarity>unknown</gsml:polarity>
		</gsml:CGI_PlanarOrientation>
	</planarOrientation>
	<!-- GSV VicMine has no shape 
