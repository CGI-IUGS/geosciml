	<mo:composition>
		<!-- GSV VicMine values that come from t_md_lithology - will have ER_MaterialRoleCode = "host rock"; values that come from t_md_alteration - will have ER_MaterialRoleCode = "alteration product" -->
		<mo:EarthResourceMaterial>
			<mo:earthResourceMaterialRole>host rock</mo:earthResourceMaterialRole>
			<mo:material>
			<!-- schema expects EarthMaterial here not Rock. This is a problem with the GeoSciML schema -->
				<gsml:EarthMaterial>
					<!-- where GSV VicMine lithology contains "unconsolidated" GSV to use class UnconsolidatedMaterial and consolidationDegree = "unconsolidated"; 
					for all others use class Rock and consolidationDegree = "consolidated" -->
				<gsml:consolidationDegree>consolidated</gsml:consolidationDegree>
					<gsml:lithology>rhyolite</gsml:lithology>
				</gsml:EarthMaterial> 
			</mo:material>
		</mo:EarthResourceMaterial>
	</mo:composition>
	<!-- For GSV VicMine MineralOccurrenceType is data is to come from Site Type. 
	For Site Type = Production Centre, use MineralOccurrenceType = "mineral deposit"; 
	for Extractive Ind. Interest Area, use MineralOccurrenceType = "district";  
	for Occurrence, use "occurrence"; for a Resource Area, use "field". 
	For Site Type = Production Centre and there is a Mine Size, use "ore deposit".  -->
	<mo:type>ore deposit</mo:type>
</MineralOccurrence>
