						<gsml:CGI_NumericValue>
							<gsml:principalValue uom="oz">99409</gsml:principalValue>
						</gsml:CGI_NumericValue>
					</mo:production>
					<mo:productName>
						<gsml:CGI_TermValue>
							<gsml:value codeSpace="http://www.gsv.vic.gov.au/GeologicalVocabulary">silver</gsml:value>
						</gsml:CGI_TermValue>
					</mo:productName>
					<mo:source xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="http://www.gsv.vic.gov.au/GEDISReference/ID28198"/>
