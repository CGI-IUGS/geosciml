						<gsml:CGI_NumericValue>
							<gsml:principalValue uom="g/t">21.073</gsml:principalValue>
						</gsml:CGI_NumericValue>
					</mo:grade>
					<!-- In GSV VicMine product is in t_md_production: commodity_cd -->
					<!-- In GSV VicMine production is in t_md_production: recorded_product_amount; product_units  -->
