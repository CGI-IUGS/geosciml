	<!--   Removed from header
	xmlns:gml="http://www.opengis.net/gml/3.1"
	 -->
	<!-- GSV VicMine "Mine Name - Primary" from [t_md_site; mine_name; mine_name_type_cd; primary_flg] should be used here -->
	<gml:name>Wilga</gml:name>
	<!-- GSV VicMine Site ID from [t_md_site; site_id] should be used for GML:name -->
	<gml:name>372429</gml:name>
	<!-- GSV VicMine doesn't contain any observation method. Observation method is mandatory defaulting to "unknown" -->
	<gsml:observationMethod>
		<gsml:CGI_TermValue>
			<gsml:value codeSpace="http://www.gsv.vic.gov.au/GeologicalVocabulary">unknown</gsml:value>
		</gsml:CGI_TermValue>
	</gsml:observationMethod>
	<!-- GSV VicMine doesn't specify purpose. All entries that are from the t_md_deposit_style have purpose "IdentifyingNorm", those which are not from the style table have purpose"Instance" -->
	<gsml:purpose>instance</gsml:purpose>
	<gsml:occurence>
		<gsml:MappedFeature gml:id="id_b1">
			<!-- GSV VicMine Site Feature, Feature Type = gml:name. Description = gml:description. Location Type = another gml:name -->
			<gml:description></gml:description>
			<gml:name>Centre of resource</gml:name>
			<gsml:positionalAccuracy>
				<!-- GSV VicMine stores positional accuracy in [t_site; locn_acc] -->
				<gsml:CGI_NumericValue>
					<gsml:principalValue uom="m">100</gsml:principalValue>
				</gsml:CGI_NumericValue>
			</gsml:positionalAccuracy>
			<gsml:observationMethod>
				<gsml:CGI_TermValue>
					<gsml:value codeSpace="http://www.gsv.vic.gov.au/GeologicalVocabulary">Digitised from map</gsml:value>
				</gsml:CGI_TermValue>
			</gsml:observationMethod>
			<!-- samplingFrame specifies the shape that contains the whole feature -->
			<gsml:samplingFrame xlink:href="id_b1"/>
			<gsml:shape>
				<!-- EPSG number for MGA94 Zone 55 is inserted here; requires coding to depend on MGA zone; in GSV VicMine this data comes from [t_site; amg_zone] -->
				<gml:Point gml:id="p_00001" srsName="EPSG:28355">
					<gml:pos>578963 5904364</gml:pos>
				</gml:Point>
			</gsml:shape>
			<!-- gsml:specification specifies which GeologicFeature the MappedFeature refers to.-->
			<gsml:specification xlink:href="id_mo1"/>
		</gsml:MappedFeature>
	</gsml:occurence> 	
	<gsml:preferredAge>
		<gsml:GeologicEvent>
			<!-- GSV VicMine has no capacity to store age, defaults to unknown -->
			<gsml:eventAge>
				<gsml:CGI_TermValue>
					<gsml:value codeSpace="http://www.gsv.vic.gov.au/GeologicalTimeScale">unspecified</gsml:value>
				</gsml:CGI_TermValue>
			</gsml:eventAge>
			<!-- GSV VicMine to use [t_md_deposit_style; deposit_style_cd] vocabulary for eventEnvironment -->
			<gsml:eventProcess>
				<gsml:CGI_TermValue>
					<gsml:value codeSpace="http://www.gsv.vic.gov.au/GeologicalVocabulary">unknown</gsml:value>
				</gsml:CGI_TermValue>
