                                                                    owl:onProperty   gsmlb:featureItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:geometryItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:mappedItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
