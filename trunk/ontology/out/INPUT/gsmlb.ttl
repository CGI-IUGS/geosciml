                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:featureItem
                                                                  ]
                                                                  [ a                   owl:Restriction ;
                                                                    owl:minCardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty      gsmlb:geometryItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:mappedItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:relationItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:samplingFeatureItem
                                                                  ]
                                                                )
                                          ]
                                          [ a                   owl:Class ;
                                            owl:intersectionOf  ( [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:earthMaterialItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:featureItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:geometryItem
                                                                  ]
                                                                  [ a                   owl:Restriction ;
                                                                    owl:minCardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty      gsmlb:mappedItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:relationItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:samplingFeatureItem
