                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:mappedItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:relationItem
                                                                  ]
                                                                  [ a                owl:Restriction ;
                                                                    owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                    owl:onProperty   gsmlb:samplingFeatureItem
                                                                  ]
