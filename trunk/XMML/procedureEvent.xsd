	<!-- ====================================================================== -->
	<!-- bring in other schemas -->
	<import namespace="http://www.opengis.net/gml" schemaLocation="./gml4procedureEvent.xsd"/>
	<import namespace="http://www.opengis.net/2005/om" schemaLocation="./om4procedureEvent.xsd"/>
	<!-- include schemaLocation="./event.xsd"/ -->
	<!-- =========================================================== -->
	<!-- ====================================================================== -->
	<element name="ProcedureEvent" type="xmml:ProcedureEventType" substitutionGroup="om:Event">
		<annotation>
			<documentation>Description of an event involving a procedure.</documentation>
		</annotation>
	</element>
	<!-- ...................... -->
	<complexType name="ProcedureEventType">
		<annotation>
			<documentation>Description of an event involving a procedure.</documentation>
		</annotation>
		<complexContent>
			<extension base="om:EventType">
				<sequence>
					<element name="procedureUsed" type="om:ProcedurePropertyType"/>
					<element name="input" type="gml:StringOrRefType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Reference to input (if not implied by context)</documentation>
						</annotation>
					</element>
					<element name="product" type="gml:StringOrRefType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Reference to result (if not implied by context)</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<!-- ====================================================================== -->
	<!-- === Global property elements involving procedure events === -->
	<!-- ====================================================================== -->
	<complexType name="ProcedureEventPropertyType">
		<sequence minOccurs="0">
			<element ref="xmml:ProcedureEvent"/>
		</sequence>
		<attributeGroup ref="gml:AssociationAttributeGroup"/>
	</complexType>
