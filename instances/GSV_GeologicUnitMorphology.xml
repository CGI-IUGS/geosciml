<?xml version="1.0" encoding="UTF-8"?>
<GSML xmlns="urn:cgi:xmlns:CGI:GeoSciML:2.0" xmlns:gml="http://www.opengis.net/gml" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:cgi:xmlns:CGI:GeoSciML:2.0 http://www.geosciml.org/geosciml/2.0/xsd/geosciml.xsd">
<!-- Revised by SMR 2010-01-31 so validates against normative v2.0 schema, and can xslt into 
SKOS version for work on Geologic Unit Morphology vocabulary  -->

	<!-- Concepts  -->
	<member>
		<GeologicVocabulary gml:id="gsv1.0">
			<gml:description>A description of the form of a Geological Unit or Geological Structure.</gml:description>
			<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifierScheme:GSV:2005:morphology</gml:name>
			<gml:name codeSpace="urn:ietf:rfc:2141">Morphology</gml:name>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-1">
					<gml:description>the body of igneous rock formed by the process of emplacement of magma in pre-existing rock</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:intrusion</gml:name>
					<name xml:lang="en">intrusion</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-2">
					<gml:description>a deep-seated igneous intrusion</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:pluton</gml:name>
					<name xml:lang="en">pluton</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-3">
					<gml:description>concordant, flat-based, convex-up intrusion; shaped like a mushroom</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:laccolith</gml:name>
					<name xml:lang="en">laccolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-4">
					<gml:description>concordant, lenticular, emplaced along a fold axis</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:phacolith</gml:name>
					<name xml:lang="en">phacolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-5">
					<gml:description>phacolith with a vertical axis; indicates multiple deformations</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:harpolith</gml:name>
					<name xml:lang="en">harpolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-6">
					<gml:description>pluton so irregular as to defy description</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:chonolith</gml:name>
					<name xml:lang="en">chonolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-7">
					<gml:description>concordant, teardrop-shaped pluton in cross section</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:ductolith</gml:name>
					<name xml:lang="en">ductolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-8">
					<gml:description>wedge-like pluton, partly concordant, partly discordant</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:sphenolith</gml:name>
					<name xml:lang="en">sphenolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-9">
					<gml:description>obscurely cactus-shaped pluton -  A cactolith is a quasi-horizontal chonolith composed of anastamosing ductoliths whose distal ends curl like a harpolith, thin like a sphenolith, or bulge discordantly like an akmolith or ethmolith.
C.B. Hunt, Geology and geography of the Henry Mountains region, Utah, 1953</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:cactolith</gml:name>
					<name xml:lang="en">cactolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-10">
					<gml:description>intrusion along a decollement zone, with or without tonguelike extensions into the surrounding rock</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:akmolith</gml:name>
					<name xml:lang="en">akmolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-11">
					<gml:description>discordant, funnel-shaped pluton that tapers downward</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:ethmolith</gml:name>
					<name xml:lang="en">ethmolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-12">
					<gml:description>a large, concordant, typically layered igneous intrusion whose floor is convex-down and whose roof may be convex-down or flat; spoon-like shape</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:lopolith</gml:name>
					<name xml:lang="en">lopolith</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-13">
					<gml:description>a concentration of dykes</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:dykeSwarm</gml:name>
					<gml:name codeSpace="urn:ietf:rfc:2141">dike swarm</gml:name>
					<name xml:lang="en">dyke swarm</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-14">
					<gml:description>discordant, vertical or steeply dipping tabular igneous intrusion; produced when magma is injected along fractures in host rock</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:dyke</gml:name>
					<gml:name codeSpace="urn:ietf:rfc:2141">dike</gml:name>
					<name xml:lang="en">dyke</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-15">
					<gml:description>an association of ring dykes and cone sheets</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:ringComplex</gml:name>
					<name xml:lang="en">ring complex</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-16">
					<gml:description>an arcuate or subcircular dyke, generally associated with a volcanic centre</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:ringDyke</gml:name>
					<name xml:lang="en">ring dyke</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-17">
					<gml:description>an arcuate dyke that dips 30-45 degrees towards the centre of the arc; occur in concentric sets, presumably converging at a magmatic centre</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:coneSheet</gml:name>
					<name xml:lang="en">cone sheet</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-18">
					<gml:description>a volcanic conduit having the form of a crack or fissure</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:igneousFissure</gml:name>
					<name xml:lang="en">igneous fissure</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-19">
					<gml:description>a vertical conduit through the Earth's crust below a volcano, through which magmatic materials have passed; represents the upper part of the conduit connecting the volcanic vent with the underlying magma source</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:pipe</gml:name>
					<name xml:lang="en">pipe</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-20">
					<gml:description>a tabular igneous intrusion that parallels the bedding or foliation of the country rock</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:sill</gml:name>
					<name xml:lang="en">sill</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-21">
					<gml:description>A small, roughly equidimensional, discordant magma body with exposed surface area of less than 2km square</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:stock</gml:name>
					<name xml:lang="en">stock</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-22">
					<gml:description>tabular intrusion, concordant or slightly discordant</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:sheet</gml:name>
					<name xml:lang="en">sheet</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-23">
					<gml:description>a large upward projection of the roof of an igneous intrusion into the country rock above</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:cupola</gml:name>
					<name xml:lang="en">cupola</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-24">
					<gml:description>an offshoot from a larger intrusive body</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:apophysis</gml:name>
					<name xml:lang="en">apophysis</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-25">
					<gml:description>a downward projection of country rock into an igneous intrusion</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:roofPendant</gml:name>
					<name xml:lang="en">roof pendant</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-26">
					<gml:description>an upward projection of country rock into an igneous intrusion</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:floorAscendant</gml:name>
					<name xml:lang="en">floor ascendant</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-27">
					<gml:description>a vertically extensive body of country rock separating two intrusions or phases of an intrusion</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:screen</gml:name>
					<name xml:lang="en">screen</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-28">
					<gml:description>tabular body formed by lateral surficial outpouring of molten lava from a vent or fissure</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:lavaFlow</gml:name>
					<name xml:lang="en">lava flow</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-29">
					<gml:description>column of rock formed by the intersection of columnar joint sets during cooling of an igneous rock</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:coolingColumn</gml:name>
					<name xml:lang="en">cooling column</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-30">
					<gml:description>a low area in the Earth's crust in which sediments have accumulated</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:basin</gml:name>
					<name xml:lang="en">basin</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-31">
					<gml:description>elongate trough or basin bounded on both sides by normal faults that dip toward the interior of the trough</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:graben</gml:name>
					<name xml:lang="en">graben</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-32">
					<gml:description>elongate trough or basin bounded on one side by a normal fault</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:halfGraben</gml:name>
					<name xml:lang="en">half-graben</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-33">
					<gml:description>a long, narrow continental trough bounded by normal faults (a graben of regional extent); marks a zone along which the entire lithosphere has ruptured under extension</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:rift</gml:name>
					<name xml:lang="en">rift</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-34">
					<gml:description>topographic depression centred over an extensional bend or extensional step-over in strike slip fault</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:pullApartBasin</gml:name>
					<name xml:lang="en">pull-apart basin</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-35">
					<gml:description>a broad, shallow structural basin with gently sloping sides</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:sag</gml:name>
					<name xml:lang="en">sag</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-36">
					<gml:description>elongate block bounded on both sides by normal faults that dip away from the interior of the horst</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:horst</gml:name>
					<name xml:lang="en">horst</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-37">
					<gml:description>an epigenetic mineral filling of a fault or fracture in a host rock, in tabular or sheetlike form, often with associated host rock replacement</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:vein</gml:name>
					<name xml:lang="en">vein</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-38">
					<gml:description>layer of sediment or sedimentary rock bounded above and below by more or less well-defined bedding surfaces</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:bed</gml:name>
					<name xml:lang="en">bed</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-39">
					<gml:description>bounded by converging surfaces, at least one of which is curved, thick in the middle and thinning towards the edges</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:lens</gml:name>
					<name xml:lang="en">lens</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-40">
					<gml:description>sedimentary body in the shape of a wedge</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:wedge</gml:name>
					<gml:name codeSpace="urn:ietf:rfc:2141">clastic wedge</gml:name>
					<name xml:lang="en">wedge</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-41">
					<gml:description>elongate lenticular or rodlike shaped body</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:pod</gml:name>
					<name xml:lang="en">pod</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-42">
					<gml:description>a gently sloping, fan-shaped mass forming a section of a very low cone</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:fan</gml:name>
					<name xml:lang="en">fan</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:GSV:2005:morphology"/>
					<prototype xlink:href="urn:x-ogc:def:nil:OGC:unknown"/>
				</ControlledConcept>
			</gml:definitionMember>
			<gml:definitionMember>
				<ControlledConcept gml:id="Morphology-43">
					<gml:description>an uplift or anticlinal structures in which the rocks dip gently away in all directions</gml:description>
					<gml:name codeSpace="urn:ietf:rfc:2141">urn:cgi:classifier:GSV:morphology:2005:morphology:dome</gml:name>
					<name xml:lang="en">dome</name>
