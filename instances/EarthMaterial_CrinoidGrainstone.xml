<GSML xmlns="urn:cgi:xmlns:CGI:GeoSciML:2.0" xmlns:gml="http://www.opengis.net/gml" xmlns:om="http://www.opengis.net/om" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:cgi:xmlns:CGI:GeoSciML:2.0
http://www.geosciml.org/geosciml/2.0/xsd/geosciml.xsd">
	<member>
		<RockMaterial>
			<!-- Description of gray bioclastic limestone consisting of 90% crinoid columnals, and 10% argillaceous micrite. This instance document is an example of usgae of ConstituentType, and ParticleGeometryDescription. Some of the controlledconcept,vocabulary, and codespace usage needs to be cleaned up, but that's not the point here
SMR original 15 Jan 2008
update for RC3 4 Sept 2008-->
			<color>
				<CGI_TermValue>
					<value codeSpace="MyColorVocabulary">light gray</value>
				</CGI_TermValue>
			</color>
			<purpose>instance</purpose>
			<compositionCategory>
				<CGI_TermValue>
					<value codeSpace="MyCompositionCategories">Carbonate</value>
				</CGI_TermValue>
			</compositionCategory>
			<geneticCategory>
				<CGI_TermValue>
					<value codeSpace="MyGenesisCategories">Sedimentary</value>
				</CGI_TermValue>
			</geneticCategory>
			<geneticCategory>
				<CGI_TermValue>
					<value codeSpace="MyGenesisCategories">Marine</value>
				</CGI_TermValue>
			</geneticCategory>
			<part>
				<ConstituentPart>
					<proportion>
						<CGI_NumericValue>
							<principalValue uom="percent">90</principalValue>
						</CGI_NumericValue>
					</proportion>
					<role codeSpace="MyConstituentRoleTerms">Framework particle</role>
					<material>
						<Mineral>
							<purpose>typicalNorm</purpose>
							<mineralName>
								<ControlledConcept gml:id="testCalcite">
									<gml:name>Calcite</gml:name>
									<name xml:lang="en">Calcite</name>
									<vocabulary xlink:href="urn:test:minerals"/>
								</ControlledConcept>
							</mineralName>
						</Mineral>
					</material>
	<!-- particleGeometry removed in final v 2.0, so can't describe geometry of individual particle types...
				<particleGeometry>
						<ParticleGeometryDescription>
							<particleType>
								<CGI_TermValue>
									<value codeSpace="">Crinoid columnal</value>
								</CGI_TermValue>
							</particleType>
							<size>
								<CGI_NumericRange>
									<lower>
										<CGI_NumericValue>
											<principalValue uom="mm">2</principalValue>
										</CGI_NumericValue>
									</lower>
									<upper>
										<CGI_NumericValue>
											<principalValue uom="mm">4</principalValue>
										</CGI_NumericValue>
									</upper>
								</CGI_NumericRange>
							</size>
						</ParticleGeometryDescription>
					</particleGeometry> -->
				</ConstituentPart>
			</part>
			<part>
				<ConstituentPart>
					<proportion>
						<CGI_NumericValue>
							<principalValue uom="percent">5
							</principalValue>
						</CGI_NumericValue>
					</proportion>
					<role codeSpace="MyConstituentRoleVocabulary">matrix</role>
					<material>
						<Mineral>
							<purpose>typicalNorm</purpose>
							<mineralName xlink:href="#testCalcite"/>
								
						</Mineral>
					</material>
	<!-- particle Geometry removed in final 2.0 model
				<particleGeometry>
						<ParticleGeometryDescription>
							<size>
								<CGI_NumericRange>
									<lower>
										<CGI_NumericValue>
											<principalValue uom="micron">3</principalValue>
										</CGI_NumericValue>
									</lower>
									<upper>
										<CGI_NumericValue>
											<principalValue uom="micron">10</principalValue>
										</CGI_NumericValue>
									</upper>
								</CGI_NumericRange>
							</size>
						</ParticleGeometryDescription>
					</particleGeometry> -->
				</ConstituentPart>
			</part>
			<part>
				<ConstituentPart>
					<proportion>
						<CGI_NumericValue>
							<principalValue uom="percent">5
							</principalValue>
						</CGI_NumericValue>
					</proportion>
					<role codeSpace="MyConstituentRoleVocabulary">matrix</role>
					<material>
						<Mineral>
						<purpose>instance</purpose>
							<mineralName>
								<ControlledConcept gml:id="testClay">
									<gml:name>urn:test:clay</gml:name>
									<name xml:lang="en">clay</name>
									<vocabulary xlink:href="urn:test:minerals"/>
								</ControlledConcept>
							</mineralName>
						</Mineral>
					</material>
		<!--			<particleGeometry>
						<ParticleGeometryDescription>
							<size>
								<CGI_NumericValue>
									<principalValue uom="micron">2</principalValue>
								</CGI_NumericValue>
							</size>
						</ParticleGeometryDescription>
					</particleGeometry> -->
				</ConstituentPart>
			</part>
			<consolidationDegree>
				<CGI_TermValue>
					<value codeSpace="MyConsolidationVocabulary">Well Consolidated</value>
				</CGI_TermValue>
			</consolidationDegree>
			<lithology>
				<ControlledConcept gml:id="test12345">
					<gml:name codeSpace="http://www.cgi-iugs.org/uri">urn:cgi:classifierScheme:CGI:SimpleLithology:200811:grainstone</gml:name>
					<name xml:lang="en">grainstone</name>
					<vocabulary xlink:href="urn:cgi:classifierScheme:CGI:SimpleLithology:200811"/>
				</ControlledConcept>
			</lithology>
		</RockMaterial>
	</member>
</GSML>
