<schema xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:gml="http://www.opengis.net/gml"
	xmlns:gsml="urn:cgi:xmlns:CGI:GeoSciML:2.0"
	xmlns:sa="http://www.opengis.net/sampling/1.0"
	targetNamespace="urn:cgi:xmlns:CGI:GeoSciML:2.0">

	<annotation>
		<documentation>
			Profile wrapper for MappedFeature with shapefile backend
		</documentation>
	</annotation>

	<include
		schemaLocation="http://www.geosciml.org/schemas/geosciml/2.0/geologicFeature.xsd" />

	<complexType name="MappedFeatureShapefile">
		<complexContent>
			<extension base="gsml:MappedFeatureType" />
		</complexContent>
	</complexType>

</schema>