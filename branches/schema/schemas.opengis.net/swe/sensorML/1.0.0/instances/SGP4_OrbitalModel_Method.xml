<?xml version="1.0" encoding="UTF-8"?>
<sml:ProcessMethod xmlns:sml="http://www.opengis.net/sensorML/1.0" xmlns:swe="http://www.opengis.net/swe/1.0" xmlns:gml="http://www.opengis.net/gml" xmlns:ism="urn:us:gov:ic:ism:v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xlink="http://www.w3.org/1999/xlink" xsi:schemaLocation="http://www.opengis.net/sensorML/1.0 ../base/process.xsd">
	<!-- Description -->
	<gml:description>
      Defines the SGP4 Keplerian Orbit Propagator for use with NORAD Elements (i.e. "Two-Line Elements")
   </gml:description>
	<!-- Keywords -->
	<sml:keywords>
		<sml:KeywordList>
			<sml:keyword>satellite</sml:keyword>
			<sml:keyword>orbit</sml:keyword>
			<sml:keyword>NORAD</sml:keyword>
		</sml:KeywordList>
	</sml:keywords>
	<!-- Identification and Classification -->
	<sml:identification>
		<sml:IdentifierList>
			<sml:identifier>
				<sml:Term definition="uniqueID">
					<sml:value>SGP4</sml:value>
				</sml:Term>
			</sml:identifier>
		</sml:IdentifierList>
	</sml:identification>
	<sml:classification>
		<sml:ClassifierList>
			<sml:classifier>
				<sml:Term definition="application">
					<sml:value>position</sml:value>
				</sml:Term>
			</sml:classifier>
		</sml:ClassifierList>
	</sml:classification>
	<!-- Security Constraints - unclassified -->
	<sml:securityConstraint>
		<sml:Security ism:classification="U"/>
	</sml:securityConstraint>
	<!-- Contact -->
	<sml:contact xlink:role="author">
		<sml:ResponsibleParty>
			<sml:individualName>Mike Botts</sml:individualName>
			<sml:organizationName>University of Alabama in Huntsville</sml:organizationName>
			<sml:positionName>Principal Research Scientist</sml:positionName>
			<sml:contactInfo>
				<sml:phone>
					<sml:voice>+01-256-961-7760</sml:voice>
				</sml:phone>
				<sml:address>
					<sml:electronicMailAddress>mike.botts@uah.edu</sml:electronicMailAddress>
				</sml:address>
			</sml:contactInfo>
		</sml:ResponsibleParty>
	</sml:contact>
	<!-- Documentation -->
	<sml:documentation xlink:role="formula">
		<sml:Document>
			<gml:description>
         	SpaceTrack Report No. 3 providing Models for Propagation of NORAD Element Sets
         </gml:description>
			<sml:onlineResource xlink:href="http://vast.uah.edu/SensorML/process/doc/spacetrk_NORAD_propagators.pdf"/>
		</sml:Document>
	</sml:documentation>
	<!-- I/O Interface rules -->
	<sml:rules>
		<sml:RulesDefinition>
			<gml:description>Input is julian time, output is position and velocity in Earth Centered Inertial frame (WGS84), parameters are the Orbital Elements
         </gml:description>
			<sml:schematron xlink:href="http://vast.uah.edu/SensorML/process/sch/SGP4_OrbitalModel_Interface.sch"/>
		</sml:RulesDefinition>
	</sml:rules>
	<!-- Algorithm -->
	<sml:algorithm>
		<sml:AlgorithmDefinition>
			<gml:description>
            Implements the SGP4 Propagator for NORAD elements
         </gml:description>
			<sml:mathML xlink:href="http://vast.uah.edu/SensorML/process/math/SGP4_OrbitalModel_Algorithm.xml"/>
		</sml:AlgorithmDefinition>
	</sml:algorithm>
	<!-- Implementations -->
	<sml:implementation>
		<sml:ImplementationCode language="java" framework="uah-dpf" version="1.0">
			<sml:sourceRef xlink:href="http://vast.uah.edu/SensorML/process/src/SGP4_OrbitalModel_Process.java"/>
			<sml:binaryRef xlink:href="http://vast.uah.edu/SensorML/process/bin/SGP4_OrbitalModel_Process.class"/>
		</sml:ImplementationCode>
	</sml:implementation>
</sml:ProcessMethod>
