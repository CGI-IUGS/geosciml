<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:entity:xmlns:xml:catalog file:///C:/dev/SVN_xmml/oasis/entity/catalog.xsd">

<!-- ===================================================== -->
	<!-- The first three elements provide local copies for three specific XML Schema documents -->

	<!-- The XML Namespace schema -->
	<uri name="http://www.w3.org/2001/xml.xsd" 
	uri="file:///C:/dev/SVN_xmml/w3c/xsd/xml.xsd"/>

	<!-- The OGC-supplied XML Schema for XLINKS -->
	<uri name="http://schemas.opengis.net/xlink/1.0.0/xlinks.xsd" 
	uri="file:///C:/dev/SVN_xmml/gml/trunk/xlink/1.0.0/xlinks.xsd"/>

	<!-- The OGC-supplied XML Schema for XLINKS, in an earlier location -->
	<uri name="http://schemas.opengis.net/gml/3.1.0/xlink/xlinks.xsd" 
	uri="file:///C:/dev/SVN_xmml/gml/trunk/gml/3.1.0/xlink/xlinks.xsd"/>

<!-- ===================================================== -->
	<!-- The following elements provide local paths for a set of resources as an alternative to web paths -->

	<!-- Exact copy of  GML 2.1.2  -->
	<rewriteURI uriIdStartString="http://schemas.opengis.net/gml/2.1.2" 
	rewritePrefix="file:///C:/dev/SVN_xmml/gml/trunk/gml/2.1.2"/>

	<!-- GML 3.1.1 profile
	 with deprecated components removed, 
	 and with one member from a choice group selected for cases where the choice represents only a syntax preference -->
	<rewriteURI uriIdStartString="http://schemas.opengis.net/gml/3.1.1" 
	rewritePrefix="file:///C:/dev/SVN_xmml/gml/branches/gml_s/3.1.1"/>

	<!-- GML 3.2.0 profile
	 with deprecated components removed, 
	 and with one member from a choice group selected for cases where the choice represents only a syntax preference -->
	<rewriteURI uriIdStartString="http://testschemas.opengis.net/gml/3.2.0" 
	rewritePrefix="file:///C:/dev/SVN_xmml/gml/branches/gml_s/3.2.0"/>

	<!-- redirect local relpaths to the GML profiles -->
	<rewriteURI uriIdStartString="../../../../gml/trunk/gml" 
	rewritePrefix="file://C:/dev/SVN_xmml/gml/branches/gml_s"/>

	<!-- Exact copy of  Discrete Coverages schema, v 0.2.0  -->
	<rewriteURI uriIdStartString="http://schemas.opengis.net/cv/0.2.0" 
	rewritePrefix="file://C:/dev/SVN_xmml/swe/trunk/cv/0.2.0"/>

	<!-- Exact copy of  SWE Common schema, v 1.0.0  -->
	<rewriteURI uriIdStartString="http://schemas.opengis.net/swe/1.0.0" 
	rewritePrefix="file://C:/dev/SVN_xmml/swe/trunk/sweCommon/1.0.0"/>

	<!-- Exact copy of  SensorML schema, v 1.0.0  -->
	<rewriteURI uriIdStartString="http://schemas.opengis.net/sensorML/1.0.0" 
	rewritePrefix="file://C:/dev/SVN_xmml/swe/trunk/sensorML/1.0.0"/>

	<!-- Exact copy of  Observations and Measurements schema, v 1.0.0  -->
	<rewriteURI uriIdStartString="http://schemas.opengis.net/om/1.0.0" 
	rewritePrefix="file://C:/dev/SVN_xmml/swe/trunk/om/1.0.0"/>

	<!-- Exact copy of  Sampling Features schema, v 1.0.0  -->
	<rewriteURI uriIdStartString="http://schemas.opengis.net/sampling/1.0.0" 
	rewritePrefix="file://C:/dev/SVN_xmml/swe/trunk/sampling/1.0.0"/>

<!-- ===================================================== -->
</catalog>