							<swe:uom xlink:href="http://www.opengis.net/def/uom/UCUM/0/m"
								xlink:title="m"/>
							<swe:value>60</swe:value>
						</swe:Quantity>
					</cgu:upper>
				</cgu:CGI_NumericRange>
			</gsml:span>
			<gsml:symmetry>
				<cgu:CGI_Term>
					<cgu:qualifier xsi:nil="true" nilReason="missing"/>
					<cgu:value xlink:href="empty/URI" xlink:title="symmetric"/>  <!-- TODO correct typo in assymmetric - see notes for this property -->
					<!-- there is no existing vocabulary for this property -->
				</cgu:CGI_Term>
			</gsml:symmetry>
