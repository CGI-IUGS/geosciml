            </sa:lineage>
            
<!--    ========================================    -->
<!--    Example of a major element oxide analysis                    -->
<!--    ========================================     -->    
            <sa:relatedObservation> 
                <om:OM_Observation gml:id="GAGeochemAnalysis_1526753_SiO2">
                    <gml:description>SiO2 whole rock geochemcial analysis</gml:description>
                    <om:phenomenonTime>
                        <gml:TimeInstant gml:id="GAGeochemAnalysisTime_1526753">
                            <gml:timePosition>1997-01-21T11:12:00</gml:timePosition>
                        </gml:TimeInstant>
                    </om:phenomenonTime>
                    <om:resultTime xlink:href="#GAGeochemAnalysisTime_1526753"/>
                    <om:procedure>
                        <glab:AnalyticalProcess gml:id="GAGeochemAnalysisMethod_XRF_1526753">
                            <glab:method>   <!-- Mandatory -->
                                <glab:AnalyticalMethod gml:id="GAMethods_XRF">
                                    <glab:methodName xlink:href="http://www.ga.gov.au/classifier/analyticalMethod/XRF" xlink:title="x-ray fluorescence mass spectrometry"/>
                                    <glab:citation xlink:href="http://www.ga.gov.au/minerals/disciplines/geochemistry.html"/>
                                </glab:AnalyticalMethod>
                            </glab:method>
                            <glab:acquisition xlink:href="TB4_GA_AnalyticalSession_rc2.xml#GAAnalyticalSession_19970121_01"/>      
<!-- *** I have used a xlink:href above to an external document describing analytical session parameters (eg, machine and operator details) and standard reference materials used. ***  
              This element may be nilled for specimen analyses for which we don't have analytical metadata.   -->
                            <glab:computation>   <!-- Use this link to LI_ProcessStep if there are data reduction or computation processes that need to be documented. -->
                                <gmd:LI_ProcessStep>
                                    <gmd:description>
                                        <gco:CharacterString>Various fudge factors, matrix effects, and other black magic were applied to the raw data.</gco:CharacterString>
                                    </gmd:description>
                                </gmd:LI_ProcessStep>
                            </glab:computation>
                        </glab:AnalyticalProcess>
                    </om:procedure>
                    <om:observedProperty xlink:href="http://sweet.jpl.nasa.gov/2.0/chemConcentration.owl#Composition" xlink:title="chemical composition"/>
                    <om:featureOfInterest xlink:href="#GASampleNo_1526753"/>
                    <om:result>
                        <swe:Quantity gml:id="GAGeochemAnalysis_1526753_SiO2_Result" definition="SiO2 concentration">
                            <swe:uom code="%25" xlink:href="http://www.opengis.net/def/uom/UCUM/0/%25" xlink:title="percent"/>
                            <swe:quality>     <!-- Analytical Error -->
                                <swe:Quantity gml:id="GAGeochemAnalysis_1526753_SiO2_Error" definition="SiO2 analytical error">
                                    <swe:uom code="%25" xlink:href="http://www.opengis.net/def/uom/UCUM/0/%25" xlink:title="percent"/>
                                    <swe:value>0.1</swe:value>
                                </swe:Quantity>
                            </swe:quality>
                            <swe:value>76.4</swe:value>
                        </swe:Quantity>
                    </om:result>
                </om:OM_Observation> 
            </sa:relatedObservation>
            <sa:relatedObservation> 
                <om:OM_Observation gml:id="GAGeochemAnalysis_1526753_Al2O3">
                    <gml:description>Al2O3 whole rock geochemcial analysis</gml:description>
                    <om:phenomenonTime xlink:href="#GAGeochemAnalysisTime_1526753"/>
                    <om:resultTime xlink:href="#GAGeochemAnalysisTime_1526753"/>
                    <om:procedure xlink:href="#GAGeochemAnalysisMethod_XRF_1526753"/>
