                                    <swe:uom code="%25" xlink:href="http://www.opengis.net/def/uom/UCUM/0/%25" xlink:title="percent"/>
                                    <swe:value>0.1</swe:value>
                                </swe:Quantity>
                            </swe:quality>
                            <swe:value>14.2</swe:value>
                        </swe:Quantity>
                    </om:result>
                </om:OM_Observation> 
            </sa:relatedObservation>
            
<!--    ========================================    -->
<!--    Example of a trace element analysis                                -->
<!--    ========================================     -->    
            <sa:relatedObservation> 
                <om:OM_Observation gml:id="GAGeochemAnalysis_1526753_Ni">   
                    <om:phenomenonTime xlink:href="GAGeochemAnalysisTime_1526753"/>
                    <om:resultTime xlink:href="GAGeochemAnalysisTime_1526753"/>
                    <om:procedure xlink:href="GAGeochemAnalysisMethod_1526753"/>
                    <om:observedProperty xlink:href="http://sweet.jpl.nasa.gov/2.0/chemConcentration.owl#Composition" xlink:title="chemical composition"/>
                    <om:featureOfInterest xlink:href="#GASampleNo_1526753"/>
                    <om:result>
                        <swe:Quantity gml:id="GAGeochemAnalysis_1526753_Ni_Result" definition="Ni concentration">
                            <swe:uom code="ppm" xlink:href="http://www.opengis.net/def/uom/UCUM/0/ppm" xlink:title="parts per million"/>
                            <swe:quality>   <!-- Analytical Error -->
                                <swe:Quantity gml:id="GAGeochemAnalysis_1526753_Ni_Error" definition="Ni analytical error">
                                    <swe:uom code="ppm" xlink:href="http://www.opengis.net/def/uom/UCUM/0/ppm" xlink:title="parts per million"/>
                                    <swe:value>0.2</swe:value>
                                </swe:Quantity>
                            </swe:quality>
                            <swe:quality>    <!-- Detection Limit -->
                                <swe:Quantity gml:id="GAGeochemAnalysis_1526753_Ni_DetectionLimit" definition="Ni detection limit">
                                    <swe:uom code="ppm" xlink:href="http://www.opengis.net/def/uom/UCUM/0/ppm" xlink:title="parts per million"/>
                                    <swe:value>0.5</swe:value>
                                </swe:Quantity>
                            </swe:quality>
    <!-- Alternatively, rather than delivering the detection limit with each result, it could be referenced from the Analytical Session document. eg, 
           <swe:quality xlink:href="TB4_GA_AnalyticalSession_rc2.xml#Ni_Detection_Limit"/>  -->
                            <swe:value>-0.5</swe:value>   <!-- NOTE: This value is below the detection limit of 0.5 ppm Ni.  GA denote "below detection limit" by using a minus sign in front of the detection limit value. 
                                See TB4_GA_AnalyticalSession_rc2.xml for description of detection limits for each analytical session.  -->
                        </swe:Quantity>
                    </om:result>
                </om:OM_Observation>
            </sa:relatedObservation>
            
