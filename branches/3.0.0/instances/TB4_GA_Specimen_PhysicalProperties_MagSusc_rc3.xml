<?xml version="1.0" encoding="UTF-8"?>
<sp:SF_Specimen 
    xmlns:sp="http://www.opengis.net/samplingSpecimen/2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gpp="http://xmlns.geosciml.org/PhysicalProperties/3.0"
    xmlns:glab="http://xmlns.geosciml.org/LaboratoryAnalysis-Specimen/3.0"
    xmlns:swe="http://www.opengis.net/swe/2.0"
    xmlns:gml="http://www.opengis.net/gml/3.2"
    xmlns:om="http://www.opengis.net/om/2.0"
    xmlns:sa="http://www.opengis.net/sampling/2.0" 
    xmlns:gmd="http://www.isotc211.org/2005/gmd"
    xmlns:gco="http://www.isotc211.org/2005/gco"
    xmlns:xlink="http://www.w3.org/1999/xlink" 
    xsi:schemaLocation="http://www.opengis.net/samplingSpecimen/2.0 http://schemas.opengis.net/samplingSpecimen/2.0/specimen.xsd
    http://xmlns.geosciml.org/PhysicalProperties/3.0 http://schemas.geosciml.org/physicalproperties/3.0/physicalProperties.xsd
    http://xmlns.geosciml.org/LaboratoryAnalysis-Specimen/3.0 http://schemas.geosciml.org/laboratoryanalysis-specimen/3.0/laboratoryAnalysis-Specimen.xsd"
    gml:id="GA.SpecimenNo.1526753">
    
    <!-- ==================================================================================================================    -->
    <!--    Testbed4 - GeoSciML v3 Release Candidate 3					                                                                                                                                 -->
    <!--     Example instance document for a geological specimen with magnetic susceptibility measurements (maximum, minimum and median values)	                         -->
    <!--     Ollie Raymond, Geoscience Australia, September 2011                                                                                                                                                                        -->
    <!-- ==================================================================================================================     -->
   
    <gml:description>Specimen sampled from GA borehole 96846431</gml:description>
    <gml:identifier codeSpace="http://www.ga.gov.au/sampleNo">1526753</gml:identifier>
    <sa:type xlink:href="http://www.opengis.net/def/samplingFeatureType/OGC-OM/2.0/SF.Specimen"/>
    <sa:sampledFeature xlink:href="http://resource.geoscience.gov.au/classifierscheme/asc/stratigraphiclexicon/32474" xlink:title="Warrumba Volcanics"/>
<!--    <sa:lineage/>-->
    <sa:relatedObservation>
        <om:OM_Observation gml:id="GA.SampleNo.1526753.magsusc.max">
            <om:phenomenonTime>
                <gml:TimeInstant gml:id="GA.SampleNo.1526753.date">
                    <gml:timePosition>2001-10-10</gml:timePosition>
                </gml:TimeInstant>
            </om:phenomenonTime>
            <om:resultTime xlink:href="#GA.SampleNo.1526753.date"/>
            <om:procedure>
                <glab:AnalyticalProcess gml:id="GA.magSuscProcess.1526753">
                    <glab:method>
                        <glab:AnalyticalMethod gml:id="GA.magSuscMethod.1526753">
                            <glab:methodName xlink:href="http://resource.geosience.gov.au/classifier/ga/analyticalMethod/0099" xlink:title="hand held magnetic susceptibility measurement"/>
                            <glab:citation xlink:href="http://www.terraplus.com/rentals/kt-10-plus.pdf"/>
                        </glab:AnalyticalMethod>
                    </glab:method>
                    <glab:acquisition>
                        <glab:AnalyticalSession gml:id="GA.AnalyticalSession.1526753">
                            <glab:time>
                                <gml:TimePeriod gml:id="GA.AnalyticalSession.TimePeriod.1526753">
                                    <gml:begin xlink:href="#GA.SampleNo.1526753.date"/>
                                    <gml:end xlink:href="#GA.SampleNo.1526753.date"/>
                                </gml:TimePeriod>
                            </glab:time>
                            <glab:operator>
                                <gmd:CI_ResponsibleParty>
                                    <gmd:individualName>
                                        <gco:CharacterString>John Smith</gco:CharacterString>
                                    </gmd:individualName>
                                    <gmd:organisationName>
                                        <gco:CharacterString>Geoscience Australia</gco:CharacterString>
                                    </gmd:organisationName>
                                    <gmd:role>
                                        <gmd:CI_RoleCode codeList="http://www.isotc211.org/2005/resources/Codelist/gmxCodelists.xml#CI.RoleCode" codeListValue="principalInvestigator"/>
                                    </gmd:role>
                                </gmd:CI_ResponsibleParty>
                            </glab:operator>
                            <glab:parameter xsi:nil="true" nilReason="inapplicable"/>
                            <glab:instrument>
                                <glab:AnalyticalInstrument gml:id="Fugro.magsusc.meter">
                                    <glab:type xlink:href="http://resource.geoscience.gov.au/classifier/ga/instrumenttype/0088" xlink:title="hand held magnetic susceptibility meter"/>
                                    <glab:model>Fugro GMS-2</glab:model>
                                    <glab:serialNumber>736464839</glab:serialNumber>
                                    <glab:commissionDate>
                                        <gml:TimeInstant gml:id="magsusc.meter.commission.date">
                                            <gml:timePosition>2002-10-26</gml:timePosition>
                                        </gml:TimeInstant>
                                    </glab:commissionDate>
                                    <glab:location>
                                        <gmd:CI_ResponsibleParty>
                                            <gmd:organisationName>
                                                <gco:CharacterString>Geoscience Australia</gco:CharacterString>
                                            </gmd:organisationName>
                                            <gmd:role>
                                                <gmd:CI_RoleCode codeList="http://www.isotc211.org/2005/resources/Codelist/gmxCodelists.xml#CI.RoleCode" codeListValue="custodian"/>
                                            </gmd:role>
                                        </gmd:CI_ResponsibleParty>
                                    </glab:location>
                                    <glab:usedIn/>
                                </glab:AnalyticalInstrument>
                            </glab:instrument>
                            <glab:referenceAnalyses/>
                        </glab:AnalyticalSession>
                    </glab:acquisition>
                    <glab:computation/>
                </glab:AnalyticalProcess>
            </om:procedure>
            <om:observedProperty xlink:href="http://sweet.jpl.nasa.gov/2.0/physElecMag.owl#Magnetism" xlink:title="magnetic susceptibility"/>
            <om:featureOfInterest xlink:href="#GA.SampleNo.1526753"/>
            <om:result>
                <gpp:PhysicalDescription>
                    <gpp:propertyName xlink:href="http://sweet.jpl.nasa.gov/2.0/physElecMag.owl#Magnetism" xlink:title="magnetic susceptibility"/>  
<!--       propertyName may appear redundant here (it effectively repeats OM_Observation/observedProperty), 
             but it is required when PhysicalDescription is used outside of O&M (eg, inside a GeologicUnit element). -->
                    <gpp:propertyMeasure>
                        <swe:Quantity definition="maximum">    <!-- There is no UncertML definition of "maximum" or "minimum".  -->
                            <swe:uom code="SI" xlink:href="http://www.opengis.net/def/uom/UCUM/0/SI" xlink:title="SI units"/>
                            <swe:value>2700e-5</swe:value>
                        </swe:Quantity>
                    </gpp:propertyMeasure>
                </gpp:PhysicalDescription>
            </om:result>
        </om:OM_Observation>
    </sa:relatedObservation>
    <sa:relatedObservation>
        <om:OM_Observation gml:id="GA.SampleNo.1526753.magsusc.min">
            <om:phenomenonTime xlink:href="#GA.SampleNo.1526753.date"/>
            <om:resultTime xlink:href="#GA.SampleNo.1526753.date"/>
            <om:procedure xlink:href="#GA.magSuscProcess.1526753"/>
            <om:observedProperty xlink:href="http://sweet.jpl.nasa.gov/2.0/physElecMag.owl#Magnetism" xlink:title="magnetic susceptibility"/>
            <om:featureOfInterest xlink:href="#GA.SampleNo.1526753"/>
            <om:result>
                <gpp:PhysicalDescription>
                    <gpp:propertyName xlink:href="http://sweet.jpl.nasa.gov/2.0/physElecMag.owl#Magnetism" xlink:title="magnetic susceptibility"/>  
                    <gpp:propertyMeasure>
                        <swe:Quantity definition="minimum">
                            <swe:uom code="SI" xlink:href="http://www.opengis.net/def/uom/UCUM/0/SI" xlink:title="SI units"/>
                            <swe:value>1470e-5</swe:value>
                        </swe:Quantity>
                    </gpp:propertyMeasure>
                </gpp:PhysicalDescription>
            </om:result>
        </om:OM_Observation>
    </sa:relatedObservation>
    <sa:relatedObservation>
        <om:OM_Observation gml:id="GA.SampleNo.1526753.magsusc.mode">
            <om:phenomenonTime xlink:href="#GA.SampleNo.1526753.date"/>
            <om:resultTime xlink:href="#GA.SampleNo.1526753.date"/>
            <om:procedure xlink:href="#GA.magSuscProcess.1526753"/>
            <om:observedProperty xlink:href="http://sweet.jpl.nasa.gov/2.0/physElecMag.owl#Magnetism" xlink:title="magnetic susceptibility"/>
            <om:featureOfInterest xlink:href="#GA.SampleNo.1526753"/>
            <om:result>
                <gpp:PhysicalDescription>
                    <gpp:propertyName xlink:href="http://sweet.jpl.nasa.gov/2.0/physElecMag.owl#Magnetism" xlink:title="magnetic susceptibility"/>  
                    <gpp:propertyMeasure>
                        <swe:Quantity definition="http://dictionary.uncertml.org/statistics/median">
                            <swe:uom code="SI" xlink:href="http://www.opengis.net/def/uom/UCUM/0/SI" xlink:title="SI units"/>
                            <swe:value>1600e-5</swe:value>
                        </swe:Quantity>
                    </gpp:propertyMeasure>
                </gpp:PhysicalDescription>
            </om:result>
        </om:OM_Observation>
    </sa:relatedObservation>
    <sp:materialClass xlink:href="http://resource.geosciml.org/classifier/cgi/materialclass/0001" xlink:title="rock"/>
    <sp:samplingTime xlink:href="#GA.SampleNo.1526753.date"/>
    <sp:samplingMethod>
        <glab:GeologicSamplingMethod gml:id="diamondDrilling">
            <glab:method xlink:href="http://resource.geosciml.org/classifier/cgi/samplingethod/9999" xlink:title="diamond drilling"/>
            <glab:parameter xsi:nil="true" nilReason="inapplicable"/>
        </glab:GeologicSamplingMethod>
    </sp:samplingMethod>
    <sp:samplingLocation>
        <gml:Point gml:id="GA.Location.SampleNo.1526753">
            <gml:pos srsName="http://spatialreference.org/ref/epsg/4283/" srsDimension="2">148.25814099 -33.83566207</gml:pos>
        </gml:Point>
    </sp:samplingLocation>
<!--    <sp:processingDetails/>    -->
<!--    <sp:size/>   -->
<!--    <sp:currentLocation/>   -->
    <sp:specimenType xlink:href="http://resource.geoscience.gov.au/classifier/ga/specimentype/0999" xlink:title="half drill core"/>
</sp:SF_Specimen>
