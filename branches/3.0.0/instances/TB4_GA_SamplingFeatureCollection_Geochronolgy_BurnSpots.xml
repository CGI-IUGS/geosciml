                    <om:observedProperty xlink:href="http://resource.geosciml.org/GeoSciML/3.0/doc/cgi/ChemicalComposition/chemicalAnalysis" xlink:title="Chemical analysis"/>
                    <om:featureOfInterest xlink:href="#GA.Specimen.Geochronology.Shrimp.BurnSpot.143241"/>
                    <om:result>
                        <swe:Quantity>
                            <swe:label>232Th/238U ratio</swe:label>
                            <swe:uom xlink:href="http://reference.for.dimensionless.quantity"/>     <!-- How do we denote a dimensionless ratio "unit ot measure"? -->
                            <swe:value>0.59</swe:value>
                        </swe:Quantity>
                    </om:result>
                </om:OM_Observation>
            </sam:relatedObservation>
            <spec:materialClass xlink:href="http://resource.geoscience.gov.au/classifier/ga/materialclass/0002" xlink:title="mineral"/>
