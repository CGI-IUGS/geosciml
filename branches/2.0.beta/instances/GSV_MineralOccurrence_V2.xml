<!-- GSV VicMine mineralDepositGroup will come from t_md_site: deposit_sub_type_cd -->
			<mo:mineralDepositGroup codeSpace="urn:cgi:classifierScheme:GSV:DepositGroup">Stratiform</mo:mineralDepositGroup>
<!-- GSV VicMine mineralDepositType will come from t_md_deposit_style: deposit_style_cd -->
