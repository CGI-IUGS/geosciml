<!-- GSV VicMine "Mine Name - Primary" from [t_md_site; mine_name; mine_name_type_cd; primary_flg] should be used here -->
	<gml:name>Wilga</gml:name>
<!-- GSV VicMine Site ID from [t_md_site; site_id] along with the GSV feature urn should be used for GML:name to identify the feature-->
	<gml:name codeSpace="urn:cgi:feature:GSV:SiteID_372429">372429</gml:name>
<!-- GSV VicMine doesn't contain any observation method. Observation method is mandatory defaulting to "null [unspecified]" -->
	<gsml:observationMethod>
		<gsml:CGI_TermValue>
			<gsml:qualifier>nil:missing</gsml:qualifier>
			<gsml:value codeSpace="urn:cgi:classifierScheme:GSV:NullValues">null [unspecified]</gsml:value>
		</gsml:CGI_TermValue>
	</gsml:observationMethod>
<!-- GSV VicMine doesn't specify purpose. All entries that are from the t_md_deposit_style have purpose "IdentifyingNorm", those which are not from the style table have purpose"Instance" -->
	<gsml:purpose>instance</gsml:purpose>
	<gsml:occurrence>
		<gsml:MappedFeature gml:id="id_mf01">
<!-- GSV VicMine Site Feature, Feature Type = gml:name. Description = gml:description. Location Type = another gml:name -->
			<gml:description></gml:description>
			<gml:name>Centre of resource</gml:name>
			<gsml:observationMethod>
				<gsml:CGI_TermValue>
					<gsml:value codeSpace="urn:cgi:classifierScheme:GSV:CaptureMethod">map located</gsml:value>
				</gsml:CGI_TermValue>
			</gsml:observationMethod>
<!-- GSV VicMine stores positional accuracy in [t_site; locn_acc] -->
			<gsml:positionalAccuracy>
				<gsml:CGI_NumericValue>
					<gsml:principalValue uom="urn:ogc:def:uom:UCUM:m">100</gsml:principalValue>
				</gsml:CGI_NumericValue>
			</gsml:positionalAccuracy>			
<!-- gsml:specification specifies which GeologicFeature the MappedFeature refers to.-->
			<gsml:specification xlink:href="urn:cgi:feature:GSV:SiteID_372429"/>
<!-- samplingFrame specifies the shape that contains the whole feature -->
			<gsml:samplingFrame xlink:href="urn:cgi:feature:CGI:EarthNaturalSurface"/>
			<gsml:shape>
<!-- EPSG number for MGA94 Zone 55 is inserted here; requires coding to depend on MGA zone; in GSV VicMine this data comes from [t_site; amg_zone] -->
				<gml:Point srsName="EPSG:28355">
					<gml:pos>578963 5904364</gml:pos>
				</gml:Point>
			</gsml:shape>
		</gsml:MappedFeature>
	</gsml:occurrence> 	
	<gsml:preferredAge>
		<gsml:GeologicEvent>
<!-- GSV VicMine has no capacity to store age, defaults to unknown -->
			<gsml:eventAge>
				<gsml:CGI_TermValue>
					<gsml:qualifier>nil:unknown</gsml:qualifier>
					<gsml:value codeSpace="urn:cgi:classifierScheme:GSV:NullValues">null [unknown]</gsml:value>
				</gsml:CGI_TermValue>
			</gsml:eventAge>
<!-- GSV VicMine to use [t_md_deposit_style; deposit_style_cd] vocabulary for eventEnvironment -->
			<gsml:eventEnvironment>
				<gsml:CGI_TermValue>
					<gsml:qualifier>nil:unknown</gsml:qualifier>
					<gsml:value codeSpace="urn:cgi:classifierScheme:GSV:NullValues">null [unknown]</gsml:value>
