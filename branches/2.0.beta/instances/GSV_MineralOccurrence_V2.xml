			<mo:material>
				<gsml:Rock>
					<gsml:purpose>instance</gsml:purpose>
<!-- where GSV VicMine lithology contains "unconsolidated" GSV to use class UnconsolidatedMaterial and consolidationDegree = "unconsolidated"; for all others use class Rock and consolidationDegree = "consolidated" -->
