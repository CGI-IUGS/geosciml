						<gsml:CGI_Term>
							<gsml:value>
								<gsml:CGI_TermValue>
									<gsml:value codeSpace="urn:cgi:classifierScheme:CGI:ConsolidationDegree">consolidated</gsml:value>
								</gsml:CGI_TermValue>
