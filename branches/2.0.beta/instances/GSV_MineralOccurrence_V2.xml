				<gsml:CGI_NumericValue>
					<gsml:principalValue uom="urn:ogc:def:uom:UCUM:degree">45</gsml:principalValue>
				</gsml:CGI_NumericValue>
			</gsml:dip>
			<gsml:polarity>unknown</gsml:polarity>
		</gsml:CGI_PlanarOrientation>
	</mo:planarOrientation>
