<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gsml="urn:cgi:xmlns:CGI:GeoSciML:2.0"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_fossil_GeoSciML_V2.xml">
	<description>Provides a mapping of classes from the fossil module of GeoSciML to elements from GeoSciML XML Schema Version 2.0.</description>
	<imports>
		<schema prefix="gsml" namespace="urn:cgi:xmlns:CGI:GeoSciML:2.0" schemaLocation="http://www.geosciml.org/geosciml/2.0/xsd/geosciml.xsd" />
	</imports>
	<entry>
		<umlClass>FossilMold</umlClass>
		<gmlObjectElement>gsml:FossilMold</gmlObjectElement>
		<gmlType>gsml:FossilMoldType</gmlType>
		<gmlPropertyType>gsml:FossilMoldPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>Organism</umlClass>
		<umlStereotype>Type</umlStereotype>
		<gmlObjectElement>gsml:Organism</gmlObjectElement>
		<gmlType>gsml:OrganismType</gmlType>
		<gmlPropertyType>gsml:OrganismPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>TraceFossil</umlClass>
		<gmlObjectElement>gsml:TraceFossil</gmlObjectElement>
		<gmlType>gsml:TraceFossilType</gmlType>
		<gmlPropertyType>gsml:TraceFossilPropertyType</gmlPropertyType>
	</entry>
</mapping>