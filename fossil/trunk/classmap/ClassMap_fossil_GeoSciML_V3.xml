<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gsml="urn:cgi:xmlns:CGI:GeoSciML:3.0"
	xmlns:gfos="urn:cgi:xmlns:CGI:Fossil:3.0"   <!--  Do we need to register a Fossil namespace?  -->
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:3.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:3.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_fossil_GeoSciML_V3.xml">
	<description>Provides a mapping of classes from the fossil module of GeoSciML to elements from GeoSciML XML Schema Version 3.0.</description>
	<imports>  <!-- What goes in here? -->
<!--		<schema prefix="gsml" namespace="urn:cgi:xmlns:CGI:GeoSciML:3.0" schemaLocation="http://www.geosciml.org/geosciml/3.0/xsd/geosciml.xsd" />-->
		<schema prefix="gfos" namespace="urn:cgi:xmlns:CGI:Fossil:3.0" schemaLocation="http://www.geosciml.org/fossil/3.0/xsd/fossil.xsd" />		
	</imports>
	<entry>
		<umlClass>FossilMold</umlClass>
		<umlStereotype>FeatureType</umlStereotype>
		<gmlObjectElement>gfos:FossilMold</gmlObjectElement>
		<gmlType>gfos:FossilMoldType</gmlType>
		<gmlPropertyType>gfos:FossilMoldPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>Organism</umlClass>
		<umlStereotype>FeatureType</umlStereotype>
		<gmlObjectElement>gfos:Organism</gmlObjectElement>
		<gmlType>gfos:OrganismType</gmlType>
		<gmlPropertyType>gfos:OrganismPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>TraceFossil</umlClass>
		<umlStereotype>FeatureType</umlStereotype>
		<gmlObjectElement>gfos:TraceFossil</gmlObjectElement>
		<gmlType>gfos:TraceFossilType</gmlType>
		<gmlPropertyType>gfos:TraceFossilPropertyType</gmlPropertyType>
	</entry>
</mapping>