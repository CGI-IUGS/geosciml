<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gspec="urn:cgi:xmlns:CGI:GeologicSpecimen:3.0.0"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_specimen_GeoSciML_V3.xml">
	<description>Provides a mapping of classes from the geologic specimen module of GeoSciML to elements from GeoSciML XML Schema Version 3.0.</description>
	<imports>
		<schema prefix="gspec" namespace="urn:cgi:xmlns:CGI:GeologicSpecimen:3.0.0" schemaLocation="http://www.geosciml.org/geologicSpecimen/3.0.0/xsd/geologicSpecimen.xsd" />
	</imports>
	<entry>
		<umlClass>GeologicSamplingMethod</umlClass> 
		<umlStereotype>FeatureType</umlStereotype> 
		<gmlObjectElement>gspec:GeologicSamplingMethod</gmlObjectElement> 
		<gmlType>gspec:GeologicSamplingMethodType</gmlType> 
		<gmlPropertyType>gspec:GeologicSamplingMethodPropertyType</gmlPropertyType> 
	</entry>
	<entry>
		<umlClass>GeologicSamplingMethodTerm</umlClass> 
		<umlStereotype>DataType</umlStereotype> 
		<gmlObjectElement>gspec:GeologicSamplingMethodTerm</gmlObjectElement> 
		<gmlType>gspec:GeologicSamplingMethodTermType</gmlType> 
		<gmlPropertyType>gspec:GeologicSamplingMethodTermPropertyType</gmlPropertyType> 
	</entry>
	<entry>
		<umlClass>GeologicSpecimenPreparation</umlClass> 
		<umlStereotype>FeatureType</umlStereotype> 
		<gmlObjectElement>gspec:GeologicSpecimenPreparation</gmlObjectElement> 
		<gmlType>gspec:GeologicSpecimenPreparationType</gmlType> 
		<gmlPropertyType>gspec:GeologicSpecimenPreparationPropertyType</gmlPropertyType> 
	</entry>
	<entry>
		<umlClass>GeologicSpecimenPreparationTerm</umlClass> 
		<umlStereotype>DataType</umlStereotype> 
		<gmlObjectElement>gspec:GeologicSpecimenPreparationTerm</gmlObjectElement> 
		<gmlType>gspec:GeologicSpecimenPreparationTermType</gmlType> 
		<gmlPropertyType>gspec:GeologicSpecimenPreparationTermPropertyType</gmlPropertyType> 
	</entry>
	<entry>
		<umlClass>ReferenceSpecimen</umlClass> 
		<umlStereotype>FeatureType</umlStereotype> 
		<gmlObjectElement>gspec:ReferenceSpecimen</gmlObjectElement> 
		<gmlType>gspec:ReferenceSpecimenType</gmlType> 
		<gmlPropertyType>gspec:ReferenceSpecimenPropertyType</gmlPropertyType> 
	</entry>
</mapping>