<?xml version="1.0" encoding="UTF-8"?>
<wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs" xmlns:gsc="http://www.nrcan.gc.ca/gsc" xmlns:ogc="http://www.opengis.net/ogc" version="1.1.0" service="WFS" maxFeatures="10">
<wfs:Query typeName="gsc:GeologicUnit">
<ogc:Filter>
	<ogc:PropertyIsEqualTo>
		<ogc:PropertyName>gsc:preferredAge/gsc:Age/gsc:ga_name</ogc:PropertyName>
		<ogc:Literal>Middle Devonian</ogc:Literal>
		</ogc:PropertyIsEqualTo>
	</ogc:Filter>
</wfs:Query>
</wfs:GetFeature>
