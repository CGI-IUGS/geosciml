<?xml version="1.0" encoding="UTF-8"?>
<wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs" xmlns:gsml="urn:cgi:xmlns:CGI:GeoSciML:2.0" xmlns:ogc="http://www.opengis.net/ogc" version="1.1.0" service="WFS" maxFeatures="10">
<wfs:Query typeName="gsml:GeologicUnit">
	<ogc:Filter>
	<ogc:PropertyIsEqualTo>
		<ogc:PropertyName>gsml:preferredAge/gsml:GeologicEvent/gsml:eventAge/gsml:CGI_Term/gsml:value</ogc:PropertyName>
		<ogc:Literal>Middle Devonian</ogc:Literal>
		</ogc:PropertyIsEqualTo>
	</ogc:Filter>
</wfs:Query>
</wfs:GetFeature>
