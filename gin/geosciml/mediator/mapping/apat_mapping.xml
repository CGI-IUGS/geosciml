<?xml version="1.0" encoding="UTF-8"?>
<!-- The Italian local age vocabulary to macth the GeoSciML schema request -->
<map:ontology xmlns:map="urn:x-lcnp:map" xmlns:xlink="http://www.w3.org/1999/xlink">
	<map:global-ontology xlink:href="cocoon:/ontology/global/StratChart2008"/>
	<map:local-ontology>
		<map:concepts>
		<map:concept name="n.d."/>
			<map:concept name="Phanerozoic">
				<map:concept name="Cenozoic">
					<map:concept name="Quaternary">
						<map:concept name="Holocene"/>
						<map:concept name="Pleistocene"/>
					</map:concept>
          <map:concept name="Neogene">
            <map:concept name="Pliocene"/>
            <map:concept name="Miocene">
              <map:concept name="Upper (Late) Miocene"/>
              <!-- Messinian -->
              <map:concept name="Lower (Early) Miocene"/>
              <!-- Aquitanian -->
            </map:concept>
          </map:concept>
            <map:concept name="Paleogene">
						<map:concept name="Oligocene"/>
						<map:concept name="Eocene"/>
						<map:concept name="Paleocene"/>
					</map:concept>
					
					</map:concept>
				</map:concept>
				<map:concept name="Mesozoic">
					<map:concept name="Cretaceous">
						<map:concept name="Upper (Late) Cretaceous"/>
					</map:concept>
          <map:concept name="Jurassic">
            <map:concept name="Upper (Late) Jurassic"/>
          </map:concept>
            <map:concept name="Triassic">
              <map:concept name="Upper (Late) Triassic"/>
              <map:concept name="Middle Triassic"/>
              <map:concept name="Lower (Early) Triassic"/>
            </map:concept>
          </map:concept>
				<map:concept name="Paleozoic">
					<map:concept name="Permian">
						<map:concept name="Lower (Early) Permian"/> <!-- cisuralian -->
					</map:concept>
					<map:concept name="Carboniferous">
						<map:concept name="Lower (Early) Carboniferous"/> <!-- tournaisian -->
					</map:concept>
					<map:concept name="Devonian"/>
					<map:concept name="Silurian"/>
					<map:concept name="Cambrian"/>
				</map:concept>
			
			<map:concept name="Precambrian">
				<map:concept name="Proterozoic"/>
			</map:concept>
			
		</map:concepts>
	</map:local-ontology>
	<map:rules>
		<map:rule global="urn:cgi:classifier:ICS:StratChart:2008:Aquitanian" local="Lower (Early) Miocene"/>
