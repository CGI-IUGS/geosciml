<?xml version="1.0" encoding="UTF-8"?>
<mapping
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:gsml="urn:cgi:xmlns:CGI:GeoSciML:2.0"
	xmlns="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0"
	xsi:schemaLocation="urn:x-auscope:xmlns:CSIRO:FullMoon:package-class-map:2.0 http://projects.arcs.org.au/svn/fullmoon/trunk/resources/dependency-util/package-class-map.xsd"
	id="ClassMap_unit_GeoSciML_V2.xml">
	<description>Provides a mapping of classes from the geologic unit module of GeoSciML to elements from GeoSciML XML Schema Version 2.0.</description>
	<imports>
		<schema prefix="gsml" namespace="urn:cgi:xmlns:CGI:GeoSciML:2.0" schemaLocation="http://www.geosciml.org/geosciml/2.0/xsd/geosciml.xsd" />
	</imports>
	<entry>
		<umlClass>BeddingDescription</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gsml:BeddingDescription</gmlObjectElement>
		<gmlType>gsml:BeddingDescriptionType</gmlType>
		<gmlPropertyType>gsml:BeddingDescriptionPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>GeologicUnit</umlClass>
		<gmlObjectElement>gsml:GeologicUnit</gmlObjectElement>
		<gmlType>gsml:GeologicUnitType</gmlType>
		<gmlPropertyType>gsml:GeologicUnitPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>GeologicUnitPart</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gsml:GeologicUnitPart</gmlObjectElement>
		<gmlType>gsml:GeologicUnitPartType</gmlType>
		<gmlPropertyType>gsml:GeologicUnitPartPropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>GeologicUnitType</umlClass>
		<gmlObjectElement xsi:nil="true"/>
		<gmlType xsi:nil="true"/>
		<gmlPropertyType>gsml:GeologicUnitTypePropertyType</gmlPropertyType>
	</entry>
	<entry>
		<umlClass>WeatheringDescription</umlClass>
		<umlStereotype>DataType</umlStereotype>
		<gmlObjectElement>gsml:WeatheringDescription</gmlObjectElement>
		<gmlType>gsml:WeatheringDescriptionType</gmlType>
		<gmlPropertyType>gsml:WeatheringDescriptionPropertyType</gmlPropertyType>
	</entry>
</mapping>