    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:amphibolite_metamorphic_facies -->

    <skos:Concept rdf:about="&xx;amphibolite_metamorphic_facies">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Metamorphic facies characterized in rocks of basaltic composition by hornblende-plagioclase (plagioclase more calcic than An17).</skos:definition>
        <skos:historyNote
            >Smulikowski et al. 2003; Fettes and Desmons 2007</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Amphibolite metamorphic facies</skos:prefLabel>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:eclogite_metamorphic_facies -->

    <owl:Thing rdf:about="&xx;eclogite_metamorphic_facies">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Eclogite metamorphic facies</skos:prefLabel>
        <skos:historyNote xml:lang="en"
            >Fettes and Desmons 2007</skos:historyNote>
        <skos:definition
            >omphacite-garnet-quartz (no plagioclase, olivine stable with garnet); assemblage for rocks of basaltic composition</skos:definition>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:epidote_amphibolite_metamorphic_facies -->

    <owl:Thing rdf:about="&xx;epidote_amphibolite_metamorphic_facies">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >hornblende-albite-epidote(-chlorite); assemblage for rocks of basaltic composition</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Epidote amphibolite metamorphic facies</skos:prefLabel>
        <skos:historyNote xml:lang="en"
            >Fettes and Desmons 2007</skos:historyNote>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:glaucophane_schist_facies -->

    <skos:Concept rdf:about="&xx;glaucophane_schist_facies">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Metamorphic facies characterized in rocks of basaltic composition by glaucophane-epidote (-garnet), glaucophane-lawsonite, or glaucophane-lawsonite-jadeite.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Glaucophane-schist facies</skos:prefLabel>
        <skos:historyNote xml:lang="en"
            >Smulikowski et al., 2003; Fettes and Desmons 2007</skos:historyNote>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:granulite_facies -->

    <skos:Concept rdf:about="&xx;granulite_facies">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Metamorphic facies characterized in rocks of basaltic composition by the mineral assemblage clinopyroxene-orthopyroxene-plagioclase (olivine not stable with plagioclase or with garnet), or especially high-temperature varieties or polymorphs of minerals (e</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Granulite facies</skos:prefLabel>
        <skos:historyNote xml:lang="en"
            >Smulikowski et al., 2003; Fettes and Desmons 2007</skos:historyNote>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:greenschist_metamorphic_facies -->

    <owl:Thing rdf:about="&xx;greenschist_metamorphic_facies">
        <rdf:type rdf:resource="&skos;Concept"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >Greenschist metamorphic facies</skos:prefLabel>
        <skos:definition
            >Metamorphic facies characterized in rocks of basaltic composition by actinolite-albite-epidote-chlorite (an epidote group mineral is the diagnostic Ca-Al silicate rather than prehnite or pumpellyite).</skos:definition>
        <skos:historyNote xml:lang="en"
            >Smulikowski et al., 2003; Fettes and Desmons 2007</skos:historyNote>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:metamorphic_facies_not_specified -->

    <skos:Concept rdf:about="&xx;metamorphic_facies_not_specified">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >For use in normative description to indicate that any metamorphic facies value is valid.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Metamorphic facies not specified</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:metamorphic_facies_unknown -->

    <skos:Concept rdf:about="&xx;metamorphic_facies_unknown">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition xml:lang="en"
            >For use in instance descriptions to indicate that no data are available to constrain metamorphic facies.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Metamorphic facies unknown</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:no_metamorphic_minerals -->

    <skos:Concept rdf:about="&xx;no_metamorphic_minerals">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Corresponds to non-metamorphosed condition; since metamorphic facies classification is based on mineral assemblages, absence of metamorphic minerals means that the classification does not apply.</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:prefLabel xml:lang="en"
            >No metamorphic minerals</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
        <skos:topConceptOf rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:pyroxene_hornfels_metamorphic_facies -->

    <skos:Concept rdf:about="&xx;pyroxene_hornfels_metamorphic_facies">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Metamorphic facies characterized in rocks of basaltic composition by clinopyroxene-orthopyroxene-plagioclase (olivine stable with plagioclase).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Pyroxene hornfels metamorphic facies</skos:prefLabel>
        <skos:historyNote xml:lang="en"
            >Smulikowski et al. 2003;Fettes and Desmons 2007</skos:historyNote>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:sanidinite_metamorphic_facies -->

    <owl:Thing rdf:about="&xx;sanidinite_metamorphic_facies">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >distinguished from the pyroxene hornfels facies by the occurrence of especially high-temperature varieties and polymorphs of minerals (eg. pigeonite, K-rich labradorite); assemblage for rocks of basaltic composition</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote xml:lang="en"
            >Fettes and Desmons 2007</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Sanidinite metamorphic facies</skos:prefLabel>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:subgreenschist_metamorphic_facies -->

    <skos:Concept rdf:about="&xx;subgreenschist_metamorphic_facies">
        <rdf:type rdf:resource="&owl;Thing"/>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:definition
            >Metamorphic facies characterized in rocks of basaltic composition by prehnite-pumpellyite, pumpellyite-actinolite, prehnite-actinolite (prehnite and pumpellyite are the diagnostic Ca-Al silicates rather than minerals of the epidote or zeolite groups).</skos:definition>
        <skos:historyNote xml:lang="en"
            >Smulikowski et al. 2003; Fettes and Desmons 2007</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Subgreenschist metamorphic facies</skos:prefLabel>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:MetamorphicFacies:2009xx:zeolite_metamorphic_facies -->

    <skos:Concept rdf:about="&xx;zeolite_metamorphic_facies">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Metamorphic facies characterized in rocks of basaltic composition by zeolite minerals such as laumontite and heulandites (in place of other Ca-Al silicates such as prehnite, pumpellyite and epidote).</skos:definition>
        <dc:creator xml:lang="en"
            >Concept Definition Task Group of IUGS CGI Interoperability Working Group (http://www.cgi-iugs.org/tech_collaboration/interoperability_working_group.html)</dc:creator>
        <skos:historyNote xml:lang="en"
            >Smulikowski et al. 2003; Fettes and Desmons 2007</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Zeolite metamorphic facies</skos:prefLabel>
        <skos:broader rdf:resource="&xx;metamorphic_facies_not_specified"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


