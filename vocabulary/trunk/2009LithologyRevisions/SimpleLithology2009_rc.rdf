        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Unconsolidated material known to have natural, ie. not human-made, origin.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Natural unconsolidated material</skos:prefLabel>
        <skos:broader rdf:resource="#unconsolidated_material"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #non_clastic_siliceous_sediment -->

    <skos:Concept rdf:about="#non_clastic_siliceous_sediment">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx"
            >non_clastic_siliceous_sediment</skos:notation>
        <skos:historyNote
            >NGMDB 2008; Hallsworth and Knox 1999</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Non-clastic siliceous sediment</skos:prefLabel>
        <skos:definition
            >Sediment that consists of at least 50 percent silicate mineral material, deposited directly by chemical or biological processes at the depositional surface, or in particles formed by chemical or biological processes within the basin of deposition.</skos:definition>
        <skos:broader rdf:resource="#non_clastic_siliceous_sedimentary_material"/>
        <skos:broader rdf:resource="#sediment"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #non_clastic_siliceous_sedimentary_material -->

    <owl:Thing rdf:about="#non_clastic_siliceous_sedimentary_material">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx"
            >non_clastic_siliceous_sedimentary_material</skos:notation>
        <skos:definition
            >Sedimentary material that consists of at least 50 percent silicate mineral material, deposited directly by chemical or biological processes at the depositional surface, or in particles formed by chemical or biological processes within the basin of deposition.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Non-clastic siliceous sedimentary material</skos:prefLabel>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:broader rdf:resource="#chemical_sedimentary_material"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #non_clastic_siliceous_sedimentary_rock -->

    <skos:Concept rdf:about="#non_clastic_siliceous_sedimentary_rock">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx"
            >non_clastic_siliceous_sedimentary_rock</skos:notation>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:definition
            >Sedimentary rock that consists of at least 50 percent silicate mineral material, deposited directly by chemical or biological processes at the depositional surface, or in particles formed by chemical or biological processes within the basin of deposition.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Non-clastic siliceous sedimentary rock</skos:prefLabel>
        <skos:broader rdf:resource="#non_clastic_siliceous_sedimentary_material"/>
        <skos:broader rdf:resource="#sedimentary_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #ooze -->

    <owl:Thing rdf:about="#ooze">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">ooze</skos:notation>
        <skos:definition xml:lang="en"
            >Biogenic sediment consisting of less than 1 percent gravel-size (greater than or equal to 2 mm) particles, with a sand to mud ratio less than 1 to 9, and less than 50 percent carbonate minerals.</skos:definition>
        <skos:prefLabel xml:lang="en">Ooze</skos:prefLabel>
        <skos:historyNote
            >based on Bates and Jackson 1987 &amp; Hallsworth &amp; Knox 1999</skos:historyNote>
        <skos:historyNote xml:lang="en"
            >update definition, add mud as related term  in 200911 version</skos:historyNote>
        <skos:broader rdf:resource="#biogenic_sediment"/>
        <skos:related rdf:resource="#mud"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #organic_rich_sediment -->

    <skos:Concept rdf:about="#organic_rich_sediment">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx"
            >organic_rich_sediment</skos:notation>
        <skos:definition
            >Sediment with color, composition, texture and apparent density indicating greater than 50 percent organic content by weight on a moisture-free basis.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Organic rich sediment</skos:prefLabel>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:broader rdf:resource="#organic_rich_sedimentary_material"/>
        <skos:broader rdf:resource="#sediment"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #organic_rich_sedimentary_material -->

    <skos:Concept rdf:about="#organic_rich_sedimentary_material">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx"
            >organic_rich_sedimentary_material</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Organic rich sedimentary material</skos:prefLabel>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:definition
            >Sedimentary material in which 50 percent or more of the primary sedimentary material is organic carbon.</skos:definition>
        <skos:broader rdf:resource="#sedimentary_material"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #organic_rich_sedimentary_rock -->

    <owl:Thing rdf:about="#organic_rich_sedimentary_rock">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx"
            >organic_rich_sedimentary_rock</skos:notation>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Organic rich sedimentary rock</skos:prefLabel>
        <skos:definition
            >Sedimentary rock with color, composition, texture and apparent density indicating greater than 50 percent organic content by weight on a moisture-free basis.</skos:definition>
        <skos:broader rdf:resource="#organic_rich_sedimentary_material"/>
        <skos:broader rdf:resource="#sedimentary_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #packstone -->

    <owl:Thing rdf:about="#packstone">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">packstone</skos:notation>
        <skos:definition
            >Carbonate sedimentary rock with discernible grain supported depositional texture, containing greater than 10 percent grains; intergranular spaces are filled by matrix.</skos:definition>
        <skos:historyNote
            >Hallsworth &amp; Knox 1999</skos:historyNote>
        <skos:prefLabel xml:lang="en">Packstone</skos:prefLabel>
        <skos:broader rdf:resource="#carbonate_sedimentary_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #peat -->

    <skos:Concept rdf:about="#peat">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">peat</skos:notation>
        <skos:definition
            >Unconsolidated organic-rich sediment composed of at least 50 percent semi-carbonised plant remains; individual remains commonly seen with unaided eye; yellowish brown to brownish black; generally fibrous texture; can be plastic or friable. In its natural state it can be readily cut and has a very high moisture content, generally greater than 90 percent.</skos:definition>
        <skos:historyNote
            >Hallsworth &amp; Knox 1999</skos:historyNote>
        <skos:prefLabel xml:lang="en">Peat</skos:prefLabel>
        <skos:broader rdf:resource="#organic_rich_sediment"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #pebble_gravel -->

    <skos:Concept rdf:about="#pebble_gravel">
        <rdf:type rdf:resource="&owl;Thing"/>
        <rdfs:label rdf:datatype="&xsd;string">Pebble gravel</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string"
            >Sediment containing greater than 30 percent pebble-size particles (2.0 -64 mm in diameter)</skos:definition>
        <skos:historyNote rdf:datatype="&xsd;string"
            >Wentworth size scale</skos:historyNote>
        <skos:prefLabel xml:lang="en">Pebble gravel</skos:prefLabel>
        <skos:broader rdf:resource="#gravel"/>
    </skos:Concept>
    


    <!-- #pegmatite -->

    <owl:Thing rdf:about="#pegmatite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote
            >Neuendorf et al. 2005</skos:historyNote>
        <skos:definition
            >Exceptionally coarse grained crystalline rock with interlocking crystals; most grains are 1cm or more diameter; composition is generally that of granite, but the term may refer to the coarse grained facies of any type of igneous rock;usually found as irregular dikes, lenses, or veins associated with plutons or batholiths.</skos:definition>
        <skos:prefLabel xml:lang="en">Pegmatite</skos:prefLabel>
        <skos:broader rdf:resource="#phaneritic_igneous_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #peridotite -->

    <skos:Concept rdf:about="#peridotite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Peridotite</skos:prefLabel>
        <skos:definition
            >Ultramafic rock consisting of more than 40 percent (by volume) olivine with pyroxene and/or amphibole and little or no feldspar. Commonly altered to serpentinite. Includes rocks defined modally in the ultramafic rock classification as dunite, harzburgite, lherzolite, wehrlite, olivinite, pyroxene peridotite, pyroxene hornblende peridotite or hornblende peridotite.</skos:definition>
        <rdfs:seeAlso xml:lang="en"
            >dunite, olivinite, harzburgite, lherzolite, wherlite.</rdfs:seeAlso>
        <skos:broader rdf:resource="#phaneritic_igneous_rock"/>
        <skos:broader rdf:resource="#ultramafic_igneous_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #phaneritic_igneous_rock -->

    <skos:Concept rdf:about="#phaneritic_igneous_rock">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >Neuendorf et al. 2005</skos:historyNote>
        <skos:definition
            >Igneous rock in which greater than 10 percent of rock is individual crystals that can be discerned with the unaided eye. Bounding grain size is on the order of 32 to 100 microns. Igneous rocks with &#39;exotic&#39; composition are excluded from this concept.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Phaneritic igneous rock</skos:prefLabel>
        <skos:narrower rdf:resource="#dioritoid"/>
        <skos:narrower rdf:resource="#foid_dioritoid"/>
        <skos:narrower rdf:resource="#foid_syenitoid"/>
        <skos:narrower rdf:resource="#gabbroid"/>
        <skos:broader rdf:resource="#igneous_rock"/>
        <skos:narrower rdf:resource="#syenitoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #phonolilte -->

    <skos:Concept rdf:about="#phonolilte">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote xml:lang="en"
            >LeMaitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Phonolite</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Phonolitoid in which the plagioclase to alkali feldspar ratio is less than 0.1. Rock consists of alkali feldspar, feldspathoid minerals, and mafic minerals.</skos:definition>
        <rdfs:seeAlso xml:lang="en"
            >peralkaline phonolite</rdfs:seeAlso>
        <skos:broader rdf:resource="#phonolitoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #phonolitic_basanite -->

    <owl:Thing rdf:about="#phonolitic_basanite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote xml:lang="en"
            >Le Maitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Phonolitic basanite</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Tephritoid that has a plagioclase to alkali feldspar ratio between 0.5 and  0.9, and contains more than 10 percent normative (CIPW) olivine.</skos:definition>
        <skos:broader rdf:resource="#tephritoid"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #phonolitic_foidite -->

    <owl:Thing rdf:about="#phonolitic_foidite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Foiditoid that contains less than 90 percent feldspathoid minerals in the QAPF fraction, and has a  plagioclase to alkali feldspar ratio that is less than 0.5</skos:definition>
        <skos:historyNote xml:lang="en"
            >LeMaitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Phonolitic foidite</skos:prefLabel>
        <skos:broader rdf:resource="#foiditoid"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #phonolitic_tephrite -->

    <skos:Concept rdf:about="#phonolitic_tephrite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote xml:lang="en"
            >Le Maitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Phonolitic tephrite</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Tephritoid that has a plagioclase to alkali feldspar ratio between 0.5 and 0.9, and contains less than 10 percent normative (CIPW) olivine.</skos:definition>
        <skos:broader rdf:resource="#tephritoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #phonolitoid -->

    <skos:Concept rdf:about="#phonolitoid">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <skos:definition xml:lang="en"
            >Fine grained igneous rock than contains less than 90 percent mafic minerals, between 10 and 60 percent feldspathoid mineral in the QAPF fraction and has a plagioclase to alkali feldspar ratio less than 0.5. Includes rocks defined modally in QAPF fields 11 and 12, and TAS field Ph.</skos:definition>
        <skos:altLabel xml:lang="en">Phonolitic rock</skos:altLabel>
        <skos:prefLabel xml:lang="en">Phonolitoid</skos:prefLabel>
        <skos:broader rdf:resource="#fine_grained_igneous_rock"/>
        <skos:narrower rdf:resource="#phonolilte"/>
        <skos:narrower rdf:resource="#tephritic_phonolite"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #phosphate_rich_sediment -->

    <owl:Thing rdf:about="#phosphate_rich_sediment">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx"
            >phosphate_rich_sediment</skos:notation>
        <skos:prefLabel xml:lang="en"
            >Phosphate rich sediment</skos:prefLabel>
        <skos:definition
            >Sediment in which at least 50 percent of the primary and/or recrystallized constituents are phosphate minerals.</skos:definition>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:historyNote xml:lang="en"
            >change phosphatic to phosphate rich in v 200911</skos:historyNote>
        <skos:broader rdf:resource="#phosphate_rich_sedimentary_material"/>
        <skos:broader rdf:resource="#sediment"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #phosphate_rich_sedimentary_material -->

    <skos:Concept rdf:about="#phosphate_rich_sedimentary_material">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx"
            >phosphate_rich_sedimentary_material</skos:notation>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Phosphate rich sedimentary material</skos:prefLabel>
        <skos:definition
            >Sedimentary material in which at least 50 percent of the primary and/or recrystallized constituents are phosphate minerals.</skos:definition>
        <skos:broader rdf:resource="#chemical_sedimentary_material"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #phosphorite -->

    <skos:Concept rdf:about="#phosphorite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">phosphorite</skos:notation>
        <skos:prefLabel xml:lang="en">Phosphorite</skos:prefLabel>
        <skos:definition
            >Sedimentary rock in which at least 50 percent of the primary or recrystallized constituents are phosphate minerals. Most commonly occurs as a bedded primary or reworked secondary marine rock, composed of microcrystalline carbonate fluorapatite in the form of lamina, pellets, oolites and nodules, and skeletal, shell and bone fragments.</skos:definition>
        <skos:historyNote
            >Hallsworth&amp;Knox 1999, Jackson 1997</skos:historyNote>
        <skos:broader rdf:resource="#phosphate_rich_sedimentary_material"/>
        <skos:broader rdf:resource="#sedimentary_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #phyllite -->

    <skos:Concept rdf:about="#phyllite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >IUGS SCMR 2007 (http://www.bgs.ac.uk/SCMR/)</skos:historyNote>
        <skos:prefLabel xml:lang="en">Phyllite</skos:prefLabel>
        <skos:definition
            >Rock with a well developed, continuous schistosity, an average grain size between 0.1 and 0.5 millimeters, and a silvery sheen on cleavage surfaces. Individual phyllosilicate grains are barely visible with the unaided eye.</skos:definition>
        <skos:broader rdf:resource="#foliated_metamorphic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #phyllonite -->

    <owl:Thing rdf:about="#phyllonite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Mylonitic rock composed largely of fine-grained mica that imparts a sheen to foliation surfaces; may have flaser lamination, isoclinal folding, and deformed veins, which indicate significant shearing. Macroscopically resembles phyllite, but formed by mechanical degradation of initially coarser rock.</skos:definition>
        <skos:historyNote
            >NADM metamorphic rock vocabulary SLTTm1.0; Marshak &amp; Mitra 1988</skos:historyNote>
        <skos:prefLabel xml:lang="en">Phyllonite</skos:prefLabel>
        <skos:broader rdf:resource="#mylonitic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #porphyry -->

    <skos:Concept rdf:about="#porphyry">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">porphyry</skos:notation>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <skos:definition
            >Igneous rock that contains conspicuous phenocrysts in a finer grained groundmass; groundmass itself may be phaneritic or fine-grained.</skos:definition>
        <skos:prefLabel xml:lang="en">Porphyry</skos:prefLabel>
        <skos:broader rdf:resource="#igneous_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #pure_calcareous_carbonate_sediment -->

    <owl:Thing rdf:about="#pure_calcareous_carbonate_sediment">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Carbonate sediment in which greater than 90 percent of the  constituents are composed of one (or more) of the carbonate minerals in particles of intrabasinal origin, and a calcite (plus aragonite) to dolomite ratio greater than 1 to 1.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Pure calcareous carbonate sediment</skos:prefLabel>
        <skos:altLabel xml:lang="en">lime sediment</skos:altLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="#calcareous_carbonate_sediment"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #pure_carbonate_sediment -->

    <skos:Concept rdf:about="#pure_carbonate_sediment">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Carbonate sediment in which greater than 90 percent of the  constituents are composed of one (or more) of the carbonate minerals in particles of intrabasinal origin.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Pure carbonate sediment</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="#carbonate_sediment"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #pure_dolomitic_sediment -->

    <skos:Concept rdf:about="#pure_dolomitic_sediment">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Carbonate sediment in which greater than 90 percent of the  constituents are composed of one (or more) of the carbonate minerals in particles of intrabasinal origin, and a ratio of magnesium carbonate to calcite (plus aragonite) greater than 1 to 1.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Pure dolomitic sediment</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="#dolomitic_sediment"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #pyroclastic_material -->

    <owl:Thing rdf:about="#pyroclastic_material">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <skos:definition
            >Fragmental igneous material that consists of more than 75 percent of particles formed by disruption as a direct result of volcanic action.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Pyroclastic material</skos:prefLabel>
        <skos:broader rdf:resource="#fragmental_igneous_material"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #pyroclastic_rock -->

    <owl:Thing rdf:about="#pyroclastic_rock">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Fragmental igneous rock that consists of greater than 75 percent fragments produced as a direct result of eruption or extrusion of magma from within the earth onto its surface. Includes autobreccia associated with lava flows and excludes deposits reworked by epiclastic processes.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Pyroclastic rock</skos:prefLabel>
        <skos:historyNote
            >based on LeMaitre et al. 2002</skos:historyNote>
        <skos:broader rdf:resource="#fragmental_igneous_rock"/>
        <skos:broader rdf:resource="#pyroclastic_material"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #pyroxenite -->

    <owl:Thing rdf:about="#pyroxenite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Pyroxenite</skos:prefLabel>
        <skos:definition
            >Ultramafic phaneritic igneous rock composed almost entirely of one or more pyroxenes and occasionally biotite, hornblende and olivine. Includes rocks defined modally in the ultramafic rock classification as olivine pyroxenite, olivine-hornblende pyroxenite, pyroxenite, orthopyroxenite, clinopyroxenite and websterite.</skos:definition>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <rdfs:seeAlso xml:lang="en"
            >orthopyroxenite, websterite, clinopyroxenite</rdfs:seeAlso>
        <skos:broader rdf:resource="#phaneritic_igneous_rock"/>
        <skos:broader rdf:resource="#ultramafic_igneous_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #quartz_alkali_feldspar_syenite -->

    <owl:Thing rdf:about="#quartz_alkali_feldspar_syenite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Alkali feldspar syenitic rock that contains 5 to 20 percent quartz and no feldspathoid in the QAPF fraction. QAPF field 6*.</skos:definition>
        <skos:broader rdf:resource="#alkali_feldspar_syenitic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #quartz_alkali_feldspar_trachyte -->

    <skos:Concept rdf:about="#quartz_alkali_feldspar_trachyte">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Alkali feldspar trachytic rock that contains and between 5 and 20 percent quartz  mineral in the QAPF fraction. QAPF field 6*.</skos:definition>
        <skos:historyNote xml:lang="en"
            >Le Maitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Quartz alkali feldspar trachyte</skos:prefLabel>
        <skos:broader rdf:resource="#alkali_feldspar_trachytic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #quartz_anorthosite -->

    <skos:Concept rdf:about="#quartz_anorthosite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote xml:lang="en"
            >Anorthositic rock that contains between 5 and 20 percent quartz in the QAPF fraction. QAPF field 10*.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Quartz anorthosite</skos:prefLabel>
        <skos:broader rdf:resource="#anorthositic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #quartz_diorite -->

    <owl:Thing rdf:about="#quartz_diorite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Dioritic rock that contains between 5 to 20 percent quartz in the QAPF fraction. QAPF field 10*.</skos:definition>
        <skos:definition xml:lang="en"
            >LeMaitre et al., 2002</skos:definition>
        <skos:prefLabel xml:lang="en">Quartz diorite</skos:prefLabel>
        <skos:broader rdf:resource="#dioritic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #quartz_gabbro -->

    <skos:Concept rdf:about="#quartz_gabbro">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Gabbroic rock that contains between 5 and 20 percent quartz in the QAPF fraction. QAPF field 10*.</skos:definition>
        <skos:historyNote xml:lang="en"
            >LeMaitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Quartz gabbro</skos:prefLabel>
        <skos:broader rdf:resource="#gabbroic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #quartz_latite -->

    <owl:Thing rdf:about="#quartz_latite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Latitic rock that contains between 5 and 20 percent quartz in the QAPF fraction. QAPF field 8*.</skos:definition>
        <skos:historyNote xml:lang="en"
            >Le Maitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Quartz latite</skos:prefLabel>
        <skos:broader rdf:resource="#latitic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #quartz_monzodiorite -->

    <skos:Concept rdf:about="#quartz_monzodiorite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote xml:lang="en"
            >LeMaitre et al., 2002</skos:historyNote>
        <skos:definition xml:lang="en"
            >Monzodioritic rock that contains between 5 and 20 percent quartz.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Quartz monzodiorite</skos:prefLabel>
        <skos:broader rdf:resource="#monzodioritic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #quartz_monzogabbro -->

    <owl:Thing rdf:about="#quartz_monzogabbro">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote xml:lang="en"
            >LeMaitre et al., 2002</skos:historyNote>
        <skos:definition xml:lang="en"
            >Monzogabbroic rock that contains between 5 and 20 percent quartz in the QAPF fraction. QAPF field 9*.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Quartz monzogabbro</skos:prefLabel>
        <skos:broader rdf:resource="#monzogabbroic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #quartz_monzonite -->

    <owl:Thing rdf:about="#quartz_monzonite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition xml:lang="en"
            >Monzonitic rock that contains 5-20 percent quartz iin the QAPF fraction. Includes rocks defined modally in QAPF Field 8*.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Quartz monzonite</skos:prefLabel>
        <skos:broader rdf:resource="#monzonitic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #quartz_rich_igneous_rock -->

    <owl:Thing rdf:about="#quartz_rich_igneous_rock">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote
            >Gillespie and Styles 1999; LeMaitre et al. 2002</skos:historyNote>
        <rdfs:comment xml:lang="en"
            >Occurrence of igneous rocks meeting this criteria seems to be vanishingly rare, thus subdividing the category does not seem warranted for the purposes of this vocabulary. Future usage of the vocabulary may motivate including quatzolite and quartz-rich granitoid in future revisions</rdfs:comment>
        <skos:definition xml:lang="en"
            >Phaneritic crystalline igneous rock that contains less than 90 percent mafic minerals and contains greater than 60 percent quartz in the QAPF fraction.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Quartz rich igneous rock</skos:prefLabel>
        <rdfs:seeAlso xml:lang="en"
            >quartzolite, quartz-rich granitoid</rdfs:seeAlso>
        <skos:broader rdf:resource="#acidic_igneous_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #quartz_syenite -->

    <owl:Thing rdf:about="#quartz_syenite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:prefLabel xml:lang="en">Quartz syenite</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Syenitic rock that contains between 5 and 20 percent quartz in the QAPF fraction. Defined modally in QAPF Field 7*.</skos:definition>
        <skos:broader rdf:resource="#syenitic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #quartz_trachyte -->

    <owl:Thing rdf:about="#quartz_trachyte">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote xml:lang="en"
            >Le Maitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Quartz trachyte</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Trachytic rock that contains between 5 and 20 percent quartz in the QAPF fraction. QAPF field 7*.</skos:definition>
        <skos:broader rdf:resource="#trachytic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #quartzite -->

    <skos:Concept rdf:about="#quartzite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Metamorphic rock consisting of greater than or equal to 75 percent quartz; typically granoblastic texture.</skos:definition>
        <skos:prefLabel xml:lang="en">Quartzite</skos:prefLabel>
        <skos:historyNote
            >after Neuendorf et al. 2005</skos:historyNote>
        <skos:broader rdf:resource="#metamorphic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #residual_material -->

    <skos:Concept rdf:about="#residual_material">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Material of composite origin resulting from weathering processes at the Earths surface, in the absence of significant epiclastic or chemical input, mostly involving removal of chemical constituents by aqueous leaching. Consolidation state is not inherent in definition. Minor input from wind-born constituents may be present</skos:definition>
        <skos:prefLabel xml:lang="en"
            >residual material</skos:prefLabel>
        <skos:historyNote xml:lang="en">this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="#material_formed_in_surficial_environment"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #rhyolite -->

    <owl:Thing rdf:about="#rhyolite">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote xml:lang="en"
            >LeMaitre et al., 2002</skos:historyNote>
        <rdfs:seeAlso xml:lang="en"
            >liparite, rhyodacite</rdfs:seeAlso>
        <skos:prefLabel xml:lang="en">rhyolite</skos:prefLabel>
        <skos:definition xml:lang="en"
            >rhyolitoid in which the ratio of plagioclase to alkali feldspar is between 0.1 and 0.65.</skos:definition>
        <skos:broader rdf:resource="#rhyolitoid"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #rhyolitoid -->

    <skos:Concept rdf:about="#rhyolitoid">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <rdfs:comment xml:lang="en"
            >Note that technical definition, based on modal mineralogy plotted in a QAPF triangle may be applied qualitatively, based on phenocryst mineralogy when ground mass mineralogy can not be determined optically, or based on CIPW norm. Although TAS categories are defined based on chemical analyses, the correspondence with the QAPF defined categories is generally close enough that QAPF categories are commonly used interchangeably with TAS categories. It is important to note the basis for assignment of fine-grained igneous rocks to a specifice lithology category.</rdfs:comment>
        <skos:altLabel xml:lang="en">Rhyolitic rock</skos:altLabel>
        <skos:prefLabel xml:lang="en">Rhyolitoid</skos:prefLabel>
        <skos:definition xml:lang="en"
            >fine_grained_igneous_rock consisting of quartz and alkali feldspar, with minor plagioclase and biotite, in a microcrystalline, cryptocrystalline or glassy groundmass. Flow texture is common. Includes rocks defined modally in QAPF fields 2 and 3 or chemically in TAS Field R as rhyolite. QAPF normative definition is based on modal mineralogy thus: less than 90 percent mafic minerals, between 10 and 60 percent quartz in the QAPF fraction, and ratio of plagioclse to alkali feldspar is less than 0.65.</skos:definition>
        <skos:broader rdf:resource="#acidic_igneous_rock"/>
        <skos:narrower rdf:resource="#alkali_feldspar_rhyolite"/>
        <skos:broader rdf:resource="#fine_grained_igneous_rock"/>
        <skos:narrower rdf:resource="#rhyolite"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #rock -->

    <skos:Concept rdf:about="#rock">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">rock</skos:notation>
        <skos:historyNote
            >Jackson, 1997; NADM C1 2004; Neuendorf et al 2005</skos:historyNote>
        <skos:definition
            >Consolidated aggregate of one or more EarthMaterials, or a body of undifferentiated mineral matter, or of solid organic material. Includes mineral aggregates such as granite, shale, marble; glassy matter such as obsidian; and organic material such a coal. Excludes unconsolidated materials.</skos:definition>
        <skos:prefLabel xml:lang="en">Rock</skos:prefLabel>
        <skos:prefLabel xml:lang="es">piedra</skos:prefLabel>
        <skos:broader rdf:resource="#compound_material"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #sand -->

    <owl:Thing rdf:about="#sand">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">sand</skos:notation>
        <skos:definition
            >Clastic sediment in which less than 30 percent of particles are gravel (greater than 2 mm in diameter) and the sand to mud ratio is at least 1.</skos:definition>
        <skos:historyNote
            >Neuendorf et al. 2005; NGMDB</skos:historyNote>
        <skos:prefLabel xml:lang="en">Sand</skos:prefLabel>
        <skos:broader rdf:resource="#clastic_sediment"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #sandstone -->

    <owl:Thing rdf:about="#sandstone">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">sandstone</skos:notation>
        <skos:definition
            >Clastic sedimentary rock in which less than 30 percent of particles are greater than 2 mm in diameter (gravel) and the sand to mud ratio is at least 1.</skos:definition>
        <skos:historyNote
            >SLTTs 2004; Neuendorf et al. 2005; particle size from Wentworth grade scale</skos:historyNote>
        <skos:prefLabel xml:lang="en">Sandstone</skos:prefLabel>
        <skos:broader rdf:resource="#clastic_sedimentary_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #schist -->

    <owl:Thing rdf:about="#schist">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote
            >SLTTm 2004; Neuendorf et al. 2005</skos:historyNote>
        <skos:definition
            >Foliated phaneritic metamorphic rock with well developed, continuous schistosity (greater than 50 percent of mineral grains with a tabular, lamellar, or prismatic crystallographic habit that are oriented in a continuous planar or linear fabric).</skos:definition>
        <skos:prefLabel xml:lang="en">Schist</skos:prefLabel>
        <skos:broader rdf:resource="#foliated_metamorphic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #sediment -->

    <skos:Concept rdf:about="#sediment">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">sediment</skos:notation>
        <skos:prefLabel xml:lang="en">Sediment</skos:prefLabel>
        <skos:definition
            >Unconsolidated material consisting of an aggregation of particles transported or deposited by air, water or ice, or that accumulated by other natural agents, such as chemical precipitation, and that forms in layers on the Earth&#39;s surface. Includes epiclastic deposits.</skos:definition>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:broader rdf:resource="#natural_unconsolidated_material"/>
        <skos:broader rdf:resource="#sedimentary_material"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #sedimentary_material -->

    <owl:Thing rdf:about="#sedimentary_material">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:definition
            >Material formed by accumulation of solid fragmental material deposited by air, water or ice, or material that accumulated by other natural agents such as chemical precipitation from solution or secretion by organisms. Includes both sediment and sedimentary rock. Includes epiclastic deposits.</skos:definition>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Sedimentary material</skos:prefLabel>
        <skos:broader rdf:resource="#compound_material"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #sedimentary_rock -->

    <skos:Concept rdf:about="#sedimentary_rock">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx"
            >sedimentary_rock</skos:notation>
        <skos:historyNote>SLTTs 2004</skos:historyNote>
        <skos:definition
            >Rock formed by accumulation and cementation of solid fragmental material deposited by air, water or ice, or as a result of other natural agents, such as precipitation from solution, the accumulation of organic material, or from biogenic processes, including secretion by organisms. Includes epiclastic deposits.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Sedimentary rock</skos:prefLabel>
        <skos:broader rdf:resource="#rock"/>
        <skos:broader rdf:resource="#sedimentary_material"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #serpentinite -->

    <skos:Concept rdf:about="#serpentinite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition
            >Rock consisting of more than 75 percent serpentine-group minerals, eg. antigorite, chrysotile or lizardite; accessory chlorite, talc and magnetite may be present; derived from hydration of ferromagnesian silicate minerals such as olivine and pyroxene.</skos:definition>
        <skos:historyNote
            >Neuendorf et al. 2005</skos:historyNote>
        <skos:prefLabel xml:lang="en">Serpentinite</skos:prefLabel>
        <skos:broader rdf:resource="#metamorphic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #shale -->

    <owl:Thing rdf:about="#shale">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:notation rdf:datatype="&SimpleLithology2;xx">shale</skos:notation>
        <skos:historyNote
            >NADM SLTT sedimentary; Neuendorf et al. 2005</skos:historyNote>
        <skos:definition
            >Laminated mudstone that will part or break along thin, closely spaced layers parallel to stratification.</skos:definition>
        <skos:prefLabel xml:lang="en">Shale</skos:prefLabel>
        <skos:broader rdf:resource="#mudstone"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #silicate_mud -->

    <skos:Concept rdf:about="#silicate_mud">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">silicate mud</skos:prefLabel>
        <skos:broader rdf:resource="#mud"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #silt -->

    <skos:Concept rdf:about="#silt">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Mud that consists of greater than 50 percent silt-size grains.</skos:definition>
        <skos:historyNote xml:lang="en"
            >based on SLTTs 2004; Neuendorf et al. 2005; particle size from Wentworth grade scale</skos:historyNote>
        <skos:altLabel xml:lang="en">loess</skos:altLabel>
        <skos:prefLabel xml:lang="en">silt</skos:prefLabel>
        <skos:broader rdf:resource="#mud"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #slate -->

    <skos:Concept rdf:about="#slate">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >NADM metamorphic rock vocabulary SLTTm1.0; Neuendorf et al. 2005</skos:historyNote>
        <skos:definition
            >Compact, fine grained rock with an average grain size less than 0.032 millimeter and a well developed schistosity (slaty cleavage), and hence can be split into slabs or thin plates.</skos:definition>
        <skos:prefLabel xml:lang="en">Slate</skos:prefLabel>
        <skos:broader rdf:resource="#foliated_metamorphic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #syenite -->

    <skos:Concept rdf:about="#syenite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Syenite</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Syenitic rock that contains between 0 and 5 percent quartz and no feldspathoid mineral in the QAPF fraction. Defined modally in QAPF Field 7.</skos:definition>
        <skos:broader rdf:resource="#syenitic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #syenitic_rock -->

    <owl:Thing rdf:about="#syenitic_rock">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Syenitic rock</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Syenitoid with a plagioclase to alkali feldspar ratio between 0.1 and 0.35.  Includes rocks in QAPF fields 7, 7*, and 7&#39;.</skos:definition>
        <skos:narrower rdf:resource="#foid_bearing_syenitic_rock"/>
        <skos:narrower rdf:resource="#quartz_syenite"/>
        <skos:broader rdf:resource="#syenitoid"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #syenitoid -->

    <owl:Thing rdf:about="#syenitoid">
        <rdf:type rdf:resource="&skos;Concept"/>
        <skos:historyNote xml:lang="en"
            >LeMaitre et al., 2002</skos:historyNote>
        <skos:definition xml:lang="en"
            >Phaneritic crystalline igneous rock with M less than 90, consisting mainly of alkali feldspar and plagioclase; minor quartz or nepheline may be present, along with pyroxene, amphibole or biotite. Ratio of plagioclase to alkali feldspar is less than 0.65, quartz forms less than 20 percent of QAPF fraction, and feldspathoid minerals form less than 10 percent of QAPF fraction.  Includes rocks classified in QAPF fields 6, 7 and 8 and their subdivisions.</skos:definition>
        <skos:prefLabel xml:lang="en">Syenitoid</skos:prefLabel>
        <skos:narrower rdf:resource="#alkali_feldspar_syenitic_rock"/>
        <skos:narrower rdf:resource="#monzonitic_rock"/>
        <skos:broader rdf:resource="#phaneritic_igneous_rock"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- #syenogranite -->

    <skos:Concept rdf:about="#syenogranite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Granite that has a plagiolcase to alkali feldspar ratio between 0.10 and 0.35. QAPF field 3a.</skos:definition>
        <skos:historyNote xml:lang="en"
            >LeMaitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Syenogranite</skos:prefLabel>
        <skos:broader rdf:resource="#granite"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #tephra -->

    <skos:Concept rdf:about="#tephra">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >Hallsworth &amp; Knox 1999; LeMaitre et al. 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Tephra</skos:prefLabel>
        <skos:definition
            >Unconsolidated pyroclastic material in which greater than 75 percent of the fragments are deposited as a direct result of volcanic processes and the deposit has not been reworked by epiclastic processes. Includes ash, lapilli tephra, bomb tephra, block tephra and unconsolidated agglomerate.</skos:definition>
        <skos:broader rdf:resource="#natural_unconsolidated_material"/>
        <skos:broader rdf:resource="#pyroclastic_material"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #tephrite -->

    <skos:Concept rdf:about="#tephrite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:prefLabel xml:lang="en">Tephrite</skos:prefLabel>
        <skos:historyNote xml:lang="en"
            >Tephritoid that has a plagioclase to alkali feldspar ratio greater than 0.9, and contains less than 10 percent normative (CIPW) olivine.</skos:historyNote>
        <skos:broader rdf:resource="#tephritoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #tephritic_foidite -->

    <skos:Concept rdf:about="#tephritic_foidite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote xml:lang="en"
            >Foiditoid that contains less than 90 percent feldspathoid minerals in the QAPF fraction, and has a plagioclase to alkali feldspar ratio greater than 0.5, and contains less than 10 precent normative (CIPW) olivine.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Tephritic foidite</skos:prefLabel>
        <skos:broader rdf:resource="#foiditoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #tephritic_phonolite -->

    <skos:Concept rdf:about="#tephritic_phonolite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote xml:lang="en"
            >LeMaitre et al., 2002</skos:historyNote>
        <skos:definition xml:lang="en"
            >Phonolitoid that has a plagioclase to alkali feldspar ratio between 0.1 and 0.5. Broadly corresponds to TAS tephriphonolite of TAS field U3.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Tephritic phonolite</skos:prefLabel>
        <rdfs:seeAlso xml:lang="en">tephriphonolite</rdfs:seeAlso>
        <skos:broader rdf:resource="#phonolitoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #tephritoid -->

    <skos:Concept rdf:about="#tephritoid">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:definition xml:lang="en"
            >Fine grained igneous rock than contains less than 90 percent mafic minerals, between 10 and 60 percent feldspathoid mineral in the QAPF fraction and has a plagioclase to alkali feldspar ratio greater than 0.5.  Includes rocks classified in QAPF field 13 and 14 or chemically in TAS field U1 as basanite or tephrite.</skos:definition>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <skos:historyNote xml:lang="en"
            >Le Maitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Tephritic rock</skos:prefLabel>
        <skos:narrower rdf:resource="#basanite"/>
        <skos:broader rdf:resource="#fine_grained_igneous_rock"/>
        <skos:narrower rdf:resource="#phonolitic_basanite"/>
        <skos:narrower rdf:resource="#phonolitic_tephrite"/>
        <skos:narrower rdf:resource="#tephrite"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #tonalite -->

    <skos:Concept rdf:about="#tonalite">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <skos:definition
            >Phaneritic crystalline rock consisting of quartz and intermediate plagioclase, usually with biotite and amphibole. Includes rocks defined modally in QAPF field 5.</skos:definition>
        <skos:prefLabel xml:lang="en">Tonalite</skos:prefLabel>
        <rdfs:seeAlso xml:lang="en"
            >trondhjemite, plagiogranite</rdfs:seeAlso>
        <skos:broader rdf:resource="#granitic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #trachyte -->

    <skos:Concept rdf:about="#trachyte">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote xml:lang="en"
            >Le Maitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Trachyte</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Trachytoid that has a plagioclase to alkali feldspar ratio between 0.1 and 0.35,  between 0 and 5 percent quartz in the QAPF fraction, and no feldspathoid minerals. QAPF field 7.</skos:definition>
        <skos:broader rdf:resource="#trachytic_rock"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #trachytic_rock -->

    <skos:Concept rdf:about="#trachytic_rock">
        <rdf:type rdf:resource="&owl;Thing"/>
        <rdfs:comment xml:lang="en"
            >Le Maitre et al. (2002) used &#39;trachyte&#39; to refer to QAPF fields 7, 7&#39;, and 7* in the text (p. 30) as well as to the more restrictive category (QAPF field 7 only). The term Trachytic rock is introduced here to label this more general category of trachyte.</rdfs:comment>
        <skos:historyNote xml:lang="en"
            >Le Maitre et al., 2002</skos:historyNote>
        <skos:prefLabel xml:lang="en">Trachytic rock</skos:prefLabel>
        <skos:definition xml:lang="en"
            >Trachytoid that has a plagioclase to alkali feldspar ratio between 0.1 and 0.35. QAPF fields 7, 7&#39;, and 7*.</skos:definition>
        <skos:narrower rdf:resource="#foid_bearing_trachyte"/>
        <skos:narrower rdf:resource="#quartz_trachyte"/>
        <skos:broader rdf:resource="#trachytoid"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- #trachytoid -->

    <skos:Concept rdf:about="#trachytoid">
        <rdf:type rdf:resource="&owl;Thing"/>
        <skos:historyNote
            >LeMaitre et al. 2002</skos:historyNote>
        <skos:definition xml:lang="en"
