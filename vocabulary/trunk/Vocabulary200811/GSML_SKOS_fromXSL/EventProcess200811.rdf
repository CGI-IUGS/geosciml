    <!-- urn:cgi:classifier:CGI:EventProcess:200811#accretion -->

    <owl:Thing rdf:about="&EventProcess200811;accretion">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">10.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">accretion</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >The addition of material to a continent. Typically involves convergent or transform motion.</skos:definition>
        <skos:prefLabel xml:lang="en">accretion</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;tectonic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#alteration -->

    <skos:Concept rdf:about="&EventProcess200811;alteration">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">11.3.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">alteration</skos:notation>
        <skos:historyNote
            >Fettes and Desmons, 2007</skos:historyNote>
        <skos:definition
            >General term for any change in the mineralogical or chemical composition of a rock. Typically related to interaction with hydrous fluids.</skos:definition>
        <skos:prefLabel xml:lang="en">alteration</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;metamorphic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#biological_precipitation -->

    <skos:Concept rdf:about="&EventProcess200811;biological_precipitation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">06.2.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >biological_precipitation</skos:notation>
        <skos:prefLabel xml:lang="en"
            >biological precipitation</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >the deposition of minerals from solution by the agency of organisms</skos:definition>
        <skos:broader rdf:resource="&EventProcess200811;deposition"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#biological_weathering -->

    <owl:Thing rdf:about="&EventProcess200811;biological_weathering">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.3.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >biological_weathering</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >biological weathering</skos:prefLabel>
        <skos:definition
            >breakdown of rocks by biological agents, e.g. the penetrating and expanding force of roots, the presence of moss and lichen causing humic acids to be retained in contact with rock, and the work of animals (worms, moles, rabbits) in modifying surface soil</skos:definition>
        <skos:broader rdf:resource="&EventProcess200811;weathering"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#bolide_impact -->

    <owl:Thing rdf:about="&EventProcess200811;bolide_impact">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">15..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">bolide_impact</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">bolide impact</skos:prefLabel>
        <skos:definition
            >the impact of an extraterrestrial body on the surface of the earth</skos:definition>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#chemical_precipitation -->

    <owl:Thing rdf:about="&EventProcess200811;chemical_precipitation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">06.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >chemical_precipitation</skos:notation>
        <skos:definition
            >the deposition of mineral matter by precipitation from solution, or the accumulation of organic material on the death of plants and animals.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >chemical precipitation</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;deposition"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#chemical_weathering -->

    <owl:Thing rdf:about="&EventProcess200811;chemical_weathering">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >chemical_weathering</skos:notation>
        <skos:definition
            >The process of weathering by which chemical reactions (hydrolysis, hydration, oxidation, carbonation, ion exchange, and solution) transform rocks and minerals into new chemical combinations that are stable under conditions prevailing at or near the Earth&#39;s surface; e.g. the alteration of orthoclase to kaolinite.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >chemical weathering</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;weathering"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#cometary_impact -->

    <owl:Thing rdf:about="&EventProcess200811;cometary_impact">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">15.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">cometary_impact</skos:notation>
        <skos:prefLabel xml:lang="en">cometary impact</skos:prefLabel>
        <skos:definition
            >the impact of a comet on the surface of the earth</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;bolide_impact"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#contact_metamorphism -->

    <skos:Concept rdf:about="&EventProcess200811;contact_metamorphism">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">11.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >contact_metamorphism</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >Metamorphism taking place in rocks at or near their contact with a genetically related body of igneous rock</skos:definition>
        <skos:prefLabel xml:lang="en"
            >contact metamorphism</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;metamorphic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#continental_breakup -->

    <skos:Concept rdf:about="&EventProcess200811;continental_breakup">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">10.3.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >continental_breakup</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Fragmentation of a continental plate into two or more smaller plates; may involve rifting or strike slip faulting.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >continental breakup</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;tectonic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#continental_collision -->

    <skos:Concept rdf:about="&EventProcess200811;continental_collision">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">10.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >continental_collision</skos:notation>
        <skos:definition
            >The amalgamation of two continental plates or blocks along a convergent margin.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >continental collision</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;tectonic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#creep -->

    <skos:Concept rdf:about="&EventProcess200811;creep">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.4.5</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">creep</skos:notation>
        <skos:definition
            >Process of slow, more or less continuous downslope movement of mineral, rock, and soil particles under gravitational stresses. (Neuendorf et al 2005). Typically loose material produced by weathering and erosion is transported and deposited by tumbling, rolling, settling, particle creep, under the influence of gravitational forces with or without the assistance of aqueous slope wash. (NADM SLTTs, 2004)</skos:definition>
        <skos:historyNote>NADM SLTTs 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en">creep</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;mass_wasting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#debris_flow -->

    <owl:Thing rdf:about="&EventProcess200811;debris_flow">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.4.4</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">debris_flow</skos:notation>
        <skos:definition
            >Process of downslope transport of silt to boulder size particles in a matrix of clay-grade fines and water, characterized by a matrix that sufficient has strength and buoyancy to support grains; particle support also by intergranular collision; matrix &#39;lubricates&#39; moving sediment mass. Leeder, 1982; p76</skos:definition>
        <skos:historyNote>Leeder, 1982</skos:historyNote>
        <skos:prefLabel xml:lang="en">debris flow</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;mass_wasting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#deep_water_oxygen_depletion -->

    <owl:Thing rdf:about="&EventProcess200811;deep_water_oxygen_depletion">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">03..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >deep_water_oxygen_depletion</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Process of removal of oxygen from from the deep part of a body of water.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >deep water oxygen depletion</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#deformation -->

    <owl:Thing rdf:about="&EventProcess200811;deformation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">deformation</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >Movement of rock bodies by displacement on fault or shear zones, or change in shape of a body of Earth material.</skos:definition>
        <skos:prefLabel xml:lang="en">deformation</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#deformation_twinning -->

    <skos:Concept rdf:about="&EventProcess200811;deformation_twinning">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12.3.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >deformation_twinning</skos:notation>
        <skos:definition
            >Deformation of a crystal by gliding to produce crystallographic twinning.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >deformation twinning</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;ductile_flow"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#deposition -->

    <skos:Concept rdf:about="&EventProcess200811;deposition">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">06..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">deposition</skos:notation>
        <skos:definition
            >Accumulation of material; the constructive process of accumulation of sedimentary particles, chemical precipitation of mineral matter from solution, or the accumulation of organic material on the death of plants and animals.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">deposition</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#diagenetic_process -->

    <skos:Concept rdf:about="&EventProcess200811;diagenetic_process">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">07..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >diagenetic_process</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >Any chemical, physical, or biological process that affects a sedimentary EarthMaterial after initial deposition, and during or after lithification, exclusive of weathering and metamorphism. [adapt. Jackson, 1997] Example processes include compaction, cementation, authigenesis, replacement, leaching, hydration, and bacterial action. Includes processes that are normal in the surficial or outer part of the earth&#8217;s crust [Jackson, 1997]. Changes in a deeply buried sedimentary rock may be continuous from diagenesis into recrystallization to form a metamorphic rock. Robertson [1999] defines the boundary between diagenesis and metamorphism in sedimentary rocks as follows: &quot;the boundary between diagenesis and metamorphism is somewhat arbitrary and strongly dependent on the rock types involved. For example changes take place in organic materials at lower temperatures than in rocks dominated by silicate minerals. In mudrocks, a white mica (illite) crystallinity value of less than 0.42D.2U obtained by X-ray diffraction analysis, is used to define the onset of metamorphism (Kisch, 1991). In this scheme, the first appearance of glaucophane, lawsonite, paragonite, prehnite, pumpellyite or stilpnomelane is taken to indicate the lower limit of metamorphism (Frey and Kisch, 1987; Bucher and Frey, 1994; Frey and Robinson, 1998). Most workers agree that such mineral growth starts at 150 &#177; 50&#176; C in silicate rocks. Many rock types may show no change in mineralogy under these conditions and hence the recognition of the onset of metamorphism will vary with bulk composition.&quot;</skos:definition>
        <skos:prefLabel xml:lang="en"
            >Diagenetic process</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#diffusion_creep -->

    <skos:Concept rdf:about="&EventProcess200811;diffusion_creep">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12.3.2</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">diffusion_creep</skos:notation>
        <skos:definition
            >Grain-scale, ductile deformation accomplished by the motion of atoms through crystals, along grain boundaries, and through pore fluids.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">diffusion creep</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;ductile_flow"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#dislocation_metamorphism -->

    <owl:Thing rdf:about="&EventProcess200811;dislocation_metamorphism">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">11.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >dislocation_metamorphism</skos:notation>
        <skos:definition
            >Metamorphism concentrated along narrow belts of shearing or crushing without an appreciable rise in temperature</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >dislocation metamorphism</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;metamorphic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#dissolution -->

    <skos:Concept rdf:about="&EventProcess200811;dissolution">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.2.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">dissolution</skos:notation>
        <skos:definition
            >The process of dissolving into a homogenous solution, as when an acidic solution dissolves limestone. In karst, refers to the process of dissolving rock to produce landforms, in contrast to solution, the chemical product of dissolution.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">dissolution</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;chemical_weathering"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#dissolution_creep -->

    <owl:Thing rdf:about="&EventProcess200811;dissolution_creep">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12.3.3</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >dissolution_creep</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >Deformation by dissolution under the effects of differential stress and its transport to a new location by movement of fluid in the rock body.</skos:definition>
        <skos:prefLabel xml:lang="en"
            >dissolution creep</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;ductile_flow"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#ductile_flow -->

    <owl:Thing rdf:about="&EventProcess200811;ductile_flow">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12.3.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">ductile_flow</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >deformation without apparent loss of continuity at the scale of observation.</skos:definition>
        <skos:prefLabel xml:lang="en">ductile flow</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;deformation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#effusive_eruption -->

    <owl:Thing rdf:about="&EventProcess200811;effusive_eruption">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09.2.4</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >effusive_eruption</skos:notation>
        <skos:definition
            >Eruptions characterized by low volatile content of the erupting magma relative to ambient pressure</skos:definition>
        <skos:historyNote
            >Orton, G.J., 1996. Volcanic environments. 485-567 in Reading, H.G. (ed.), Sedimentary Environments: Processes, Facies and Stratigraphy, Third Edition. Blackwell Science, Oxford</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >effusive eruption</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;eruption"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#erosion -->

    <owl:Thing rdf:about="&EventProcess200811;erosion">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">erosion</skos:notation>
        <skos:definition
            >The process of disaggregation of rock and displacement of the resultant particles (sediment) usually by the agents of currents such as, wind, water, or ice by downward or down-slope movement in response to gravity or by living organisms (in the case of bioerosion).</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">erosion</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;sedimentary_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#eruption -->

    <owl:Thing rdf:about="&EventProcess200811;eruption">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">eruption</skos:notation>
        <skos:definition
            >The ejection of volcanic materials (lava, pyroclasts, and volcanic gases) onto the Earth&#39;s surface, either from a central vent or from a fissure or group of fissures</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">eruption</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;magmatic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#excavation -->

    <owl:Thing rdf:about="&EventProcess200811;excavation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">14.3.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">excavation</skos:notation>
        <skos:definition
            >removal of material, as in a mining operation</skos:definition>
        <skos:prefLabel xml:lang="en">excavation</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;human_activity"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#extinction -->

    <skos:Concept rdf:about="&EventProcess200811;extinction">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">01..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">extinction</skos:notation>
        <skos:definition
            >Process of disappearance of a species or higher taxon, so that it no longer exists anywhere or in the subsequent fossil record.</skos:definition>
        <skos:prefLabel xml:lang="en">extinction</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#fall -->

    <skos:Concept rdf:about="&EventProcess200811;fall">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.4.2</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">fall</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >The relatively free falling or precipitous movement of a newly detached segment of bedrock (usually massive, homogeneous, or jointed) of any size from a cliff or other very steep slope; it is the fastest form of mass movement and is most frequent in mountain areas and during spring when there is repeated freezing and thawing of water in cracks in the rock. Movement may be straight down, or in a series of leaps and bounds down the slope; it is not guided by an underlying slip surface</skos:definition>
        <skos:prefLabel xml:lang="en">fall</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;mass_wasting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#faulting -->

    <skos:Concept rdf:about="&EventProcess200811;faulting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12.1.2</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">faulting</skos:notation>
        <skos:definition
            >The process of fracturing, frictional slip, and displacement accumulation that produces a fault</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">faulting</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;deformation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#folding -->

    <owl:Thing rdf:about="&EventProcess200811;folding">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12.3.4</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">folding</skos:notation>
        <skos:definition
            >deformation in which planar surfaces become regularly curviplanar surfaces with definable limbs (zones of lower curvature) and hinges (zones of higher curvature).</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">folding</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;deformation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#fracturing -->

    <skos:Concept rdf:about="&EventProcess200811;fracturing">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">fracturing</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >The formation of a surface of failure resulting from stress</skos:definition>
        <skos:prefLabel xml:lang="en">fracturing</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;deformation"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#frost_shattering -->

    <owl:Thing rdf:about="&EventProcess200811;frost_shattering">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.1.3</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >frost_shattering</skos:notation>
        <skos:definition
            >Propagation of fractures due to expansion of freezing water in intergranular spaces and fractures in a rock body. Result is mechanical disintegration spliitting, or breakup of rock.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >frost shattering</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;physical_weathering"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#geologic_process -->

    <skos:Concept rdf:about="&EventProcess200811;geologic_process">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">0..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >geologic_process</skos:notation>
        <skos:definition
            >process that effects the geologic record</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >Geologic Process</skos:prefLabel>
        <skos:topConceptOf rdf:resource=""/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#geomagnetic_process -->

    <owl:Thing rdf:about="&EventProcess200811;geomagnetic_process">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">08..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >geomagnetic_process</skos:notation>
        <skos:prefLabel xml:lang="en"
            >geomagnetic process</skos:prefLabel>
        <skos:definition
            >process that results in change in Earth&#39;s magnetic field</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#grading -->

    <owl:Thing rdf:about="&EventProcess200811;grading">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">14.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">grading</skos:notation>
        <skos:definition
            >leveling of earth surface by rearrangement of prexisting material</skos:definition>
        <skos:prefLabel xml:lang="en">grading</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;human_activity"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#grain_flow -->

    <owl:Thing rdf:about="&EventProcess200811;grain_flow">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.4.6</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">grain_flow</skos:notation>
        <skos:historyNote>Leeder, 1982</skos:historyNote>
        <skos:definition
            >Process of downslope movement of a mass of sediment particles set in motion by gravity when depositional slope exceeds a critical angle; flow characterized by grain-grain collisions. Leeder 1982, p76</skos:definition>
        <skos:prefLabel xml:lang="en">grain flow</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;mass_wasting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#haloclasty -->

    <owl:Thing rdf:about="&EventProcess200811;haloclasty">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.1.4</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">haloclasty</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">haloclasty</skos:prefLabel>
        <skos:definition
            >propagation of fractures in rock due to crytallization of mineral salts (typically sodium chloride) from interstitial water, or volumetrick expansion of salts in capillaries, or hydration pressure of interstitial, trapped salts. Generally results in mechanical disintegration of the rock surface.</skos:definition>
        <skos:broader rdf:resource="&EventProcess200811;physical_weathering"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#hawaiian_eruption -->

    <owl:Thing rdf:about="&EventProcess200811;hawaiian_eruption">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09.2.2</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >hawaiian_eruption</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >Eruption in which great quantities of extremely fluid basaltic lava are poured out, mainly issuing in lava fountains from fissures on the flanks of a volcano. Explosive phenomena are rare, but much spatter and scoria are piled into cones and mounds along the vents. Characteristic of shield volcanoes</skos:definition>
        <skos:prefLabel xml:lang="en"
            >hawaiian eruption</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;eruption"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#human_activity -->

    <skos:Concept rdf:about="&EventProcess200811;human_activity">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">14..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">human_activity</skos:notation>
        <skos:definition
            >processes of human modification of the earth to produce geologic features</skos:definition>
        <skos:prefLabel xml:lang="en">Human activity</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#hydration -->

    <skos:Concept rdf:about="&EventProcess200811;hydration">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.2.2</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">hydration</skos:notation>
        <skos:definition
            >The process of absorption of water into the crystal structure of a mineral, thereby changing its volume and fracturing and loosening grains</skos:definition>
        <skos:prefLabel xml:lang="en">hydration</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;chemical_weathering"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#hydrolysis -->

    <owl:Thing rdf:about="&EventProcess200811;hydrolysis">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.2.3</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">hydrolysis</skos:notation>
        <skos:definition
            >A decomposition reaction involving water. In geology, it commonly indicates reaction between silicate minerals and either pure water or aqueous solution. In such reactions, H</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">hydrolysis</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;chemical_weathering"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#ice_erosion -->

    <skos:Concept rdf:about="&EventProcess200811;ice_erosion">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.3</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">ice_erosion</skos:notation>
        <skos:definition
            >Erosion by corrasion or plucking by moving ice.</skos:definition>
        <skos:prefLabel xml:lang="en">ice erosion</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;erosion"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#intrusion -->

    <owl:Thing rdf:about="&EventProcess200811;intrusion">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">intrusion</skos:notation>
        <skos:definition
            >The process of emplacement of magma in pre-existing rock</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">intrusion</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;magmatic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#magmatic_crystallisation -->

    <owl:Thing rdf:about="&EventProcess200811;magmatic_crystallisation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09.4.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >magmatic_crystallisation</skos:notation>
        <skos:definition
            >The process by which matter becomes crystalline, from a gaseous, fluid, or dispersed state</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >magmatic crystallisation</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;magmatic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#magmatic_process -->

    <owl:Thing rdf:about="&EventProcess200811;magmatic_process">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >magmatic_process</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >A process involving melted rock (magma).</skos:definition>
        <skos:prefLabel xml:lang="en"
            >magmatic process</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#magnetic_field_reversal -->

    <skos:Concept rdf:about="&EventProcess200811;magnetic_field_reversal">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">08.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >magnetic_field_reversal</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >geomagnetic event</skos:definition>
        <skos:prefLabel xml:lang="en"
            >magnetic field reversal</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;geomagnetic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#mass_wasting -->

    <owl:Thing rdf:about="&EventProcess200811;mass_wasting">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.4</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">mass_wasting</skos:notation>
        <skos:definition
            >the dislodgement and downslope transport of soil and rock material under the direct application of gravitational body stresses. In contrast to other erosion processes, the debris removed by mass wasting is not carried within, on, or under another medium. The mass properties of the material being transported depend on the interaction of the soil and rock particles and on the moisture content.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">mass wasting</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;erosion"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#material_transport_and_deposition -->

    <skos:Concept rdf:about="&EventProcess200811;material_transport_and_deposition">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">14.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >material_transport_and_deposition</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >transport and heaping of material, as in a land fill, mine dump, dredging operations</skos:definition>
        <skos:prefLabel xml:lang="en"
            >material transport and deposition</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;human_activity"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#mechanical_deposition -->

    <owl:Thing rdf:about="&EventProcess200811;mechanical_deposition">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">06.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >mechanical_deposition</skos:notation>
        <skos:definition
            >process by which material that is being transported as particles by moving air, water, ice, or other fluid comes to rest and accumulates (NADM SLTTs 2004)</skos:definition>
        <skos:historyNote>NADM SLTTs 2004</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >mechanical deposition</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;deposition"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#melting -->

    <skos:Concept rdf:about="&EventProcess200811;melting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09.3.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">melting</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >change of state from a solid to a liquid</skos:definition>
        <skos:prefLabel xml:lang="en">melting</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;magmatic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#metamorphic_process -->

    <owl:Thing rdf:about="&EventProcess200811;metamorphic_process">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">11..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >metamorphic_process</skos:notation>
        <skos:definition
            >Mineralogical, chemical, and structural adjustment of solid rocks to physical and chemical conditions that differ from the conditions under which the rocks in question originated, and are generally been imposed at depth, below the surface zones of weathering and cementation.</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >metamorphic process</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#meteorite_impact -->

    <skos:Concept rdf:about="&EventProcess200811;meteorite_impact">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">15.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >meteorite_impact</skos:notation>
        <skos:prefLabel xml:lang="en"
            >meteorite impact</skos:prefLabel>
        <skos:definition
            >the impact of a meteorite on the surface of the earth</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;bolide_impact"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#microfracturing -->

    <owl:Thing rdf:about="&EventProcess200811;microfracturing">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12.1.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">microfracturing</skos:notation>
        <skos:definition
            >Development of fractures within a single grain or cutting several grains.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">microfracturing</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;fracturing"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#mud_flow -->

    <owl:Thing rdf:about="&EventProcess200811;mud_flow">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.4.3</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">mud_flow</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >a process characterized by a flowing mass of predominantly fine-grained earth material possessing a high degree of fluidity during movement. The water content of mudflows may range up to 60%. The degree of water bonding, determined by the clay content and mineralogy, critically affects the viscosity of the matrix and the velocity and morphology of the flow. (Neuendorf et al 2005)</skos:definition>
        <skos:prefLabel xml:lang="en">mud flow</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;mass_wasting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#oxidation -->

    <skos:Concept rdf:about="&EventProcess200811;oxidation">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.2.4</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">oxidation</skos:notation>
        <skos:definition
            >Chemical reaction that involve stripping of electrons from cations. Typicall reactions include converting sulfide minerals to oxide minerals, or increasing the oxidation state of cations in existing oxide minerals. The most commonly observed is the oxidation of Fe</skos:definition>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">oxidation</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;chemical_weathering"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#partial_melting -->

    <skos:Concept rdf:about="&EventProcess200811;partial_melting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09.3.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">partial_melting</skos:notation>
        <skos:definition
            >Process of melting involving only some of the mineral phases in a rock, to produce a mixture of melt and residual particles.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">partial melting</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;melting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#physical_weathering -->

    <owl:Thing rdf:about="&EventProcess200811;physical_weathering">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >physical_weathering</skos:notation>
        <skos:definition
            >The process of weathering by which frost action, salt-crystal growth, absorption of water, and other physical processes break down a rock to fragments, involving no chemical change</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >physical weathering</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;weathering"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#plinian_eruption -->

    <owl:Thing rdf:about="&EventProcess200811;plinian_eruption">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09.2.1.2</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >plinian_eruption</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >An explosive eruption in which a steady, turbulent stream of fragmented magma and magmatic gas is released at a high velocity from a vent. Large volumes of tephra and tall eruption columns are characteristic</skos:definition>
        <skos:prefLabel xml:lang="en"
            >plinian eruption</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;pyroclastic_eruption"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#polar_wander -->

    <owl:Thing rdf:about="&EventProcess200811;polar_wander">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">08.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">polar_wander</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:prefLabel xml:lang="en">polar wander</skos:prefLabel>
        <skos:definition
            >process of migration of the axis of the earth&#39;s dipole field relative to the rotation axis of the Earth.</skos:definition>
        <skos:broader rdf:resource="&EventProcess200811;geomagnetic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#pressure_release_weathering -->

    <owl:Thing rdf:about="&EventProcess200811;pressure_release_weathering">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">13.1.2</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >pressure_release_weathering</skos:notation>
        <skos:definition
            >propagation of fractures near the surface of solid rock due to expansion related to release of confining pressure when deeply buried rock is unroofed. Fractures typically propagate along surfaces close to and subparallel to the surface of the outcrop.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en"
            >pressure release weathering</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;physical_weathering"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#pyroclastic_eruption -->

    <owl:Thing rdf:about="&EventProcess200811;pyroclastic_eruption">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">09.2.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >pyroclastic_eruption</skos:notation>
        <skos:historyNote
            >Orton, G.J., 1996. Volcanic environments. 485-567 in Reading, H.G. (ed.), Sedimentary Environments: Processes, Facies and Stratigraphy, Third Edition. Blackwell Science, Oxford</skos:historyNote>
        <skos:definition
            >Eruption produced by the generation and rapid expansion of a gas phase that disrupts magma, surrounding wall rock or sediment</skos:definition>
        <skos:prefLabel xml:lang="en"
            >pyroclastic eruption</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;eruption"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#rifting -->

    <skos:Concept rdf:about="&EventProcess200811;rifting">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">10.3.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">rifting</skos:notation>
        <skos:definition
            >Extension of the crust to form one or more long, narrow graben of regional extent.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">rifting</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;continental_breakup"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#sea_level_change -->

    <owl:Thing rdf:about="&EventProcess200811;sea_level_change">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">04..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >sea_level_change</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >process of mean sea level changing relative to some datum</skos:definition>
        <skos:prefLabel xml:lang="en"
            >sea level change</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#sea_level_fall -->

    <owl:Thing rdf:about="&EventProcess200811;sea_level_fall">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">04.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">sea_level_fall</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >process of mean sea level falling relative to some datum</skos:definition>
        <skos:prefLabel xml:lang="en">sea level fall</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;sea_level_change"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#sea_level_rise -->

    <owl:Thing rdf:about="&EventProcess200811;sea_level_rise">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">04.1.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">sea_level_rise</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >process of mean sea level rising relative to some datum</skos:definition>
        <skos:prefLabel xml:lang="en">sea level rise</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;sea_level_change"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#sedimentary_process -->

    <skos:Concept rdf:about="&EventProcess200811;sedimentary_process">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811"
            >sedimentary_process</skos:notation>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:definition
            >a phenomenon that changes the distribution or physical properties of sediment at or near the earth&#39;s surface</skos:definition>
        <skos:prefLabel xml:lang="en"
            >sedimentary process</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#shearing -->

    <owl:Thing rdf:about="&EventProcess200811;shearing">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">12.2.</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">shearing</skos:notation>
        <skos:definition
            >A deformation in which contiguous parts of a body are displaced relatively to each other in a direction parallel to a surface. The surface may be a discrete fault, or the deformation may be a penetrative strain and the shear surface is a geometric abstraction.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">shearing</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;deformation"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#slide -->

    <owl:Thing rdf:about="&EventProcess200811;slide">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.4.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">slide</skos:notation>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:definition
            >downslope transport, under gravitational influence, of soil and rock material en masse over a relatively confined zone or surface of shear.</skos:definition>
        <skos:prefLabel xml:lang="en">slide</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;mass_wasting"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#solifluction -->

    <skos:Concept rdf:about="&EventProcess200811;solifluction">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">05.1.4.7</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">solifluction</skos:notation>
        <skos:definition
            >process of slow viscous downslope flow of waterlogged soil and other unsorted and saturated surficial material, normally at 0.5-5.0 cm/yr; especially on slopes underlain by frozen ground that acts as a downward barrier to water percolation, initiated by frost action and augmented by meltwater resulting from alternate freezing and thawing of snow and ground ice.</skos:definition>
        <skos:historyNote
            >Neuendorf, K.K.E, Mehl, J.P. &amp; Jackson, J.A. (eds), 2005. Glossary of geology, 5th Edition. American Geological Institute, Alexandria, 779 p.</skos:historyNote>
        <skos:prefLabel xml:lang="en">solifluction</skos:prefLabel>
        <skos:broader rdf:resource="&EventProcess200811;mass_wasting"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#speciation -->

    <owl:Thing rdf:about="&EventProcess200811;speciation">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">02..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:EventProcess:200811">speciation</skos:notation>
        <skos:definition
            >process that results inappearance of new species</skos:definition>
        <skos:prefLabel xml:lang="en">speciation</skos:prefLabel>
        <skos:historyNote>this vocabulary</skos:historyNote>
        <skos:broader rdf:resource="&EventProcess200811;geologic_process"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:EventProcess:200811#spreading -->

