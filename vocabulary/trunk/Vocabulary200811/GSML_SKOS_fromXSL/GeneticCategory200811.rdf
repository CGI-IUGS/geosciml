    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#anthropogenic_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;anthropogenic_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">1..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >anthropogenic_genesis</skos:notation>
        <skos:definition
            >Formation predominantly by human activity.</skos:definition>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#biological_sedimentary_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;biological_sedimentary_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">4.2..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >biological_sedimentary_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:definition
            >Formation predominantly by deposition of material produced by living organisms either as part of their body (e.g., exoskeleton, bone, pollen, wood) or through their activities (e.g., faecal pellets).</skos:definition>
        <skos:broader rdf:resource="&GeneticCategory200811;sedimentary_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#cataclastic_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;cataclastic_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">6.2..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >cataclastic_genesis</skos:notation>
        <skos:definition
            >Formation predominantly by brittle deformation, i.e. the formation and growth of fractures and frictional sliding along fracture surfaces.</skos:definition>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:historyNote
            >based on Neuendorf et al 2005</skos:historyNote>
        <skos:broader rdf:resource="&GeneticCategory200811;deformation_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#chemical_sedimentary_genesis -->

    <owl:Thing rdf:about="&GeneticCategory200811;chemical_sedimentary_genesis">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">4.3..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >chemical_sedimentary_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:definition
            >Formation predominantly by direct chemical precipitation (e.g., evaporites, exhalative deposits).</skos:definition>
        <skos:broader rdf:resource="&GeneticCategory200811;sedimentary_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#clastic_sedimentary_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;clastic_sedimentary_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">4.1..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >clastic_sedimentary_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:definition
            >Formation predominantly by accumulation of particles (clasts) derived by weathering, erosion, or fragmentation of pre-existing rock or produced by chemical or biologically-mediated precipitation.</skos:definition>
        <skos:broader rdf:resource="&GeneticCategory200811;sedimentary_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#composite_process_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;composite_process_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">8..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >composite_process_genesis</skos:notation>
        <skos:definition
            >Formation in which more than one genetic process is dominant. This usually involves some combination of igneous, weathering, sedimentary, metamorphic, deformation or impact-related processes that better represents the origin of the material</skos:definition>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#contact_metamorphic_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;contact_metamorphic_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">5.2.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >contact_metamorphic_genesis</skos:notation>
        <skos:historyNote
            >based on Smulikowski et al, 2003</skos:historyNote>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation predominantly by metamorphism due to the effect of a magma body on the rocks it intrudes.</skos:definition>
        <skos:broader rdf:resource="&GeneticCategory200811;metamorphic_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#deformation_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;deformation_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">6..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >deformation_genesis</skos:notation>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation predominantly by strain-related processes resulting in changes of shape of a rock body, including folding, faulting, shearing, or fabric development; deformation may be either brittle or ductile. Local deformation metamorphic genesis related to a particular fault or shear zone is categorized as &#39;Dislocation metamorphic genesis&#39;</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#diagenetic_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;diagenetic_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">4.4..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >diagenetic_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation predominantly by chemical and physical changes subsequent to deposition of sediment, during and after lithification that occur under temperature and pressure conditions too low to be considered metamorphic.</skos:definition>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:broader rdf:resource="&GeneticCategory200811;sedimentary_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#dislocation_metamorphic_genesis -->

    <owl:Thing rdf:about="&GeneticCategory200811;dislocation_metamorphic_genesis">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">5.2.3.1</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >dislocation_metamorphic_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation predominantly by metamorphism of local extent associated with fault zones or shear zones. &#39;Dislocation metamorphic genesis&#39; is &#39;Deformation genesis&#39; of local extent, explicitly associated with a particular fault or shear zone.</skos:definition>
        <skos:historyNote
            >based on Smulikowski et al, 2003</skos:historyNote>
        <skos:broader rdf:resource="&GeneticCategory200811;local_metamorphic_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#ductile_deformation_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;ductile_deformation_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">6.1..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >ductile_deformation_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation predominantly by deformation without loss of material continuity at the scale of observation.</skos:definition>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:broader rdf:resource="&GeneticCategory200811;deformation_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#hypabyssal_intrusive_genesis -->

    <owl:Thing rdf:about="&GeneticCategory200811;hypabyssal_intrusive_genesis">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">2.2.2</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >hypabyssal_intrusive_genesis</skos:notation>
        <skos:definition
            >Formed by crystallisation close to the Earth&#39;s surface, characterized by more rapid cooling than plutonic setting to produce generally fine-grained intrusive igneous rock, commonly associated with co-magmatic volcanic rocks.</skos:definition>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:broader rdf:resource="&GeneticCategory200811;igneous_intrusive_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#igneous-sedimentary_genesis -->

    <owl:Thing rdf:about="&GeneticCategory200811;igneous-sedimentary_genesis"/>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#igneous_extrusive_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;igneous_extrusive_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">2.1..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >igneous_extrusive_genesis</skos:notation>
        <skos:definition
            >Formation predominantly by crystallisation of magma at or immediately adjacent to Earth&#39;s surface.</skos:definition>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:broader rdf:resource="&GeneticCategory200811;igneous_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#igneous_genesis -->

    <owl:Thing rdf:about="&GeneticCategory200811;igneous_genesis">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">2..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811">igneous_genesis</skos:notation>
        <skos:definition
            >Formation predominantly by crystallisation from magma</skos:definition>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#igneous_intrusive_genesis -->

    <owl:Thing rdf:about="&GeneticCategory200811;igneous_intrusive_genesis">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">2.2..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >igneous_intrusive_genesis</skos:notation>
        <skos:definition
            >Formation predominantly by crystallisation of magma within the Earth.</skos:definition>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:broader rdf:resource="&GeneticCategory200811;igneous_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#igneous_sedimentary_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;igneous_sedimentary_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">8.1..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >igneous_sedimentary_genesis</skos:notation>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:definition
            >Formation by a combination of igneous and sedimentary processes</skos:definition>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:broader rdf:resource="&GeneticCategory200811;composite_process_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#impact_genesis -->

    <owl:Thing rdf:about="&GeneticCategory200811;impact_genesis">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">7..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811">impact_genesis</skos:notation>
        <skos:historyNote
            >Smulikowski et al, 2003</skos:historyNote>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation predominantly by metamorphism related to the passage of a shock wave through a body of material, typically the result of impact of a planetary body (impactor) on a planetary surface (target)</skos:definition>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#local_metamorphic_genesis -->

    <owl:Thing rdf:about="&GeneticCategory200811;local_metamorphic_genesis">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">5.2.3..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >local_metamorphic_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation predominantly by metamorphism that may be attributed to a localized cause, such as magmatic intrusion, faulting, metorite impact, combustion of naturally occuring substances (coal), or lightning.</skos:definition>
        <skos:historyNote
            >Smulikowski et al, 2003</skos:historyNote>
        <skos:broader rdf:resource="&GeneticCategory200811;metamorphic_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#metaigneous_genesis -->

    <owl:Thing rdf:about="&GeneticCategory200811;metaigneous_genesis">
        <rdf:type rdf:resource="&skos;Concept"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">8.2..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >metaigneous_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation by metamorphism of an igneous protolith</skos:definition>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:broader rdf:resource="&GeneticCategory200811;composite_process_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </owl:Thing>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#metamorphic_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;metamorphic_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">5.2..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >metamorphic_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation predominantly by closed-system changes in mineralogy, texture, or fabric of a rock in response to chemical and physical conditions that have been imposed below the surface zones of weathering and cementation (diagenesis), and that differ from the conditions under which the rocks in question originated. As defined here the changes in rock volume may include removal of some chemical constituents from the system (especially water and other volatile constituents). Note narrower use that Neuendorf et al. 2005; this vocabulary distinguishes metasomatism from metamorphism.</skos:definition>
        <skos:historyNote
            >based on Neuendorf et al. 2005</skos:historyNote>
        <skos:broader rdf:resource="&GeneticCategory200811;metamorphic_metasomatic_or_hydrothermal_genesis"/>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#metamorphic_metasomatic_or_hydrothermal_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;metamorphic_metasomatic_or_hydrothermal_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">5..</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >metamorphic_metasomatic_or_hydrothermal_genesis</skos:notation>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
            >Formation predominantly by changes in chemical, mineralogical, or structural properties of rocks in response to chemical and physical conditions that have been imposed below the surface zones of weathering and cementation (diagenesis), and that differ from the conditions under which the rocks in question originated.</skos:definition>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:inScheme rdf:resource=""/>
    </skos:Concept>
    


    <!-- urn:cgi:classifier:CGI:GeneticCategory:200811#metaplutonic_genesis -->

    <skos:Concept rdf:about="&GeneticCategory200811;metaplutonic_genesis">
        <rdf:type rdf:resource="&owl;Thing"/>
        <!--<skos:notation rdf:datatype="LocalHierarchyKey">8.2.2</skos:notation>-->
        <skos:notation rdf:datatype="urn:cgi:classifierScheme:CGI:GeneticCategory:200811"
            >metaplutonic_genesis</skos:notation>
        <skos:historyNote>this vocab</skos:historyNote>
        <skos:prefLabel xml:lang="en"></skos:prefLabel>
        <skos:definition
